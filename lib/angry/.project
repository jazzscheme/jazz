(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: angry      description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "angry"}
        (<Project-Directory>                  directory: {Directory :context "src" "angry" "jazz"}
          (<Jazz-File>       name: _jazz      source: {File :context "src" "angry" "jazz" "_jazz.jazz"}))
        (<Jazz-File>         name: objcproxy  source: {File :context "src" "angry" "objcproxy.jazz"})
        (<Jazz-File>         name: database   source: {File :context "src" "angry" "database.jazz"}))))))
