(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: backend.javascript description-file: {File :context ".package"}
    (<Project-Directory>                              directory: {Directory :context "src"}
      (<Project-Directory>                            directory: {Directory :context "src" "backend"}
        (<Project-Directory>                          directory: {Directory :context "src" "backend" "javascript"}
          (<Jazz-File>       name: _javascript        source: {File :context "src" "backend" "javascript" "_javascript.jazz"})))))))
