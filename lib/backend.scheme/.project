(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: backend.scheme description-file: {File :context ".package"}
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "backend"}
        (<Project-Directory>                        directory: {Directory :context "src" "backend" "scheme"}
          (<Project-Directory>                      directory: {Directory :context "src" "backend" "scheme" "runtime"}
            (<Project-File>                         source: {File :context "src" "backend" "scheme" "runtime" "_runtime.scm"})
            (<Project-File>                         source: {File :context "src" "backend" "scheme" "runtime" "syntax.scm"}))
          (<Project-File>                           source: {File :context "src" "backend" "scheme" "_scheme.scm"})))))))
