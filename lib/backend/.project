;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: backend
    (<Project-Directory>                     directory: {Directory :context "src"}
      (<Project-Directory>                   directory: {Directory :context "src" "backend"}
        (<Project-Directory>                 directory: {Directory :context "src" "backend" "describe"}
          (<Jazz-File>       name: _describe source: {File :context "src" "backend" "describe" "_describe.jazz"})
          (<Jazz-File>       name: backend   source: {File :context "src" "backend" "describe" "backend.jazz"})
          (<Jazz-File>       name: base      source: {File :context "src" "backend" "describe" "base.jazz"})
          (<Jazz-File>       name: jazz      source: {File :context "src" "backend" "describe" "jazz.jazz"})
          (<Jazz-File>       name: scheme    source: {File :context "src" "backend" "describe" "scheme.jazz"}))
        (<Jazz-File>         name: _backend  source: {File :context "src" "backend" "_backend.jazz"})
        (<Jazz-File>         name: syntax    source: {File :context "src" "backend" "syntax.jazz"}))))))
