(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: contrib.aes description-file: {File :context ".package"}
    (<Project-Directory>                     directory: {Directory :context "src"}
      (<Project-Directory>                   directory: {Directory :context "src" "aes"}
        (<Jazz-File>       name: _aes        source: {File :context "src" "aes" "_aes.jazz"})
        (<Project-File>                      source: {File :context "src" "aes" "implementation.scm"})
        (<Project-File>                      source: {File :context "src" "aes" "fixnum.scm"})
        (<Project-File>                      source: {File :context "src" "aes" "homovector.scm"}))))))
