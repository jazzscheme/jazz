(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: contrib.irregex description-file: {File :context ".package"}
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "irregex"}
        (<Jazz-File>         name: _irregex        source: {File :context "src" "irregex" "_irregex.jazz"})
        (<Project-Directory>                       directory: {Directory :context "src" "irregex" "implementation"}
          (<Project-File>                          source: {File :context "src" "irregex" "implementation" "README"})
          (<Project-File>                          source: {File :context "src" "irregex" "implementation" "irregex.scm"})))))))
