          (<Jazz-File>         name: _irregex source: {File Jazz "lib" "contrib.irregex" "src" "irregex" "_irregex.jazz"})
          (<Project-Directory>                directory: {Directory Jazz "lib" "contrib.irregex" "src" "irregex" "implementation"}
            (<Project-File>                   source: {File Jazz "lib" "contrib.irregex" "src" "irregex" "implementation" "README"})
            (<Project-File>                   source: {File Jazz "lib" "contrib.irregex" "src" "irregex" "implementation" "irregex.scm"})))))))


(register-autoclass IrRegex-Manifest))
