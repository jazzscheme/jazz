(import (jazz.io)
        (jazz.jml)
        (jazz.library)
        (jedi))


(class IrRegex-Manifest extends Project
  
  
  (form
    (<install>                                title: "contrib.irregex" catalog?: #t catalog-extensions: :jazz include-extensions: :jazz module-name: project.irregex.IrRegex-Manifest
      (<Project-Directory>                    directory: {Directory Jazz "lib" "contrib.irregex" "src"}
