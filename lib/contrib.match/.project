;;;
;;;; Match Manifest
;;;


(<Project>                                    title: "contrib.match" catalog?: #t catalog-extensions: :jazz include-extensions: :jazz
  (<Project-Directory>                        directory: {Directory contrib.match "src"}
    (<Project-Directory>                      directory: {Directory contrib.match "src" "match"}
      (<Project-Directory>                    directory: {Directory contrib.match "src" "match" "validate"}
        (<Jazz-File>       name: Match-Tester source: {File contrib.match "src" "match" "validate" "Match-Tester.jazz"})
        (<Jazz-File>       name: run          source: {File contrib.match "src" "match" "validate" "run.jazz"}))
      (<Jazz-File>         name: _match       source: {File contrib.match "src" "match" "_match.jazz"}))))
