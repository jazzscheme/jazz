;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: contrib.match catalog?: #t catalog-extensions: :jazz include-extensions: :jazz
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "match"}
        (<Project-Directory>                     directory: {Directory :context "src" "match" "validate"}
          (<Jazz-File>       name: Match-Tester  source: {File :context "src" "match" "validate" "Match-Tester.jazz"})
          (<Jazz-File>       name: run           source: {File :context "src" "match" "validate" "run.jazz"}))
        (<Jazz-File>         name: _match        source: {File :context "src" "match" "_match.jazz"}))))))
