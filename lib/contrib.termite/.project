(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>               name: contrib.termite description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "termite"}
        (<Jazz-File>       name: _termite        source: {File :context "src" "termite" "_termite.jazz"})
        (<Project-File>                          source: {File :context "src" "termite" "implementation.scm"})
        (<Jazz-File>       name: test            source: {File :context "src" "termite" "test.jazz"}))))))
