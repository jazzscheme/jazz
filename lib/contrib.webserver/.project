(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                  catalog?: #t catalog-extensions: :jazz include-extensions: :jazz catalog-name: contrib.webserver
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "webserver"}
        (<Project-Directory>                  directory: {Directory :context "src" "webserver" "implementation"}
          (<Project-File>                     source: {File :context "src" "webserver" "implementation" "http.scm"}))
        (<Jazz-File>         name: _webserver source: {File :context "src" "webserver" "_webserver.jazz"}))))))
