(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: contrib.xml description-file: {File :context ".package"}
    (<Project-Directory>                     directory: {Directory :context "src"}
      (<Project-Directory>                   directory: {Directory :context "src" "xml"}
        (<Project-File>                      source: {File :context "src" "xml" "implementation.scm"})
        (<Jazz-File>       name: test        source: {File :context "src" "xml" "test.jazz"})
        (<Project-File>                      source: {File :context "src" "xml" "_xml.scm"}))))))
