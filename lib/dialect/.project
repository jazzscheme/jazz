;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: dialect description-file: {File :context ".package"}
    (<Project-Directory>                 directory: {Directory :context "src"}
      (<Project-Directory>               directory: {Directory :context "src" "dialect"}
        (<Project-File>                  source: {File :context "src" "dialect" "_dialect.scm"})
        (<Project-File>                  source: {File :context "src" "dialect" "development.scm"})
        (<Project-File>                  source: {File :context "src" "dialect" "expression.scm"})
        (<Project-File>                  source: {File :context "src" "dialect" "runtime.scm"})
        (<Project-File>                  source: {File :context "src" "dialect" "syntax.scm"}))))))
