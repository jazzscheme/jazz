(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: foundation.backend.describe description-file: {File :context ".package"}
    (<Project-Directory>                                directory: {Directory :context "src"}
      (<Project-Directory>                              directory: {Directory :context "src" "foundation"}
        (<Project-Directory>                            directory: {Directory :context "src" "foundation" "backend"}
          (<Project-Directory>                          directory: {Directory :context "src" "foundation" "backend" "describe"}
            (<Jazz-File>       name: _describe          source: {File :context "src" "foundation" "backend" "describe" "_describe.jazz"})
            (<Jazz-File>       name: emit               source: {File :context "src" "foundation" "backend" "describe" "emit.jazz"}))))))))
