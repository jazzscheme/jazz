(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: foundation.backend.javascript description-file: {File :context ".package"}
    (<Project-Directory>                                           directory: {Directory :context "src"}
      (<Project-Directory>                                         directory: {Directory :context "src" "foundation"}
        (<Project-Directory>                                       directory: {Directory :context "src" "foundation" "backend"}
          (<Project-Directory>                                     directory: {Directory :context "src" "foundation" "backend" "javascript"}
            (<Jazz-File>       name: _javascript                   source: {File :context "src" "foundation" "backend" "javascript" "_javascript.jazz"})
            (<Jazz-File>       name: emit                          source: {File :context "src" "foundation" "backend" "javascript" "emit.jazz"}))))))))
