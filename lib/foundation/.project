;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: foundation
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "foundation"}
        (<Project-Directory>                  directory: {Directory :context "src" "foundation" "dialect"}
          (<Project-File>                     source: {File :context "src" "foundation" "dialect" "_dialect.scm"})
          (<Project-File>                     source: {File :context "src" "foundation" "dialect" "runtime.scm"}))
        (<Project-File>                       source: {File :context "src" "foundation" "_foundation.scm"}))))))
