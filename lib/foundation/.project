;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: foundation description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "foundation"}
        (<Project-File>                       source: {File :context "src" "foundation" "_foundation.scm"})
        (<Project-Directory>                  directory: {Directory :context "src" "foundation" "language"}
          (<Project-File>                     source: {File :context "src" "foundation" "language" "runtime.scm"})))))))
