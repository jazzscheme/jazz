;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
(jazz.editor.jazz))


(form
  (<Project>                   name: foundation
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "foundation"}
        (<Project-Directory>                    directory: {Directory :context "src" "foundation" "runtime"}
          (<Project-File>                       source: {File :context "src" "foundation" "runtime" "_runtime.scm"}))
        (<Project-Directory>                    directory: {Directory :context "src" "foundation" "syntax"}
          (<Project-Directory>                  directory: {Directory :context "src" "foundation" "syntax" "dialect"}
            (<Project-File>                     source: {File :context "src" "foundation" "syntax" "dialect" "runtime.scm"})
            (<Project-File>                     source: {File :context "src" "foundation" "syntax" "dialect" "syntax.scm"}))
          (<Project-Directory>                  directory: {Directory :context "src" "foundation" "syntax" "walker"}
            (<Project-File>                     source: {File :context "src" "foundation" "syntax" "walker" "runtime.scm"})
            (<Project-File>                     source: {File :context "src" "foundation" "syntax" "walker" "syntax.scm"}))
          (<Project-File>                       source: {File :context "src" "foundation" "syntax" "_syntax.scm"}))
        (<Project-File>                         source: {File :context "src" "foundation" "_foundation.scm"}))))))
