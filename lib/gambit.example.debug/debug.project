(data jazz.debugger.gambit.data.project


(form
  (<Gambit-Project> title: "Gambit Debug" product: gambit-debug)))


@convert
(method override (project-run . rest)
  (message-box "This project doesn't support Run as the Gambit console-based debugger cannot handle input from multiple threads. The purpose of this project is to demonstrate multiple threads debugging using the Jedi debugger."))
