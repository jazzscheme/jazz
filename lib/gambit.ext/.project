          (<Project-File>                     source: {File :context "src" "gambit" "log" "demo.scm"})
          (<C-File>                           source: {File :context "src" "gambit" "log" "demo1.c"})
          (<C-File>                           source: {File :context "src" "gambit" "log" "demo2.c"})
          (<C-File>                           source: {File :context "src" "gambit" "log" "demo3.c"})
          (<C-File>                           source: {File :context "src" "gambit" "log" "loglib.c"})
          (<Project-File>                     source: {File :context "src" "gambit" "log" "loglib.h"})
          (<Project-File>                     source: {File :context "src" "gambit" "log" "loglib.scm"})
          (<C-File>                           source: {File :context "src" "gambit" "log" "xlog.c"}))
        (<Project-File>                       source: {File :context "src" "gambit" "log.scm"}))))))
