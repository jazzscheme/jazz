(data jazz.ide.data.project


(form
  (<Project>                 catalog?: #t catalog-extensions: :text include-extensions: :text catalog-name: gambit
    (<Project-Directory>     directory: {Directory :context "src"}
      (<Project-Directory>   directory: {Directory :context "src" "gambit"}
        (<Project-Directory> directory: {Directory :context "src" "gambit" "dialect"}
          (<Project-File>    source: {File :context "src" "gambit" "dialect" "_dialect.scm"})
          (<Project-File>    source: {File :context "src" "gambit" "dialect" "classes.scm"})
          (<Project-File>    source: {File :context "src" "gambit" "dialect" "dialect.scm"})
          (<Project-File>    source: {File :context "src" "gambit" "dialect" "kernel.scm"}))
        (<Project-File>      source: {File :context "src" "gambit" "_gambit.scm"}))))))
