(package jam
  
  (title "Jam")
  (description "Jazz package manager")
  (authors "Guillaume Cartier")
  (stage experimental)
  (root "src")
  (products (jam (update jam.help
                         jam.list
                         jam.search
                         jam.info)
                 (dependencies jazz))
            (jam-help (run jam.help))
            (jam-list (run jam.list))
            (jam-search (run jam.search))
            (jam-info (run jam.info)))
  (project ".project"))
