(package jam
  
  (title "Jam")
  (description "Jazz package manager")
  (authors "Guillaume Cartier")
  (stage experimental)
  (root "src")
  (products (jam (update jam
                         jam.help
                         jam.list
                         jam.search
                         jam.info
                         jam.dependencies
                         jam.references)
                 (dependencies jazz))
            (jam-help (run jam.help))
            (jam-list (run jam.list))
            (jam-search (run jam.search))
            (jam-info (run jam.info))
            (jam-dependencies (run jam.dependencies))
            (jam-deps (alias jam-dependencies))
            (jam-references (run jam.references))
            (jam-refs (alias jam-references)))
  (project ".project"))
