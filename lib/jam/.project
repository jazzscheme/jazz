(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>               name: jam  description-file: {File :context ".package"}
    (<Project-Directory>              directory: {Directory :context "src"}
      (<Project-Directory>            directory: {Directory :context "src" "jam"}
        (<Jazz-File>       name: info source: {File :context "src" "jam" "info.jazz"})
        (<Jazz-File>       name: list source: {File :context "src" "jam" "list.jazz"})
        (<Jazz-File>       name: help source: {File :context "src" "jam" "help.jazz"}))))))
