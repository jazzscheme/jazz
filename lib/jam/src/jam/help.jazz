(module jam.help jazz


(import (jazz.runtime))


(definition (help)
  (format :console "Usage:{%}")
  (format :console "  jam list{%}")
  (format :console "  jam search target{%}")
  (format :console "  jam info package{%}")
  (format :console "  jam dependencies package{%}")
  (format :console "  jam references package{%}")
  (format :console "{%}")
  (format :console "Help:{%}")
  (format :console "  jam help"))


(register-product-run 'jam-help
  (lambda (descriptor)
    (help))))
