(module jam.help jazz


(import (jazz.runtime))


(definition (help)
  (format :console "Usage:{%}")
  (format :console "  jam list{%}")
  (format :console "  jam search{%}")
  (format :console "  jam info{%}")
  (format :console "{%}")
  (format :console "Help:{%}")
  (format :console "  jam help"))


(register-product-run 'jam-help
  (lambda (descriptor)
    (help))))
