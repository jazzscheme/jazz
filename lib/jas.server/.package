(package jas.server
  
  (title "JAS Server")
  (description "Jazz Associative Storage Server")
  (authors "Guillaume Cartier")
  (stage experimental)
  (root "src")
  (products (jas-server (run jas.server.run)
                        (build (jas-server executable: "jas-server" console?: #t))
                        (update jas.server
                                jas.server.report
                                jas.server.run)
                        (dependencies scheme.core
                                      jazz
                                      jazz.associative
                                      jazz.log
                                      jazz.runtime
                                      jazz.snapshot)))
  (project ".project"))
