(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jas.server description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jas"}
        (<Project-Directory>                  directory: {Directory :context "src" "jas" "server"}
          (<Jazz-File>       name: _server    source: {File :context "src" "jas" "server" "_server.jazz"})
          (<Jazz-File>       name: run        source: {File :context "src" "jas" "server" "run.jazz"})
          (<Jazz-File>       name: report     source: {File :context "src" "jas" "server" "report.jazz"})))))))
