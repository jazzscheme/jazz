(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>               name: jas  description-file: {File :context ".package"}
    (<Project-Directory>              directory: {Directory :context "src"}
      (<Project-Directory>            directory: {Directory :context "src" "jas"}
        (<Jazz-File>       name: _jas source: {File :context "src" "jas" "_jas.jazz"})
        (<Jazz-File>       name: run  source: {File :context "src" "jas" "run.jazz"}))))))
