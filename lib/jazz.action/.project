;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.action      description-file: {File :context ".package"}
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                        directory: {Directory :context "src" "jazz" "action"}
          (<Jazz-File>       name: _action          source: {File :context "src" "jazz" "action" "_action.jazz"})
          (<Jazz-File>       name: Action-Item      source: {File :context "src" "jazz" "action" "Action-Item.jazz"})
          (<Jazz-File>       name: Action           source: {File :context "src" "jazz" "action" "Action.jazz"})
          (<Jazz-File>       name: Actionable       source: {File :context "src" "jazz" "action" "Actionable.jazz"})
          (<Jazz-File>       name: Actions          source: {File :context "src" "jazz" "action" "Actions.jazz"})
          (<Jazz-File>       name: autoload         source: {File :context "src" "jazz" "action" "autoload.jazz"})
          (<Project-File>                           source: {File :context "src" "jazz" "action" "install.scm"})
          (<Jazz-File>       name: Action-Event     source: {File :context "src" "jazz" "action" "Action-Event.jazz"})
          (<Jazz-File>       name: Orphaned-Actions source: {File :context "src" "jazz" "action" "Orphaned-Actions.jazz"})
          (<Jazz-File>       name: Shortcut         source: {File :context "src" "jazz" "action" "Shortcut.jazz"})
          (<Jazz-File>       name: Bindings         source: {File :context "src" "jazz" "action" "Bindings.jazz"})))))))
