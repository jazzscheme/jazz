;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.appl description-file: {File :context ".package"}
    (<Project-Directory>                     directory: {Directory :context "src"}
      (<Project-Directory>                   directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                 directory: {Directory :context "src" "jazz" "appl"}
          (<Jazz-File>       name: _appl     source: {File :context "src" "jazz" "appl" "_appl.jazz"})
          (<Jazz-File>       name: product   source: {File :context "src" "jazz" "appl" "product.jazz"})
          (<Jazz-File>       name: mac       source: {File :context "src" "jazz" "appl" "mac.jazz"})
          (<Jazz-File>       name: windows   source: {File :context "src" "jazz" "appl" "windows.jazz"})
          (<Jazz-File>       name: linux     source: {File :context "src" "jazz" "appl" "linux.jazz"})))))))
