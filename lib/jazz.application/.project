;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.application        description-file: {File :context ".package"}
    (<Project-Directory>                                   directory: {Directory :context "src"}
      (<Project-Directory>                                 directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                               directory: {Directory :context "src" "jazz" "application"}
          (<Jazz-File>       name: _application            source: {File :context "src" "jazz" "application" "_application.jazz"})
          (<Jazz-File>       name: Application-Actions     source: {File :context "src" "jazz" "application" "Application-Actions.jazz"})
          (<Jazz-File>       name: Application-Bindings    source: {File :context "src" "jazz" "application" "Application-Bindings.jazz"})
          (<Jazz-File>       name: Application-Event       source: {File :context "src" "jazz" "application" "Application-Event.jazz"})
          (<Jazz-File>       name: Application-Preferences source: {File :context "src" "jazz" "application" "Application-Preferences.jazz"})
          (<Jazz-File>       name: Application             source: {File :context "src" "jazz" "application" "Application.jazz"})
          (<Jazz-File>       name: autoload                source: {File :context "src" "jazz" "application" "autoload.jazz"})
          (<Jazz-File>       name: Exception-Presenter     source: {File :context "src" "jazz" "application" "Exception-Presenter.jazz"})))))))
