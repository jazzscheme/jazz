(module protected jazz.application.Application-Bindings jazz


(import (jazz.action)
        (jazz.application)
        (jazz.jml)
        (jazz.menu)
        (jazz.tree)
        (jazz.ui))


(class undocumented Application-Bindings extends Bindings
  
  
  (form
    (<install>
      (<Application-Actions>    name: application)
      (<View-Actions>           name: view)
      (<Text-Actions>           name: text)
      (<Formatted-Text-Actions> name: formatted-text)
      (<Tree-Actions>           name: tree)
      (<Menubar-Actions>        name: menubar)
      (<Menu-Actions>           name: menu)
      (<Guest-Actions>          name: guest)))))
