;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.associative   description-file: {File :context ".package"}
    (<Project-Directory>                                directory: {Directory :context "src"}
      (<Project-Directory>                              directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                            directory: {Directory :context "src" "jazz" "associative"}
          (<Jazz-File>         name: _associative       source: {File :context "src" "jazz" "associative" "_associative.jazz"})
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "associative" "validate"}
            (<Jazz-File>       name: run                source: {File :context "src" "jazz" "associative" "validate" "run.jazz"})
            (<Jazz-File>       name: _validate          source: {File :context "src" "jazz" "associative" "validate" "_validate.jazz"})
            (<Jazz-File>       name: Associative-Tester source: {File :context "src" "jazz" "associative" "validate" "Associative-Tester.jazz"}))
          (<Jazz-File>         name: client             source: {File :context "src" "jazz" "associative" "client.jazz"})
          (<Jazz-File>         name: remote             source: {File :context "src" "jazz" "associative" "remote.jazz"})))))))
