(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.backend.describe description-file: {File :context ".package"}
    (<Project-Directory>                                 directory: {Directory :context "src"}
      (<Project-Directory>                               directory: {Directory :context "src" "backend"}
        (<Project-Directory>                             directory: {Directory :context "src" "backend" "describe"}
          (<Jazz-File>       name: _describe             source: {File :context "src" "backend" "describe" "_describe.jazz"})
          (<Jazz-File>       name: emit                  source: {File :context "src" "backend" "describe" "emit.jazz"})))))))
