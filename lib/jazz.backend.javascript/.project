(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.backend.javascript description-file: {File :context ".package"}
    (<Project-Directory>                                     directory: {Directory :context "src"}
      (<Project-Directory>                                   directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "backend"}
          (<Project-Directory>                               directory: {Directory :context "src" "jazz" "backend" "javascript"}
            (<Jazz-File>       name: _javascript             source: {File :context "src" "jazz" "backend" "javascript" "_javascript.jazz"})
            (<Jazz-File>       name: emit                    source: {File :context "src" "jazz" "backend" "javascript" "emit.jazz"}))))))))
