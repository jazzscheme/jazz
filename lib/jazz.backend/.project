(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.backend description-file: {File :context ".package"}
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "backend"}
          (<Jazz-File>       name: _backend     source: {File :context "src" "jazz" "backend" "_backend.jazz"})
          (<Jazz-File>       name: syntax       source: {File :context "src" "jazz" "backend" "syntax.jazz"})))))))
