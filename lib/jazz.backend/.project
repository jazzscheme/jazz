;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.backend
    (<Project-Directory>                          directory: {Directory :context "src"}
      (<Project-Directory>                        directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                      directory: {Directory :context "src" "jazz" "backend"}
          (<Project-Directory>                    directory: {Directory :context "src" "jazz" "backend" "describe"}
            (<Jazz-File>       name: _describe    source: {File :context "src" "jazz" "backend" "describe" "_describe.jazz"})
            (<Jazz-File>       name: emit         source: {File :context "src" "jazz" "backend" "describe" "emit.jazz"}))
          (<Jazz-File>         name: _backend     source: {File :context "src" "jazz" "backend" "_backend.jazz"})
          (<Jazz-File>         name: syntax       source: {File :context "src" "jazz" "backend" "syntax.jazz"})))))))
