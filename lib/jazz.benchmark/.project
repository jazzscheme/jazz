(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.benchmark catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                        directory: {Directory :context "src" "jazz" "benchmark"}
          (<Jazz-File>         name: _benchmark     source: {File :context "src" "jazz" "benchmark" "_benchmark.jazz"})
          (<Jazz-File>         name: run            source: {File :context "src" "jazz" "benchmark" "run.jazz"})
          (<Project-Directory>                      directory: {Directory :context "src" "jazz" "benchmark" "catalog"}
            (<Jazz-File>       name: _catalog       source: {File :context "src" "jazz" "benchmark" "catalog" "_catalog.jazz"}))))))))
