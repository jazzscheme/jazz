(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.builder description-file: {File :context ".package"}
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "builder"}
          (<Jazz-File>       name: _builder     source: {File :context "src" "jazz" "builder" "_builder.jazz"})
          (<Jazz-File>       name: run          source: {File :context "src" "jazz" "builder" "run.jazz"})))))))
