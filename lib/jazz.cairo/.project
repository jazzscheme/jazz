;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.cairo     description-file: {File :context ".package"}
    (<Project-Directory>                          directory: {Directory :context "src"}
      (<Project-Directory>                        directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                      directory: {Directory :context "src" "jazz" "cairo"}
          (<Jazz-File>       name: _cairo         source: {File :context "src" "jazz" "cairo" "_cairo.jazz"})
          (<Jazz-File>       name: cairo-base     source: {File :context "src" "jazz" "cairo" "cairo-base.jazz"})
          (<Jazz-File>       name: cairo-freetype source: {File :context "src" "jazz" "cairo" "cairo-freetype.jazz"})
          (<Jazz-File>       name: cairo-logfont  source: {File :context "src" "jazz" "cairo" "cairo-logfont.jazz"})
          (<Jazz-File>       name: cairo-quartz   source: {File :context "src" "jazz" "cairo" "cairo-quartz.jazz"})
          (<Jazz-File>       name: cairo-windows  source: {File :context "src" "jazz" "cairo" "cairo-windows.jazz"})
          (<Jazz-File>       name: cairo-x11      source: {File :context "src" "jazz" "cairo" "cairo-x11.jazz"})
          (<Project-File>                         source: {File :context "src" "jazz" "cairo" "install.scm"})
          (<Project-File>                         source: {File :context "src" "jazz" "cairo" "product.scm"})
          (<Jazz-File>       name: test           source: {File :context "src" "jazz" "cairo" "test.jazz"})))))))
