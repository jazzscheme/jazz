;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.cairo description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                  directory: {Directory :context "src" "jazz" "cairo"}
          (<Jazz-File>       name: _cairo     source: {File :context "src" "jazz" "cairo" "_cairo.jazz"})
          (<Project-File>                     source: {File :context "src" "jazz" "cairo" "install.scm"})
          (<Project-File>                     source: {File :context "src" "jazz" "cairo" "product.scm"})
          (<Jazz-File>       name: test       source: {File :context "src" "jazz" "cairo" "test.jazz"})))))))
