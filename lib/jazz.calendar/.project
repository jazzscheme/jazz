(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                          catalog?: #t catalog-extensions: :text include-extensions: :text catalog-name: jazz.calendar
    (<Project-Directory>                              directory: {Directory :context "src"}
      (<Project-Directory>                            directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                          directory: {Directory :context "src" "jazz" "calendar"}
          (<Jazz-File>       name: _calendar          source: {File :context "src" "jazz" "calendar" "_calendar.jazz"})
          (<Jazz-File>       name: autoload           source: {File :context "src" "jazz" "calendar" "autoload.jazz"})
          (<Jazz-File>       name: Calendar-Actions   source: {File :context "src" "jazz" "calendar" "Calendar-Actions.jazz"})
          (<Jazz-File>       name: Calendar-Day       source: {File :context "src" "jazz" "calendar" "Calendar-Day.jazz"})
          (<Jazz-File>       name: Calendar-Dialog    source: {File :context "src" "jazz" "calendar" "Calendar-Dialog.jazz"})
          (<Jazz-File>       name: Calendar-Navigator source: {File :context "src" "jazz" "calendar" "Calendar-Navigator.jazz"})
          (<Jazz-File>       name: Calendar-View      source: {File :context "src" "jazz" "calendar" "Calendar-View.jazz"})
          (<Jazz-File>       name: Date-Domain        source: {File :context "src" "jazz" "calendar" "Date-Domain.jazz"})
          (<Jazz-File>       name: Date-Entry         source: {File :context "src" "jazz" "calendar" "Date-Entry.jazz"})
          (<Jazz-File>       name: Date-Row           source: {File :context "src" "jazz" "calendar" "Date-Row.jazz"})
          (<Jazz-File>       name: Periods-Chooser    source: {File :context "src" "jazz" "calendar" "Periods-Chooser.jazz"})))))))
