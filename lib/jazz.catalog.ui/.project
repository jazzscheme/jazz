(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.catalog.ui description-file: {File :context ".package"}
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                       directory: {Directory :context "src" "jazz" "catalog"}
          (<Jazz-File>       name: browse          source: {File :context "src" "jazz" "catalog" "browse.jazz"})
          (<Jazz-File>       name: edit            source: {File :context "src" "jazz" "catalog" "edit.jazz"})))))))
