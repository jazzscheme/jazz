;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.catalog
    (<Project-Directory>                                 directory: {Directory :context "src"}
      (<Project-Directory>                               directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                             directory: {Directory :context "src" "jazz" "catalog"}
          (<Project-Directory>                           directory: {Directory :context "src" "jazz" "catalog" "catalog"}
            (<Jazz-File>       name: Catalog             source: {File :context "src" "jazz" "catalog" "catalog" "Catalog.jazz"})
            (<Jazz-File>       name: Cataloguer          source: {File :context "src" "jazz" "catalog" "catalog" "Cataloguer.jazz"})
            (<Jazz-File>       name: Filing-Catalog      source: {File :context "src" "jazz" "catalog" "catalog" "Filing-Catalog.jazz"}))
          (<Project-Directory>                           directory: {Directory :context "src" "jazz" "catalog" "entry"}
            (<Jazz-File>       name: Catalog-Entry       source: {File :context "src" "jazz" "catalog" "entry" "Catalog-Entry.jazz"})
            (<Jazz-File>       name: Category-Entry      source: {File :context "src" "jazz" "catalog" "entry" "Category-Entry.jazz"})
            (<Jazz-File>       name: Class-Entry         source: {File :context "src" "jazz" "catalog" "entry" "Class-Entry.jazz"})
            (<Jazz-File>       name: Code-Entry          source: {File :context "src" "jazz" "catalog" "entry" "Code-Entry.jazz"})
            (<Jazz-File>       name: Code-Location       source: {File :context "src" "jazz" "catalog" "entry" "Code-Location.jazz"})
            (<Jazz-File>       name: Constructor-Entry   source: {File :context "src" "jazz" "catalog" "entry" "Constructor-Entry.jazz"})
            (<Jazz-File>       name: Definition-Entry    source: {File :context "src" "jazz" "catalog" "entry" "Definition-Entry.jazz"})
            (<Jazz-File>       name: Entry               source: {File :context "src" "jazz" "catalog" "entry" "Entry.jazz"})
            (<Jazz-File>       name: File-Entry          source: {File :context "src" "jazz" "catalog" "entry" "File-Entry.jazz"})
            (<Jazz-File>       name: Hierarchical-Entry  source: {File :context "src" "jazz" "catalog" "entry" "Hierarchical-Entry.jazz"})
            (<Jazz-File>       name: Import-Entry        source: {File :context "src" "jazz" "catalog" "entry" "Import-Entry.jazz"})
            (<Jazz-File>       name: Indexed-Entry       source: {File :context "src" "jazz" "catalog" "entry" "Indexed-Entry.jazz"})
            (<Jazz-File>       name: Instance-Init-Entry source: {File :context "src" "jazz" "catalog" "entry" "Instance-Init-Entry.jazz"})
            (<Jazz-File>       name: Interface-Entry     source: {File :context "src" "jazz" "catalog" "entry" "Interface-Entry.jazz"})
            (<Jazz-File>       name: Method-Entry        source: {File :context "src" "jazz" "catalog" "entry" "Method-Entry.jazz"})
            (<Jazz-File>       name: Package-Entry       source: {File :context "src" "jazz" "catalog" "entry" "Package-Entry.jazz"})
            (<Jazz-File>       name: Static-Init-Entry   source: {File :context "src" "jazz" "catalog" "entry" "Static-Init-Entry.jazz"})
            (<Jazz-File>       name: Variable-Entry      source: {File :context "src" "jazz" "catalog" "entry" "Variable-Entry.jazz"}))
          (<Project-Directory>                           directory: {Directory :context "src" "jazz" "catalog" "parser"}
            (<Jazz-File>       name: File-Parser         source: {File :context "src" "jazz" "catalog" "parser" "File-Parser.jazz"}))
          (<Jazz-File>         name: _catalog            source: {File :context "src" "jazz" "catalog" "_catalog.jazz"})
          (<Jazz-File>         name: autoload            source: {File :context "src" "jazz" "catalog" "autoload.jazz"})))))))
