(module protected jazz.catalog.entry.Static-Init-Entry jazz


(import (jazz.catalog)
        (jazz.resource))


(class undocumented Static-Init-Entry extends Hierarchical-Entry
  

  (method override (present-name self)
    "<static init>")
  
  
  (method override (print-catalog self level output)
    (format output "{%}{_ :v}~{{s} {s} {s} {s} {s} {s} {s} {s}}"
            (* level 2)
            (category-name (class-of self))
            modifiers name line column end-line end-column references))
  
  
  (method override (get-image self)
    {Image-Resource "StaticInit"})
  
  
  ;;;
  ;;;; Indexes
  ;;;

  
  (method override (update-indexes self definitions-index references-index)
    (for-each (lambda (key)
                (update-index self references-index key))
              references))))
