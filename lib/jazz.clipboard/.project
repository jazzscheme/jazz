;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.clipboard description-file: {File :context ".package"}
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                        directory: {Directory :context "src" "jazz" "clipboard"}
          (<Project-Directory>                      directory: {Directory :context "src" "jazz" "clipboard" "platform"}
            (<Jazz-File>       name: cocoa          source: {File :context "src" "jazz" "clipboard" "platform" "cocoa.jazz"})
            (<Jazz-File>       name: windows        source: {File :context "src" "jazz" "clipboard" "platform" "windows.jazz"})
            (<Jazz-File>       name: x11            source: {File :context "src" "jazz" "clipboard" "platform" "x11.jazz"}))
          (<Jazz-File>         name: _clipboard     source: {File :context "src" "jazz" "clipboard" "_clipboard.jazz"})))))))
