(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.columnar                description-file: {File :context ".package"}
    (<Project-Directory>                                        directory: {Directory :context "src"}
      (<Project-Directory>                                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                    directory: {Directory :context "src" "jazz" "columnar"}
          (<Jazz-File>       name: _columnar                    source: {File :context "src" "jazz" "columnar" "_columnar.jazz"})
          (<Jazz-File>       name: Aligned-Column-Preferences   source: {File :context "src" "jazz" "columnar" "Aligned-Column-Preferences.jazz"})
          (<Jazz-File>       name: Aligned-Column               source: {File :context "src" "jazz" "columnar" "Aligned-Column.jazz"})
          (<Jazz-File>       name: Column-Preferences           source: {File :context "src" "jazz" "columnar" "Column-Preferences.jazz"})
          (<Jazz-File>       name: Columns-Display              source: {File :context "src" "jazz" "columnar" "Columns-Display.jazz"})
          (<Jazz-File>       name: Display-Column               source: {File :context "src" "jazz" "columnar" "Display-Column.jazz"})
          (<Jazz-File>       name: Indented-Column-Preferences  source: {File :context "src" "jazz" "columnar" "Indented-Column-Preferences.jazz"})
          (<Jazz-File>       name: Indented-Column              source: {File :context "src" "jazz" "columnar" "Indented-Column.jazz"})
          (<Jazz-File>       name: Sequenced-Column-Preferences source: {File :context "src" "jazz" "columnar" "Sequenced-Column-Preferences.jazz"})
          (<Jazz-File>       name: Sequenced-Column             source: {File :context "src" "jazz" "columnar" "Sequenced-Column.jazz"})
          (<Jazz-File>       name: autoload                     source: {File :context "src" "jazz" "columnar" "autoload.jazz"})
          (<Jazz-File>       name: designer                     source: {File :context "src" "jazz" "columnar" "designer.jazz"})))))))
