;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.command             description-file: {File :context ".package"}
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "command"}
          (<Jazz-File>       name: Command-Shell            source: {File :context "src" "jazz" "command" "Command-Shell.jazz"})
          (<Jazz-File>       name: Command-Text-Preferences source: {File :context "src" "jazz" "command" "Command-Text-Preferences.jazz"})
          (<Jazz-File>       name: Command-Text-View        source: {File :context "src" "jazz" "command" "Command-Text-View.jazz"})
          (<Jazz-File>       name: Execute-Mode-Actions     source: {File :context "src" "jazz" "command" "Execute-Mode-Actions.jazz"})
          (<Jazz-File>       name: Extended-Mode-Actions    source: {File :context "src" "jazz" "command" "Extended-Mode-Actions.jazz"})
          (<Jazz-File>       name: Find-Mode-Actions        source: {File :context "src" "jazz" "command" "Find-Mode-Actions.jazz"})
          (<Jazz-File>       name: Highlight-Mode-Actions   source: {File :context "src" "jazz" "command" "Highlight-Mode-Actions.jazz"})
          (<Jazz-File>       name: Search-Mode-Actions      source: {File :context "src" "jazz" "command" "Search-Mode-Actions.jazz"})
          (<Jazz-File>       name: _command                 source: {File :context "src" "jazz" "command" "_command.jazz"})
          (<Jazz-File>       name: Command-Toolbar          source: {File :context "src" "jazz" "command" "Command-Toolbar.jazz"})))))))
