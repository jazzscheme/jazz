;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.component  description-file: {File :context ".package"}
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                       directory: {Directory :context "src" "jazz" "component"}
          (<Jazz-File>       name: _component      source: {File :context "src" "jazz" "component" "_component.jazz"})
          (<Jazz-File>       name: autoload        source: {File :context "src" "jazz" "component" "autoload.jazz"})
          (<Jazz-File>       name: Branch          source: {File :context "src" "jazz" "component" "Branch.jazz"})
          (<Jazz-File>       name: Component       source: {File :context "src" "jazz" "component" "Component.jazz"})
          (<Jazz-File>       name: Data            source: {File :context "src" "jazz" "component" "Data.jazz"})
          (<Jazz-File>       name: Form            source: {File :context "src" "jazz" "component" "Form.jazz"})
          (<Jazz-File>       name: Restore-Form    source: {File :context "src" "jazz" "component" "Restore-Form.jazz"})
          (<Jazz-File>       name: Trait           source: {File :context "src" "jazz" "component" "Trait.jazz"})
          (<Project-File>                          source: {File :context "src" "jazz" "component" "install.scm"})
          (<Jazz-File>       name: Trait-Reference source: {File :context "src" "jazz" "component" "Trait-Reference.jazz"})
          (<Jazz-File>       name: syntax          source: {File :context "src" "jazz" "component" "syntax.jazz"})
          (<Jazz-File>       name: Group           source: {File :context "src" "jazz" "component" "Group.jazz"})
          (<Jazz-File>       name: descriptors     source: {File :context "src" "jazz" "component" "descriptors.jazz"})
          (<Jazz-File>       name: Former          source: {File :context "src" "jazz" "component" "Former.jazz"})))))))
