(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.console             catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "console"}
          (<Jazz-File>       name: _console                 source: {File :context "src" "jazz" "console" "_console.jazz"})
          (<Jazz-File>       name: autoload                 source: {File :context "src" "jazz" "console" "autoload.jazz"})
          (<Jazz-File>       name: Console-Preferences      source: {File :context "src" "jazz" "console" "Console-Preferences.jazz"})
          (<Jazz-File>       name: Console-Text-Actions     source: {File :context "src" "jazz" "console" "Console-Text-Actions.jazz"})
          (<Jazz-File>       name: Console-Text-Preferences source: {File :context "src" "jazz" "console" "Console-Text-Preferences.jazz"})
          (<Jazz-File>       name: Console-Text-View        source: {File :context "src" "jazz" "console" "Console-Text-View.jazz"})
          (<Jazz-File>       name: Console-View             source: {File :context "src" "jazz" "console" "Console-View.jazz"})
          (<Jazz-File>       name: Console-Manager          source: {File :context "src" "jazz" "console" "Console-Manager.jazz"})))))))
