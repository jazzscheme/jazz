(data jazz.ide.data.project


(import (jazz.editor.jazz)
        (jazz.project))


(form
  (<Project>                 name: jazz.core description-file: {File :context ".package"}
    (<Project-Directory>                     directory: {Directory :context "src"}
      (<Project-Directory>                   directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                 directory: {Directory :context "src" "jazz" "core"}
          (<Jazz-File>       name: _core     source: {File :context "src" "jazz" "core" "_core.jazz"})))))))
