;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.crash description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                  directory: {Directory :context "src" "jazz" "crash"}
          (<Jazz-File>       name: _crash     source: {File :context "src" "jazz" "crash" "_crash.jazz"})
          (<Jazz-File>       name: mac        source: {File :context "src" "jazz" "crash" "mac.jazz"})
          (<Jazz-File>       name: windows    source: {File :context "src" "jazz" "crash" "windows.jazz"})
          (<Project-File>                     source: {File :context "src" "jazz" "crash" "product.scm"})
          (<Jazz-File>       name: linux      source: {File :context "src" "jazz" "crash" "linux.jazz"})))))))
