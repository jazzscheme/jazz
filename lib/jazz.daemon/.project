;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.daemon     description-file: {File :context ".package"}
    (<Project-Directory>                               directory: {Directory :context "src"}
      (<Project-Directory>                             directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                           directory: {Directory :context "src" "jazz" "daemon"}
          (<Project-Directory>                         directory: {Directory :context "src" "jazz" "daemon" "platform"}
            (<Project-Directory>                       directory: {Directory :context "src" "jazz" "daemon" "platform" "windows"}
              (<Jazz-File>       name: autoload        source: {File :context "src" "jazz" "daemon" "platform" "windows" "autoload.jazz"})
              (<Jazz-File>       name: Service-Handle  source: {File :context "src" "jazz" "daemon" "platform" "windows" "Service-Handle.jazz"})
              (<Jazz-File>       name: Service-Manager source: {File :context "src" "jazz" "daemon" "platform" "windows" "Service-Manager.jazz"})
              (<Jazz-File>       name: Service         source: {File :context "src" "jazz" "daemon" "platform" "windows" "Service.jazz"})
              (<Jazz-File>       name: daemon          source: {File :context "src" "jazz" "daemon" "platform" "windows" "daemon.jazz"})))
          (<Jazz-File>           name: _daemon         source: {File :context "src" "jazz" "daemon" "_daemon.jazz"})))))))
