(library protected jazz.database.sqlserver.inventory.SQLServer-Datatype jazz


(import (jazz.database)
        (jazz.library)
        (jazz.schema))


(class SQLServer-Datatype extends Datatype
  
  
  ;; this method is temp for a design convertion utility
  (method meta public (call-construct-literal context elements)
    (apply construct-literal context elements))
  
  
  (method meta (get-systypes)
    '({Systype "bigint"           8  {} {} 0}
      {Systype "binary"           {} {} {} 0}
      {Systype "bit"              1  {} {} 0}
      {Systype "char"             {} {} {} ""}
      {Systype "datetime"         8  {} {} "1900-01-01"}
      {Systype "decimal"          9  18 0  0}
      {Systype "float"            8  {} {} 0}
      {Systype "image"            16 {} {} {}}
      {Systype "int"              4  {} {} 0}
      {Systype "money"            8  {} {} 0}
      {Systype "nchar"            {} {} {} ""}
      {Systype "ntext"            16 {} {} ""}
      {Systype "numeric"          9  18 0  0}
      {Systype "nvarchar"         {} {} {} ""}
      {Systype "real"             4  {} {} 0}
      {Systype "smalldatetime"    4  {} {} "1900-01-01"}
      {Systype "smallint"         2  {} {} 0}
      {Systype "smallmoney"       4  {} {} 0}
      {Systype "sql_variant"      0  {} {} {}}
      {Systype "text"             16 {} {} ""}
      {Systype "timestamp"        8  {} {} read-only}
      {Systype "tinyint"          1  {} {} 0}
      {Systype "uniqueidentifier" 16 {} {} {}}
      {Systype "varbinary"        {} {} {} {}}
      {Systype "varchar"          {} {} {} ""}))))
