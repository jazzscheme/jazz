            (<Jazz-File>       name: Provider              source: {File :context "src" "jazz" "database" "provider" "Provider.jazz"})
            (<Jazz-File>       name: Systype               source: {File :context "src" "jazz" "database" "provider" "Systype.jazz"}))
          (<Project-Directory>                             directory: {Directory :context "src" "jazz" "database" "view"}
