(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.debuggee.jazz    description-file: {File :context ".package"}
    (<Project-Directory>                                   directory: {Directory :context "src"}
      (<Project-Directory>                                 directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                               directory: {Directory :context "src" "jazz" "debuggee"}
          (<Project-Directory>                             directory: {Directory :context "src" "jazz" "debuggee" "jazz"}
            (<Jazz-File>       name: Jazz-Debuggee-Frame   source: {File :context "src" "jazz" "debuggee" "jazz" "Jazz-Debuggee-Frame.jazz"})
            (<Jazz-File>       name: Jazz-Debuggee-Process source: {File :context "src" "jazz" "debuggee" "jazz" "Jazz-Debuggee-Process.jazz"})
            (<Jazz-File>       name: Jazz-Debuggee-Restart source: {File :context "src" "jazz" "debuggee" "jazz" "Jazz-Debuggee-Restart.jazz"})
            (<Jazz-File>       name: Jazz-Debuggee-Stop    source: {File :context "src" "jazz" "debuggee" "jazz" "Jazz-Debuggee-Stop.jazz"})
            (<Jazz-File>       name: Jazz-Debuggee-Thread  source: {File :context "src" "jazz" "debuggee" "jazz" "Jazz-Debuggee-Thread.jazz"})
            (<Jazz-File>       name: autoload              source: {File :context "src" "jazz" "debuggee" "jazz" "autoload.jazz"})
            (<Jazz-File>       name: _jazz                 source: {File :context "src" "jazz" "debuggee" "jazz" "_jazz.jazz"})
            (<Jazz-File>       name: autoload-external     source: {File :context "src" "jazz" "debuggee" "jazz" "autoload-external.jazz"}))))))))
