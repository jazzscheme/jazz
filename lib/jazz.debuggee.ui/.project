(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.debuggee.ui   description-file: {File :context ".package"}
    (<Project-Directory>                                directory: {Directory :context "src"}
      (<Project-Directory>                              directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                            directory: {Directory :context "src" "jazz" "debuggee"}
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "debuggee" "ui"}
            (<Jazz-File>       name: _ui                source: {File :context "src" "jazz" "debuggee" "ui" "_ui.jazz"})
            (<Jazz-File>       name: autoload           source: {File :context "src" "jazz" "debuggee" "ui" "autoload.jazz"})
            (<Jazz-File>       name: stub               source: {File :context "src" "jazz" "debuggee" "ui" "stub.jazz"})
            (<Jazz-File>       name: UI-Debuggee        source: {File :context "src" "jazz" "debuggee" "ui" "UI-Debuggee.jazz"}))
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "debuggee" "view"}
            (<Jazz-File>       name: _view              source: {File :context "src" "jazz" "debuggee" "view" "_view.jazz"})
            (<Jazz-File>       name: autoload           source: {File :context "src" "jazz" "debuggee" "view" "autoload.jazz"})
            (<Jazz-File>       name: Exception-Log-Node source: {File :context "src" "jazz" "debuggee" "view" "Exception-Log-Node.jazz"})
            (<Jazz-File>       name: Log-Node           source: {File :context "src" "jazz" "debuggee" "view" "Log-Node.jazz"})
            (<Jazz-File>       name: Log-Tree           source: {File :context "src" "jazz" "debuggee" "view" "Log-Tree.jazz"})
            (<Jazz-File>       name: Log-View           source: {File :context "src" "jazz" "debuggee" "view" "Log-View.jazz"})
            (<Jazz-File>       name: Profiler-Manager   source: {File :context "src" "jazz" "debuggee" "view" "Profiler-Manager.jazz"})
            (<Jazz-File>       name: Profiler-View      source: {File :context "src" "jazz" "debuggee" "view" "Profiler-View.jazz"})
            (<Jazz-File>       name: Resources-Monitor  source: {File :context "src" "jazz" "debuggee" "view" "Resources-Monitor.jazz"})
            (<Jazz-File>       name: Resources-View     source: {File :context "src" "jazz" "debuggee" "view" "Resources-View.jazz"})
            (<Jazz-File>       name: View-Debugger      source: {File :context "src" "jazz" "debuggee" "view" "View-Debugger.jazz"}))))))))
