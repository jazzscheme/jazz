;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                           catalog?: #t catalog-extensions: :text include-extensions: :text catalog-name: jazz.debuggee
    (<Project-Directory>                               directory: {Directory :context "src"}
      (<Project-Directory>                             directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                           directory: {Directory :context "src" "jazz" "debuggee"}
          (<Project-Directory>                         directory: {Directory :context "src" "jazz" "debuggee" "view"}
            (<Jazz-File>       name: Log-View          source: {File :context "src" "jazz" "debuggee" "view" "Log-View.jazz"})
            (<Jazz-File>       name: Profiler-Manager  source: {File :context "src" "jazz" "debuggee" "view" "Profiler-Manager.jazz"})
            (<Jazz-File>       name: Profiler-View     source: {File :context "src" "jazz" "debuggee" "view" "Profiler-View.jazz"})
            (<Jazz-File>       name: Resources-Monitor source: {File :context "src" "jazz" "debuggee" "view" "Resources-Monitor.jazz"})
            (<Jazz-File>       name: Resources-View    source: {File :context "src" "jazz" "debuggee" "view" "Resources-View.jazz"})
            (<Jazz-File>       name: View-Debugger     source: {File :context "src" "jazz" "debuggee" "view" "View-Debugger.jazz"})
            (<Jazz-File>       name: _view             source: {File :context "src" "jazz" "debuggee" "view" "_view.jazz"}))
          (<Jazz-File>         name: _debuggee         source: {File :context "src" "jazz" "debuggee" "_debuggee.jazz"})
          (<Jazz-File>         name: autoload          source: {File :context "src" "jazz" "debuggee" "autoload.jazz"})
          (<Jazz-File>         name: Debuggee-Frame    source: {File :context "src" "jazz" "debuggee" "Debuggee-Frame.jazz"})
          (<Jazz-File>         name: Debuggee-Process  source: {File :context "src" "jazz" "debuggee" "Debuggee-Process.jazz"})
          (<Jazz-File>         name: Debuggee-Restart  source: {File :context "src" "jazz" "debuggee" "Debuggee-Restart.jazz"})
          (<Jazz-File>         name: Debuggee-Stop     source: {File :context "src" "jazz" "debuggee" "Debuggee-Stop.jazz"})
          (<Jazz-File>         name: Debuggee-Thread   source: {File :context "src" "jazz" "debuggee" "Debuggee-Thread.jazz"})
          (<Jazz-File>         name: setup             source: {File :context "src" "jazz" "debuggee" "setup.jazz"})
          (<Jazz-File>         name: stub              source: {File :context "src" "jazz" "debuggee" "stub.jazz"})
          (<Jazz-File>         name: update            source: {File :context "src" "jazz" "debuggee" "update.jazz"})))))))
