;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.debuggee description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "debuggee"}
          (<Jazz-File>       name: _debuggee     source: {File :context "src" "jazz" "debuggee" "_debuggee.jazz"})
          (<Jazz-File>       name: autoload      source: {File :context "src" "jazz" "debuggee" "autoload.jazz"})
          (<Jazz-File>       name: setup         source: {File :context "src" "jazz" "debuggee" "setup.jazz"})
          (<Jazz-File>       name: stub          source: {File :context "src" "jazz" "debuggee" "stub.jazz"})
          (<Jazz-File>       name: update        source: {File :context "src" "jazz" "debuggee" "update.jazz"})
          (<Jazz-File>       name: base          source: {File :context "src" "jazz" "debuggee" "base.jazz"})))))))
