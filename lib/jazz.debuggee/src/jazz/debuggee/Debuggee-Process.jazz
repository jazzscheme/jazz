(module protected jazz.debuggee.Debuggee-Process jazz


(class Debuggee-Process extends Object
  
  
  (method public virtual (get-id)
    )
  
  
  (method public virtual (get-title)
    )
  
  
  (method public virtual (get-traits)
    )
  
  
  (method public virtual (get-icon)
    )
  
  
  ;;;
  ;;;; Control
  ;;;
  
  
  (method public virtual (detach-process)
    )
  
  
  (method public virtual (reload-process)
    )
  
  
  (method public virtual (attach-to-debugger controller-register debugger-cookie)
    )
  
  
  (method public virtual (exit-process)
    )
  
  
  ;;;
  ;;;; Console
  ;;;
  
  
  (method public virtual (console-input thread-proxy str)
    )
  
  
  (method public virtual (console-close thread-proxy)
    )
  
  
  ;;;
  ;;;; Process
  ;;;
  
  
  (method public virtual (get-toplevel-hwnd)
    )
  
  
  ;;;
  ;;;; State
  ;;;
  
  
  (method public virtual (get-threads)
    )
  
  
  ;;;
  ;;;; Restart
  ;;;
  
  
  (method public virtual (restart-stop restart-proxy)
    )
  
  
  ;;;
  ;;;; Thread
  ;;;
  
  
  (method public virtual (break-thread thread-proxy)
    )
  
  
  (method public virtual (terminate-thread thread-proxy preserve-console?)
    )
  
  
  (method public virtual (kill-thread thread-proxy)
    )
  
  
  ;;;
  ;;;; Stop
  ;;;
  
  
  (method public virtual (continue-stop stop-proxy)
    )
  
  
  (method public virtual (step-stop stop-proxy command)
    )
  
  
  ;;;
  ;;;; Runtime
  ;;;
  
  
  (method public virtual (transmit-runtime-information what)
    )
  
  
  (method public virtual (transmit-product-runtime-information what product)
    )
  
  
  ;;;
  ;;;; Evaluate
  ;;;
  
  
  (method public virtual (evaluate-expressions forms syntax str container line col evaluation-mode walk-debugger walk-cookie)
    )
  
  
  ;;;
  ;;;; Profiler
  ;;;
  
  
  (method public virtual (profiler-start)
    )
  
  
  (method public virtual (profiler-stop)
    )


  (method public virtual (profiler-profiles)
    )
  
  
  (method public virtual (profiler-selected-profile)
    )
  
  
  (method public virtual (profiler-selected-profile-set! name)
    )
  
  
  (method public virtual (profiler-profile)
    )
  
  
  (method public virtual (profile-reset profiler depth all?)
    )
  
  
  ;;;
  ;;;; Inspector
  ;;;
  
  
  (method public virtual (inspect-serial serial max-content)
    )
  
  
  (method public virtual (inspect-continuation serial)
    )
  
  
  (method public virtual (present-serial serial)
    )
  
  
  ;;;
  ;;;; Designer
  ;;;
  
  
  (method public virtual (try-out class)
    )))
