;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.debugger.gambit description-file: {File :context ".package"}
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "debugger"}
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "debugger" "gambit"}
            (<Jazz-File>         name: _gambit              source: {File :context "src" "jazz" "debugger" "gambit" "_gambit.jazz"})
            (<Jazz-File>         name: autoload             source: {File :context "src" "jazz" "debugger" "gambit" "autoload.jazz"})
            (<Jazz-File>         name: Gambit-Debugger      source: {File :context "src" "jazz" "debugger" "gambit" "Gambit-Debugger.jazz"})
            (<Jazz-File>         name: Gambit-Project       source: {File :context "src" "jazz" "debugger" "gambit" "Gambit-Project.jazz"})
            (<Project-Directory>                            directory: {Directory :context "src" "jazz" "debugger" "gambit" "data"}
              (<Jazz-File>       name: project              source: {File :context "src" "jazz" "debugger" "gambit" "data" "project.jazz"}))
            (<Project-File>                                 source: {File :context "src" "jazz" "debugger" "gambit" "debugger.scm"})
            (<Project-File>                                 source: {File :context "src" "jazz" "debugger" "gambit" "rdi.scm"})
            (<Project-File>                                 source: {File :context "src" "jazz" "debugger" "gambit" "README"})
            (<Jazz-File>         name: descriptors          source: {File :context "src" "jazz" "debugger" "gambit" "descriptors.jazz"}))))))))
