;;;==============
;;;  JazzScheme
;;;==============
;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.debugger.jazz catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                                  directory: {Directory :context "src"}
      (<Project-Directory>                                directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                              directory: {Directory :context "src" "jazz" "debugger"}
          (<Project-Directory>                            directory: {Directory :context "src" "jazz" "debugger" "jazz"}
            (<Jazz-File>         name: _jazz              source: {File :context "src" "jazz" "debugger" "jazz" "_jazz.jazz"})
            (<Jazz-File>         name: autoload           source: {File :context "src" "jazz" "debugger" "jazz" "autoload.jazz"})
            (<Jazz-File>         name: Jazz-Project       source: {File :context "src" "jazz" "debugger" "jazz" "Jazz-Project.jazz"})
            (<Jazz-File>         name: stub-autoload      source: {File :context "src" "jazz" "debugger" "jazz" "stub-autoload.jazz"})
            (<Jazz-File>         name: stub               source: {File :context "src" "jazz" "debugger" "jazz" "stub.jazz"})
            (<Jazz-File>         name: Jazz-Debugger      source: {File :context "src" "jazz" "debugger" "jazz" "Jazz-Debugger.jazz"})
            (<Jazz-File>         name: Snapshot-Debugger  source: {File :context "src" "jazz" "debugger" "jazz" "Snapshot-Debugger.jazz"})
            (<Jazz-File>         name: Swank-Debugger     source: {File :context "src" "jazz" "debugger" "jazz" "Swank-Debugger.jazz"})
            (<Jazz-File>         name: Swank-Snooper      source: {File :context "src" "jazz" "debugger" "jazz" "Swank-Snooper.jazz"})
            (<Jazz-File>         name: Swank-Process      source: {File :context "src" "jazz" "debugger" "jazz" "Swank-Process.jazz"})
            (<Jazz-File>         name: Swank-Stop         source: {File :context "src" "jazz" "debugger" "jazz" "Swank-Stop.jazz"})
            (<Jazz-File>         name: Swank-Thread       source: {File :context "src" "jazz" "debugger" "jazz" "Swank-Thread.jazz"})
            (<Jazz-File>         name: Swank-Restart      source: {File :context "src" "jazz" "debugger" "jazz" "Swank-Restart.jazz"})
            (<Project-Directory>                          directory: {Directory :context "src" "jazz" "debugger" "jazz" "data"}
              (<Jazz-File>       name: project            source: {File :context "src" "jazz" "debugger" "jazz" "data" "project.jazz"}))
            (<Jazz-File>         name: Jazz-Configuration source: {File :context "src" "jazz" "debugger" "jazz" "Jazz-Configuration.jazz"}))))))))
