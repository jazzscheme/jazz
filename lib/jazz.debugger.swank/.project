(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.debugger.swank description-file: {File :context ".package"}
    (<Project-Directory>                                   directory: {Directory :context "src"}
      (<Project-Directory>                                 directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                               directory: {Directory :context "src" "jazz" "debugger"}
          (<Project-Directory>                             directory: {Directory :context "src" "jazz" "debugger" "swank"}
            (<Jazz-File>         name: _swank              source: {File :context "src" "jazz" "debugger" "swank" "_swank.jazz"})
            (<Jazz-File>         name: autoload            source: {File :context "src" "jazz" "debugger" "swank" "autoload.jazz"})
            (<Jazz-File>         name: Swank-Debugger      source: {File :context "src" "jazz" "debugger" "swank" "Swank-Debugger.jazz"})
            (<Jazz-File>         name: Swank-Snooper       source: {File :context "src" "jazz" "debugger" "swank" "Swank-Snooper.jazz"})
            (<Jazz-File>         name: Swank-Process       source: {File :context "src" "jazz" "debugger" "swank" "Swank-Process.jazz"})
            (<Jazz-File>         name: Swank-Stop          source: {File :context "src" "jazz" "debugger" "swank" "Swank-Stop.jazz"})
            (<Jazz-File>         name: Swank-Thread        source: {File :context "src" "jazz" "debugger" "swank" "Swank-Thread.jazz"})
            (<Jazz-File>         name: Swank-Restart       source: {File :context "src" "jazz" "debugger" "swank" "Swank-Restart.jazz"}))))))))
