(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.debugger      catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                                directory: {Directory :context "src"}
      (<Project-Directory>                              directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                            directory: {Directory :context "src" "jazz" "debugger"}
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "debugger" "break"}
            (<Jazz-File>       name: Break-Event        source: {File :context "src" "jazz" "debugger" "break" "Break-Event.jazz"}))
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "debugger" "debugged"}
            (<Jazz-File>       name: Debugged-Frame     source: {File :context "src" "jazz" "debugger" "debugged" "Debugged-Frame.jazz"})
            (<Jazz-File>       name: Debugged-Process   source: {File :context "src" "jazz" "debugger" "debugged" "Debugged-Process.jazz"})
            (<Jazz-File>       name: Debugged-Restart   source: {File :context "src" "jazz" "debugger" "debugged" "Debugged-Restart.jazz"})
            (<Jazz-File>       name: Debugged-Stop      source: {File :context "src" "jazz" "debugger" "debugged" "Debugged-Stop.jazz"})
            (<Jazz-File>       name: Debugged-Thread    source: {File :context "src" "jazz" "debugger" "debugged" "Debugged-Thread.jazz"})
            (<Jazz-File>       name: Debugged           source: {File :context "src" "jazz" "debugger" "debugged" "Debugged.jazz"}))
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "debugger" "debugger"}
            (<Jazz-File>       name: Debugger-Event     source: {File :context "src" "jazz" "debugger" "debugger" "Debugger-Event.jazz"})
            (<Jazz-File>       name: Debugger-Listener  source: {File :context "src" "jazz" "debugger" "debugger" "Debugger-Listener.jazz"})
            (<Jazz-File>       name: Debugger-Resource  source: {File :context "src" "jazz" "debugger" "debugger" "Debugger-Resource.jazz"})
            (<Jazz-File>       name: Debugger           source: {File :context "src" "jazz" "debugger" "debugger" "Debugger.jazz"})
            (<Jazz-File>       name: Exception-View     source: {File :context "src" "jazz" "debugger" "debugger" "Exception-View.jazz"})
            (<Jazz-File>       name: Frame-Indicator    source: {File :context "src" "jazz" "debugger" "debugger" "Frame-Indicator.jazz"})
            (<Jazz-File>       name: Frames-View        source: {File :context "src" "jazz" "debugger" "debugger" "Frames-View.jazz"})
            (<Jazz-File>       name: Processes-View     source: {File :context "src" "jazz" "debugger" "debugger" "Processes-View.jazz"})
            (<Jazz-File>       name: Threads-View       source: {File :context "src" "jazz" "debugger" "debugger" "Threads-View.jazz"})
            (<Jazz-File>       name: Variables-View     source: {File :context "src" "jazz" "debugger" "debugger" "Variables-View.jazz"})
            (<Jazz-File>       name: Debugger-Manager   source: {File :context "src" "jazz" "debugger" "debugger" "Debugger-Manager.jazz"})
            (<Jazz-File>       name: Restarts-View      source: {File :context "src" "jazz" "debugger" "debugger" "Restarts-View.jazz"}))
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "debugger" "preference"}
            (<Jazz-File>       name: Debug-Preferences  source: {File :context "src" "jazz" "debugger" "preference" "Debug-Preferences.jazz"}))
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "debugger" "snapshot"}
            (<Jazz-File>       name: Computer-Snapshot  source: {File :context "src" "jazz" "debugger" "snapshot" "Computer-Snapshot.jazz"})
            (<Jazz-File>       name: Exception-Snapshot source: {File :context "src" "jazz" "debugger" "snapshot" "Exception-Snapshot.jazz"})
            (<Jazz-File>       name: Frame-Snapshot     source: {File :context "src" "jazz" "debugger" "snapshot" "Frame-Snapshot.jazz"})
            (<Jazz-File>       name: Process-Snapshot   source: {File :context "src" "jazz" "debugger" "snapshot" "Process-Snapshot.jazz"})
            (<Jazz-File>       name: Stack-Snapshot     source: {File :context "src" "jazz" "debugger" "snapshot" "Stack-Snapshot.jazz"})
            (<Jazz-File>       name: Thread-Snapshot    source: {File :context "src" "jazz" "debugger" "snapshot" "Thread-Snapshot.jazz"}))
          (<Jazz-File>         name: autoload           source: {File :context "src" "jazz" "debugger" "autoload.jazz"})
          (<Jazz-File>         name: _debugger          source: {File :context "src" "jazz" "debugger" "_debugger.jazz"})))))))
