;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.descriptor       description-file: {File :context ".package"}
    (<Project-Directory>                                 directory: {Directory :context "src"}
      (<Project-Directory>                               directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                             directory: {Directory :context "src" "jazz" "descriptor"}
          (<Jazz-File>       name: _descriptor           source: {File :context "src" "jazz" "descriptor" "_descriptor.jazz"})
          (<Jazz-File>       name: autoload              source: {File :context "src" "jazz" "descriptor" "autoload.jazz"})
          (<Jazz-File>       name: Child-Descriptor      source: {File :context "src" "jazz" "descriptor" "Child-Descriptor.jazz"})
          (<Jazz-File>       name: Children-Descriptor   source: {File :context "src" "jazz" "descriptor" "Children-Descriptor.jazz"})
          (<Jazz-File>       name: Component-Descriptor  source: {File :context "src" "jazz" "descriptor" "Component-Descriptor.jazz"})
          (<Jazz-File>       name: Properties-Descriptor source: {File :context "src" "jazz" "descriptor" "Properties-Descriptor.jazz"})
          (<Jazz-File>       name: Property-Descriptor   source: {File :context "src" "jazz" "descriptor" "Property-Descriptor.jazz"})))))))
