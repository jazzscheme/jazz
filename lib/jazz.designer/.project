;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.designer        documentation: {File :context "doc" "designer.jml"} description-file: {File :context ".package"}
    (<Project-Directory>                                  directory: {Directory :context "src"}
      (<Project-Directory>                                directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                              directory: {Directory :context "src" "jazz" "designer"}
          (<Jazz-File>         name: _designer            source: {File :context "src" "jazz" "designer" "_designer.jazz"})
          (<Jazz-File>         name: autoload             source: {File :context "src" "jazz" "designer" "autoload.jazz"})
          (<Jazz-File>         name: Designer             source: {File :context "src" "jazz" "designer" "Designer.jazz"})
          (<Jazz-File>         name: Dispatch-Designer    source: {File :context "src" "jazz" "designer" "Dispatch-Designer.jazz"})
          (<Project-Directory>                            directory: {Directory :context "src" "jazz" "designer" "clipboard"}
            (<Jazz-File>       name: Component-Parcel     source: {File :context "src" "jazz" "designer" "clipboard" "Component-Parcel.jazz"}))
          (<Project-File>                                 source: {File :context "src" "jazz" "designer" "install.scm"})
          (<Jazz-File>         name: literals             source: {File :context "src" "jazz" "designer" "literals.jazz"})
          (<Project-Directory>                            directory: {Directory :context "src" "jazz" "designer" "moniker"}
            (<Jazz-File>       name: Property-Moniker     source: {File :context "src" "jazz" "designer" "moniker" "Property-Moniker.jazz"}))
          (<Project-Directory>                            directory: {Directory :context "src" "jazz" "designer" "preference"}
            (<Jazz-File>       name: Designer-Preferences source: {File :context "src" "jazz" "designer" "preference" "Designer-Preferences.jazz"}))
          (<Jazz-File>         name: syntax               source: {File :context "src" "jazz" "designer" "syntax.jazz"})
          (<Project-Directory>                            directory: {Directory :context "src" "jazz" "designer" "validate"}
            (<Jazz-File>       name: _validate            source: {File :context "src" "jazz" "designer" "validate" "_validate.jazz"})))))
    (<Project-Directory>                                  directory: {Directory :context "doc"}
      (<Project-File>                                     source: {File :context "doc" "designer.jml"})))))
