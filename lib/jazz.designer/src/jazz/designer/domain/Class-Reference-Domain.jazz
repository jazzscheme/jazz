(module jazz.designer.domain.Class-Reference-Domain jazz


(import (jazz.designer))


(class Class-Reference-Domain extends Domain
  
  
  (property classes initialize #f accessors generate)
  
  
  (method override (present reference)
    (symbol->string (category-name (resolve-runtime-reference reference))))
  
  
  (method override (setup-entry view client)
    (new Class-Reference-Entry parent: view size: (get-size~ view) domain: self classes: classes))))
