(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.digest description-file: {File :context ".package"}
    (<Project-Directory>                       directory: {Directory :context "src"}
      (<Project-Directory>                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                   directory: {Directory :context "src" "jazz" "digest"}
          (<Jazz-File>       name: _digest     source: {File :context "src" "jazz" "digest" "_digest.jazz"})
          (<Project-File>                      source: {File :context "src" "jazz" "digest" "install.scm"})))))))
