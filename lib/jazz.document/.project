;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.document         description-file: {File :context ".package"}
    (<Project-Directory>                                 directory: {Directory :context "src"}
      (<Project-Directory>                               directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                             directory: {Directory :context "src" "jazz" "document"}
          (<Jazz-File>       name: Document-Controller   source: {File :context "src" "jazz" "document" "Document-Controller.jazz"})
          (<Jazz-File>       name: Document-Moniker      source: {File :context "src" "jazz" "document" "Document-Moniker.jazz"})
          (<Jazz-File>       name: Document-Preferences  source: {File :context "src" "jazz" "document" "Document-Preferences.jazz"})
          (<Jazz-File>       name: Document              source: {File :context "src" "jazz" "document" "Document.jazz"})
          (<Jazz-File>       name: Documents-Preferences source: {File :context "src" "jazz" "document" "Documents-Preferences.jazz"})
          (<Jazz-File>       name: _document             source: {File :context "src" "jazz" "document" "_document.jazz"})))))))
