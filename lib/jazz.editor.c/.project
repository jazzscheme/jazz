;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.c)
        (jazz.editor.jazz))


(form
  (<Project>                                              catalog?: #t catalog-extensions: :text include-extensions: :text catalog-name: jazz.editor.c
    (<Project-Directory>                                  directory: {Directory :context "src"}
      (<Project-Directory>                                directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                              directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                            directory: {Directory :context "src" "jazz" "editor" "c"}
            (<Project-Directory>                          directory: {Directory :context "src" "jazz" "editor" "c" "builder"}
              (<Jazz-File>       name: C-File             source: {File :context "src" "jazz" "editor" "c" "builder" "C-File.jazz"}))
            (<Project-Directory>                          directory: {Directory :context "src" "jazz" "editor" "c" "catalog"}
              (<Jazz-File>       name: C-Category-Entry   source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Category-Entry.jazz"})
              (<Jazz-File>       name: C-Define-Entry     source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Define-Entry.jazz"})
              (<Jazz-File>       name: C-Definition-Entry source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Definition-Entry.jazz"})
              (<Jazz-File>       name: C-Entry            source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Entry.jazz"})
              (<Jazz-File>       name: C-Export-Entry     source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Export-Entry.jazz"})
              (<Jazz-File>       name: C-Extern-Entry     source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Extern-Entry.jazz"})
              (<Jazz-File>       name: C-File-Entry       source: {File :context "src" "jazz" "editor" "c" "catalog" "C-File-Entry.jazz"})
              (<Jazz-File>       name: C-Include-Entry    source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Include-Entry.jazz"})
              (<Jazz-File>       name: C-Method-Entry     source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Method-Entry.jazz"})
              (<Jazz-File>       name: C-Static-Entry     source: {File :context "src" "jazz" "editor" "c" "catalog" "C-Static-Entry.jazz"}))
            (<Project-Directory>                          directory: {Directory :context "src" "jazz" "editor" "c" "parser"}
              (<Jazz-File>       name: C-Parser           source: {File :context "src" "jazz" "editor" "c" "parser" "C-Parser.jazz"}))
            (<Project-Directory>                          directory: {Directory :context "src" "jazz" "editor" "c" "syntax"}
              (<Jazz-File>       name: C-Syntax           source: {File :context "src" "jazz" "editor" "c" "syntax" "C-Syntax.jazz"}))
            (<Project-Directory>                          directory: {Directory :context "src" "jazz" "editor" "c" "text"}
              (<Jazz-File>       name: C-Explorer         source: {File :context "src" "jazz" "editor" "c" "text" "C-Explorer.jazz"})
              (<Jazz-File>       name: C-Text-Preferences source: {File :context "src" "jazz" "editor" "c" "text" "C-Text-Preferences.jazz"})
              (<Jazz-File>       name: C-Text-View        source: {File :context "src" "jazz" "editor" "c" "text" "C-Text-View.jazz"}))
            (<Jazz-File>         name: _c                 source: {File :context "src" "jazz" "editor" "c" "_c.jazz"})
            (<Jazz-File>         name: autoload           source: {File :context "src" "jazz" "editor" "c" "autoload.jazz"})
            (<Project-File>                               source: {File :context "src" "jazz" "editor" "c" "install.scm"})
            (<Jazz-File>         name: literals           source: {File :context "src" "jazz" "editor" "c" "literals.jazz"}))))))))
