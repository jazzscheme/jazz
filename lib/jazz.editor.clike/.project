(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.clike      description-file: {File :context ".package"}
    (<Project-Directory>                                      directory: {Directory :context "src"}
      (<Project-Directory>                                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                  directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                directory: {Directory :context "src" "jazz" "editor" "clike"}
            (<Project-Directory>                              directory: {Directory :context "src" "jazz" "editor" "clike" "text"}
              (<Jazz-File>       name: CLike-Text-Preferences source: {File :context "src" "jazz" "editor" "clike" "text" "CLike-Text-Preferences.jazz"})
              (<Jazz-File>       name: CLike-Text-View        source: {File :context "src" "jazz" "editor" "clike" "text" "CLike-Text-View.jazz"})
              (<Jazz-File>       name: CLike-Syntax           source: {File :context "src" "jazz" "editor" "clike" "text" "CLike-Syntax.jazz"}))
            (<Jazz-File>         name: _clike                 source: {File :context "src" "jazz" "editor" "clike" "_clike.jazz"})
            (<Jazz-File>         name: autoload               source: {File :context "src" "jazz" "editor" "clike" "autoload.jazz"}))))))))
