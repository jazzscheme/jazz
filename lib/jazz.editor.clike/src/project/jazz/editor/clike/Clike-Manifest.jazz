(library project.jazz.editor.clike.Clike-Manifest jazz


(import (jazz.io)
        (jazz.jml)
        (jedi))


(class Clike-Manifest extends Project
  
  
  (form
    (<install>                                                  title: "jazz.editor.clike" catalog?: #t catalog-extensions: :text include-extensions: :text module-name: project.jazz.editor.clike.Clike-Manifest
      (<Project-Directory>                                      directory: {Directory jazz.editor.clike "src"}
        (<Project-Directory>                                    directory: {Directory jazz.editor.clike "src" "jazz"}
          (<Project-Directory>                                  directory: {Directory jazz.editor.clike "src" "jazz" "editor"}
            (<Project-Directory>                                directory: {Directory jazz.editor.clike "src" "jazz" "editor" "clike"}
              (<Project-Directory>                              directory: {Directory jazz.editor.clike "src" "jazz" "editor" "clike" "text"}
                (<Jazz-File>       name: CLike-Text-Preferences source: {File jazz.editor.clike "src" "jazz" "editor" "clike" "text" "CLike-Text-Preferences.jazz"})
                (<Jazz-File>       name: CLike-Text-View        source: {File jazz.editor.clike "src" "jazz" "editor" "clike" "text" "CLike-Text-View.jazz"}))
              (<Jazz-File>         name: _clike                 source: {File jazz.editor.clike "src" "jazz" "editor" "clike" "_clike.jazz"})
              (<Jazz-File>         name: autoload               source: {File jazz.editor.clike "src" "jazz" "editor" "clike" "autoload.jazz"}))))))))


(register-autoclass Clike-Manifest))
