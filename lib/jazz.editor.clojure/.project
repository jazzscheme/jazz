;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.clojure      description-file: {File :context ".package"}
    (<Project-Directory>                                        directory: {Directory :context "src"}
      (<Project-Directory>                                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                    directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                  directory: {Directory :context "src" "jazz" "editor" "clojure"}
            (<Project-Directory>                                directory: {Directory :context "src" "jazz" "editor" "clojure" "parser"}
              (<Jazz-File>       name: Clojure-Parser           source: {File :context "src" "jazz" "editor" "clojure" "parser" "Clojure-Parser.jazz"}))
            (<Project-Directory>                                directory: {Directory :context "src" "jazz" "editor" "clojure" "text"}
              (<Jazz-File>       name: Clojure-Explorer         source: {File :context "src" "jazz" "editor" "clojure" "text" "Clojure-Explorer.jazz"})
              (<Jazz-File>       name: Clojure-Text-Preferences source: {File :context "src" "jazz" "editor" "clojure" "text" "Clojure-Text-Preferences.jazz"})
              (<Jazz-File>       name: Clojure-Text-View        source: {File :context "src" "jazz" "editor" "clojure" "text" "Clojure-Text-View.jazz"})
              (<Jazz-File>       name: Clojure-Syntax           source: {File :context "src" "jazz" "editor" "clojure" "text" "Clojure-Syntax.jazz"}))
            (<Jazz-File>         name: _clojure                 source: {File :context "src" "jazz" "editor" "clojure" "_clojure.jazz"})
            (<Jazz-File>         name: autoload                 source: {File :context "src" "jazz" "editor" "clojure" "autoload.jazz"})
            (<Jazz-File>         name: forms                    source: {File :context "src" "jazz" "editor" "clojure" "forms.jazz"})
            (<Project-File>                                     source: {File :context "src" "jazz" "editor" "clojure" "install.scm"})
            (<Project-Directory>                                directory: {Directory :context "src" "jazz" "editor" "clojure" "couple"}
              (<Jazz-File>       name: catalog                  source: {File :context "src" "jazz" "editor" "clojure" "couple" "catalog.jazz"})))))))))
