(module jazz.editor.commonlisp.forms jazz


(import (jazz.editor.lisp)
        (jazz.editor.commonlisp))


;;;
;;;; Define
;;;


(define-expression defclass
  dialect: commonlisp
  declaration?: #t
  tabulate: 2
  walk: :define-variable)

(define-expression defgeneric
  dialect: commonlisp
  declaration?: #t
  tabulate: 2
  walk: :define)

(define-expression defmacro
  dialect: commonlisp
  declaration?: #t
  tabulate: 2
  walk: :define-macro)

(define-expression defmethod
  dialect: commonlisp
  declaration?: #t
  tabulate: 2
  walk: :define)

(define-expression defpackage
  dialect: commonlisp
  declaration?: #t
  tabulate: 1
  walk: :define-variable)

(define-expression defparameter
  dialect: commonlisp
  declaration?: #t
  tabulate: 1
  walk: :define-variable)

(define-expression defstruct
  dialect: commonlisp
  declaration?: #t
  tabulate: 1
  walk: :define-variable)

(define-expression deftype
  dialect: commonlisp
  declaration?: #t
  tabulate: 2
  walk: :define)

(define-expression defun
  dialect: commonlisp
  declaration?: #t
  tabulate: 2
  walk: :define)

(define-expression defvar
  dialect: commonlisp
  declaration?: #t
  tabulate: 1
  walk: :define-variable)

(define-expression define-condition
  dialect: commonlisp
  declaration?: #t
  tabulate: 3
  walk: :define-variable)

(define-expression define-modify-macro
  dialect: commonlisp
  declaration?: #t
  tabulate: 2
  walk: :define-variable)


;;;
;;;; Syntax
;;;


(define-expression eval-when
  dialect: commonlisp
  tabulate: 0
  walk: :eval-when))
