;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.dart        description-file: {File :context ".package"}
    (<Project-Directory>                                       directory: {Directory :context "src"}
      (<Project-Directory>                                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "editor" "dart"}
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "dart" "parser"}
              (<Jazz-File>       name: Dart-Parser             source: {File :context "src" "jazz" "editor" "dart" "parser" "Dart-Parser.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "dart" "syntax"}
              (<Jazz-File>       name: Dart-Syntax             source: {File :context "src" "jazz" "editor" "dart" "syntax" "Dart-Syntax.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "dart" "text"}
              (<Jazz-File>       name: Dart-Explorer           source: {File :context "src" "jazz" "editor" "dart" "text" "Dart-Explorer.jazz"})
              (<Jazz-File>       name: Dart-Text-Preferences   source: {File :context "src" "jazz" "editor" "dart" "text" "Dart-Text-Preferences.jazz"})
              (<Jazz-File>       name: Dart-Text-View          source: {File :context "src" "jazz" "editor" "dart" "text" "Dart-Text-View.jazz"}))
            (<Jazz-File>         name: _dart                   source: {File :context "src" "jazz" "editor" "dart" "_dart.jazz"})
            (<Jazz-File>         name: autoload                source: {File :context "src" "jazz" "editor" "dart" "autoload.jazz"})
            (<Project-File>                                    source: {File :context "src" "jazz" "editor" "dart" "install.scm"})
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "dart" "couple"}
              (<Jazz-File>       name: catalog                 source: {File :context "src" "jazz" "editor" "dart" "couple" "catalog.jazz"}))
            (<Jazz-File>         name: descriptors             source: {File :context "src" "jazz" "editor" "dart" "descriptors.jazz"}))))))))
