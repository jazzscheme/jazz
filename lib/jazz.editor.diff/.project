;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.diff
    (<Project-Directory>                                     directory: {Directory :context "src"}
      (<Project-Directory>                                   directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "diff"}
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "diff" "text"}
              (<Jazz-File>       name: Diff-Text-Preferences source: {File :context "src" "jazz" "editor" "diff" "text" "Diff-Text-Preferences.jazz"})
              (<Jazz-File>       name: Diff-Text-View        source: {File :context "src" "jazz" "editor" "diff" "text" "Diff-Text-View.jazz"}))
            (<Jazz-File>         name: _diff                 source: {File :context "src" "jazz" "editor" "diff" "_diff.jazz"})
            (<Jazz-File>         name: autoload              source: {File :context "src" "jazz" "editor" "diff" "autoload.jazz"}))))))))
