(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.java      description-file: {File :context ".package"}
    (<Project-Directory>                                     directory: {Directory :context "src"}
      (<Project-Directory>                                   directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "java"}
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "java" "catalog"}
              (<Jazz-File>       name: Java-Category-Entry   source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Category-Entry.jazz"})
              (<Jazz-File>       name: Java-Define-Entry     source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Define-Entry.jazz"})
              (<Jazz-File>       name: Java-Definition-Entry source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Definition-Entry.jazz"})
              (<Jazz-File>       name: Java-Entry            source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Entry.jazz"})
              (<Jazz-File>       name: Java-Export-Entry     source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Export-Entry.jazz"})
              (<Jazz-File>       name: Java-Extern-Entry     source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Extern-Entry.jazz"})
              (<Jazz-File>       name: Java-File-Entry       source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-File-Entry.jazz"})
              (<Jazz-File>       name: Java-Method-Entry     source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Method-Entry.jazz"})
              (<Jazz-File>       name: Java-Static-Entry     source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Static-Entry.jazz"})
              (<Jazz-File>       name: Java-Import-Entry     source: {File :context "src" "jazz" "editor" "java" "catalog" "Java-Import-Entry.jazz"}))
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "java" "parser"}
              (<Jazz-File>       name: Java-Parser           source: {File :context "src" "jazz" "editor" "java" "parser" "Java-Parser.jazz"}))
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "java" "syntax"}
              (<Jazz-File>       name: Java-Syntax           source: {File :context "src" "jazz" "editor" "java" "syntax" "Java-Syntax.jazz"}))
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "java" "text"}
              (<Jazz-File>       name: Java-Explorer         source: {File :context "src" "jazz" "editor" "java" "text" "Java-Explorer.jazz"})
              (<Jazz-File>       name: Java-Text-Preferences source: {File :context "src" "jazz" "editor" "java" "text" "Java-Text-Preferences.jazz"})
              (<Jazz-File>       name: Java-Text-View        source: {File :context "src" "jazz" "editor" "java" "text" "Java-Text-View.jazz"}))
            (<Jazz-File>         name: _java                 source: {File :context "src" "jazz" "editor" "java" "_java.jazz"})
            (<Jazz-File>         name: autoload              source: {File :context "src" "jazz" "editor" "java" "autoload.jazz"})
            (<Project-File>                                  source: {File :context "src" "jazz" "editor" "java" "install.scm"})
            (<Jazz-File>         name: literals              source: {File :context "src" "jazz" "editor" "java" "literals.jazz"})
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "java" "couple"}
              (<Jazz-File>       name: catalog               source: {File :context "src" "jazz" "editor" "java" "couple" "catalog.jazz"}))
            (<Jazz-File>         name: descriptors           source: {File :context "src" "jazz" "editor" "java" "descriptors.jazz"}))))))))
