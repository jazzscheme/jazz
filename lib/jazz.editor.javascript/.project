;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.javascript      description-file: {File :context ".package"}
    (<Project-Directory>                                           directory: {Directory :context "src"}
      (<Project-Directory>                                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                       directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                     directory: {Directory :context "src" "jazz" "editor" "javascript"}
            (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "editor" "javascript" "text"}
              (<Jazz-File>       name: JavaScript-Explorer         source: {File :context "src" "jazz" "editor" "javascript" "text" "JavaScript-Explorer.jazz"})
              (<Jazz-File>       name: JavaScript-Syntax           source: {File :context "src" "jazz" "editor" "javascript" "text" "JavaScript-Syntax.jazz"})
              (<Jazz-File>       name: JavaScript-Text-Preferences source: {File :context "src" "jazz" "editor" "javascript" "text" "JavaScript-Text-Preferences.jazz"})
              (<Jazz-File>       name: JavaScript-Text-View        source: {File :context "src" "jazz" "editor" "javascript" "text" "JavaScript-Text-View.jazz"}))
            (<Jazz-File>         name: _javascript                 source: {File :context "src" "jazz" "editor" "javascript" "_javascript.jazz"})
            (<Jazz-File>         name: autoload                    source: {File :context "src" "jazz" "editor" "javascript" "autoload.jazz"})
            (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "editor" "javascript" "parser"}
              (<Jazz-File>       name: JavaScript-Parser           source: {File :context "src" "jazz" "editor" "javascript" "parser" "JavaScript-Parser.jazz"})))))))))
