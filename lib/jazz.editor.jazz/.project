;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.jazz
    (<Project-Directory>                                       directory: {Directory :context "src"}
      (<Project-Directory>                                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "editor" "jazz"}
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "builder"}
              (<Jazz-File>       name: Jazz-File               source: {File :context "src" "jazz" "editor" "jazz" "builder" "Jazz-File.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "inspector"}
              (<Jazz-File>       name: _inspector              source: {File :context "src" "jazz" "editor" "jazz" "inspector" "_inspector.jazz"})
              (<Jazz-File>       name: Inspector-View          source: {File :context "src" "jazz" "editor" "jazz" "inspector" "Inspector-View.jazz"})
              (<Jazz-File>       name: Inspector               source: {File :context "src" "jazz" "editor" "jazz" "inspector" "Inspector.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "log"}
              (<Jazz-File>       name: Logs-Viewer             source: {File :context "src" "jazz" "editor" "jazz" "log" "Logs-Viewer.jazz"})
              (<Jazz-File>       name: User-Log                source: {File :context "src" "jazz" "editor" "jazz" "log" "User-Log.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "parser"}
              (<Jazz-File>       name: Jazz-Parser             source: {File :context "src" "jazz" "editor" "jazz" "parser" "Jazz-Parser.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "profiler"}
              (<Jazz-File>       name: Profile-Results         source: {File :context "src" "jazz" "editor" "jazz" "profiler" "Profile-Results.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "registry"}
              (<Jazz-File>       name: Registry-Viewer         source: {File :context "src" "jazz" "editor" "jazz" "registry" "Registry-Viewer.jazz"})
              (<Jazz-File>       name: Type-Extractor          source: {File :context "src" "jazz" "editor" "jazz" "registry" "Type-Extractor.jazz"})
              (<Jazz-File>       name: Type-Viewer             source: {File :context "src" "jazz" "editor" "jazz" "registry" "Type-Viewer.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "spy"}
              (<Jazz-File>       name: Event-Spy               source: {File :context "src" "jazz" "editor" "jazz" "spy" "Event-Spy.jazz"})
              (<Jazz-File>       name: Spy-View                source: {File :context "src" "jazz" "editor" "jazz" "spy" "Spy-View.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "syntax"}
              (<Jazz-File>       name: Jazz-Syntax             source: {File :context "src" "jazz" "editor" "jazz" "syntax" "Jazz-Syntax.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "text"}
              (<Jazz-File>       name: Jazz-Code-Location      source: {File :context "src" "jazz" "editor" "jazz" "text" "Jazz-Code-Location.jazz"})
              (<Jazz-File>       name: Jazz-Explorer           source: {File :context "src" "jazz" "editor" "jazz" "text" "Jazz-Explorer.jazz"})
              (<Jazz-File>       name: Jazz-Plain-Text-View    source: {File :context "src" "jazz" "editor" "jazz" "text" "Jazz-Plain-Text-View.jazz"})
              (<Jazz-File>       name: Jazz-Text-Actions       source: {File :context "src" "jazz" "editor" "jazz" "text" "Jazz-Text-Actions.jazz"})
              (<Jazz-File>       name: Jazz-Text-Preferences   source: {File :context "src" "jazz" "editor" "jazz" "text" "Jazz-Text-Preferences.jazz"})
              (<Jazz-File>       name: Jazz-Text-View          source: {File :context "src" "jazz" "editor" "jazz" "text" "Jazz-Text-View.jazz"})
              (<Jazz-File>       name: Location-Error          source: {File :context "src" "jazz" "editor" "jazz" "text" "Location-Error.jazz"})
              (<Jazz-File>       name: Jazz-Plain-Syntax       source: {File :context "src" "jazz" "editor" "jazz" "text" "Jazz-Plain-Syntax.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "tools"}
              (<Jazz-File>       name: Class-Browser-Actions   source: {File :context "src" "jazz" "editor" "jazz" "tools" "Class-Browser-Actions.jazz"})
              (<Jazz-File>       name: Class-Browser           source: {File :context "src" "jazz" "editor" "jazz" "tools" "Class-Browser.jazz"})
              (<Jazz-File>       name: References-Analyser     source: {File :context "src" "jazz" "editor" "jazz" "tools" "References-Analyser.jazz"}))
            (<Jazz-File>         name: _jazz                   source: {File :context "src" "jazz" "editor" "jazz" "_jazz.jazz"})
            (<Jazz-File>         name: autoload                source: {File :context "src" "jazz" "editor" "jazz" "autoload.jazz"})
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "jazz" "documentation"}
              (<Jazz-File>       name: Documentation-Text-View source: {File :context "src" "jazz" "editor" "jazz" "documentation" "Documentation-Text-View.jazz"}))
            (<Jazz-File>         name: forms                   source: {File :context "src" "jazz" "editor" "jazz" "forms.jazz"}))))))))
