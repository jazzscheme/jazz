  (method (find-named-walker name)
    (case name
      ((:proclaim)        walk-proclaim)
      ((:properties)      walk-properties)
      ((:attributes)      walk-attributes)
      ((:native)          walk-native)
      ((:definition)      walk-definition)
      ((:specialize)      walk-specialize)
      ((:method)          walk-method)
      ((:slot)            walk-slot)
      ((:property)        walk-property)
      ((:form)            walk-form)
      ((:global)          walk-global)
      ((:constant)        walk-constant)
      ((:enumeration)     walk-enumeration)
      ((:c-constant)      walk-c-constant)
      ((:c-enumeration)   walk-c-enumeration)
      ((:c-initialize)    walk-c-initialize)
      ((:c-declare)       walk-c-declare)
      ((:c-named-declare) walk-c-named-declare)
      ((:c-include)       walk-c-include)
      ((:c-type)          walk-c-type)
      ((:c-structure)     walk-c-structure)
      ((:c-definition)    walk-c-definition)
      ((:c-external)      walk-external)
      ((:com-external)    walk-com-external)
      ((:generic)         walk-generic)
      ((:specific)        walk-specific)
      ((:datacontainer)   walk-datacontainer)
      (else               (nextmethod name))))

  @w
  (method (walk-named-declaration context symbol form declaration keyword expression doc walk-expressions?)
    (case symbol
      ((:proclaim)        (walk-proclaim        context symbol form declaration keyword expression doc walk-expressions?))
      ((:properties)      (walk-properties      context symbol form declaration keyword expression doc walk-expressions?))
      ((:attributes)      (walk-attributes      context symbol form declaration keyword expression doc walk-expressions?))
      ((:native)          (walk-native          context symbol form declaration keyword expression doc walk-expressions?))
      ((:definition)      (walk-definition      context symbol form declaration keyword expression doc walk-expressions?))
      ((:specialize)      (walk-specialize      context symbol form declaration keyword expression doc walk-expressions?))
      ((:method)          (walk-method          context symbol form declaration keyword expression doc walk-expressions?))
      ((:slot)            (walk-slot            context symbol form declaration keyword expression doc walk-expressions?))
      ((:property)        (walk-property        context symbol form declaration keyword expression doc walk-expressions?))
      ((:form)            (walk-form            context symbol form declaration keyword expression doc walk-expressions?))
      ((:global)          (walk-global          context symbol form declaration keyword expression doc walk-expressions?))
      ((:constant)        (walk-constant        context symbol form declaration keyword expression doc walk-expressions?))
      ((:enumeration)     (walk-enumeration     context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-constant)      (walk-c-constant      context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-enumeration)   (walk-c-enumeration   context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-initialize)    (walk-c-initialize    context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-declare)       (walk-c-declare       context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-named-declare) (walk-c-named-declare context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-include)       (walk-c-include       context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-type)          (walk-c-type          context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-structure)     (walk-c-structure     context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-definition)    (walk-c-definition    context symbol form declaration keyword expression doc walk-expressions?))
      ((:c-external)      (walk-external        context symbol form declaration keyword expression doc walk-expressions?))
      ((:com-external)    (walk-com-external    context symbol form declaration keyword expression doc walk-expressions?))
      ((:generic)         (walk-generic         context symbol form declaration keyword expression doc walk-expressions?))
      ((:specific)        (walk-specific        context symbol form declaration keyword expression doc walk-expressions?))
      ((:datacontainer)   (walk-datacontainer   context symbol form declaration keyword expression doc walk-expressions?))
      (else               (nextmethod           context symbol form declaration keyword expression doc walk-expressions?))))
  
  
  (method (walk-proclaim context symbol form declaration keyword expression doc walk-expressions?)
    (keyword context start end))

  
  (method (walk-properties context symbol form declaration keyword expression doc walk-expressions?)
    (keyword context start end)
    (when walk-expressions?
      (walk-expressions context expression)))
  
  
  (method (walk-attributes context symbol form declaration keyword expression doc walk-expressions?)
    (keyword context start end)
    (when walk-expressions?
      (walk-expressions context expression)))

  
  (method (walk-definition context symbol form declaration keyword expression doc walk-expressions?)
    (let ((entry context))
      (keyword context start end)
      (walk-modifiers context declaration keyword (get-modifiers~ form))
      (forward-whites)
      (let ((list? (eqv? (char-forward 0) #\open-parenthesis)))
        (cond ((not list?)
               (when (and (forward-expr) (eq? kind 'symbol))
                 (set! entry (declaration context symbol start end (get-marked) (symbol-documented? (find-marked-symbol) doc)))))
              (else
               (enter-list)
               (when (and (forward-expr) (eq? kind 'symbol))
                 (set! entry (declaration context symbol start end (get-marked) (symbol-documented? (find-marked-symbol) doc))))
               (walk-parameters entry declaration keyword expression doc)
               (exit-list))))
      (when walk-expressions?
        (walk-expressions entry expression 'code #f))
      entry))
  
  
  (method (walk-specialize context symbol form declaration keyword expression doc walk-expressions?)
    (let ((entry context))
      (keyword context start end)
      (walk-modifiers context declaration keyword (get-modifiers~ form))
      (forward-whites)
      (when (not (eqv? (char-forward 0) #\open-parenthesis))
        (when (and (forward-expr) (eq? kind 'symbol) (eq? (find-marked-symbol) 'as))
          (keyword context start end)
          (forward-expr)
          (declaration context symbol start end (get-marked) (symbol-documented? (find-marked-symbol) doc))
          (forward-whites)))
      (let ((list? (eqv? (char-forward 0) #\open-parenthesis)))
        (cond ((not list?)
               (when (and (forward-expr) (eq? kind 'symbol))
                 (set! entry (declaration context symbol start end (get-marked) (symbol-documented? (find-marked-symbol) doc)))))
              (else
               (enter-list)
               (when (and (forward-expr) (eq? kind 'symbol))
                 (set! entry (declaration context symbol start end (get-marked) (symbol-documented? (find-marked-symbol) doc))))
               (walk-parameters entry declaration keyword expression doc)
               (exit-list))))
      (when walk-expressions?
        (walk-expressions entry expression 'code #f))
      entry))

  
  (method public (walk-method context symbol form declaration keyword expression doc walk-expressions?)
    (let ((entry context))
      (keyword context start end)
      (walk-modifiers context declaration keyword (get-modifiers~ form))
      (enter-list)
      (when (and (forward-expr) (eq? kind 'symbol))
        (set! entry (declaration context symbol start end (get-marked) (symbol-documented? (find-marked-symbol) doc)))
        (forward-whites)
        (walk-parameters entry declaration keyword expression doc)
        (exit-list)
        (when walk-expressions?
          (walk-expressions entry expression 'code #f)))
      entry))
  
  
  (method (walk-slot/property-accessors context slot-name declaration keyword expression doc walk-expressions?)
    (define (declare-accessor accessor)
      (let ((name (string-append (case accessor ((getter) "get-") ((setter) "set-")) slot-name)))
        (declaration context accessor start end name (symbol-documented? (find-marked-symbol) doc))
        #t))
    
    (while (forward-expr)
      (unless (and (eq? kind 'symbol)
                   (let ((marked-symbol (find-marked-symbol)))
                     (case marked-symbol
                       ((initialize)
                        (keyword context start end)
                        #f)
                       ((getter setter accessors)
                        (keyword context start end)
                        (when (and (forward-expr) (eq? kind 'symbol) (string=? (get-marked) "generate"))
                          (case marked-symbol
                            ((getter)
                             (declare-accessor 'getter))
                            ((setter)
                             (declare-accessor 'setter))
                            ((accessors)
                             (declare-accessor 'getter)
                             (declare-accessor 'setter)))))
                       (else
                        #f))))
        (walk-expression context expression 'code #f))))
  
  
  (method (walk-slot context symbol form declaration keyword expression doc walk-expressions?)
    (let ((entry context))
      (keyword context start end)
      (walk-modifiers context declaration keyword (get-modifiers~ form))
      (forward-whites)
      (when (and (forward-expr) (eq? kind 'symbol))
        (let ((marked (get-marked)))
          (set! entry (declaration context symbol start end marked (symbol-documented? (find-marked-symbol) doc)))
          (walk-slot/property-accessors context marked declaration keyword expression doc walk-expressions?)))
      (when walk-expressions?
        (walk-expressions entry expression))
      entry))
  
  
