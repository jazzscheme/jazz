;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.lisp
    (<Project-Directory>                                     directory: {Directory :context "src"}
      (<Project-Directory>                                   directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "lisp"}
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "lisp" "catalog"}
              (<Jazz-File>       name: Lisp-Entry            source: {File :context "src" "jazz" "editor" "lisp" "catalog" "Lisp-Entry.jazz"})
              (<Jazz-File>       name: Lisp-File-Entry       source: {File :context "src" "jazz" "editor" "lisp" "catalog" "Lisp-File-Entry.jazz"}))
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "lisp" "parser"}
              (<Jazz-File>       name: Lisp-Parser           source: {File :context "src" "jazz" "editor" "lisp" "parser" "Lisp-Parser.jazz"}))
            (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor" "lisp" "text"}
              (<Jazz-File>       name: Lisp-Explorer         source: {File :context "src" "jazz" "editor" "lisp" "text" "Lisp-Explorer.jazz"})
              (<Jazz-File>       name: Lisp-Syntax           source: {File :context "src" "jazz" "editor" "lisp" "text" "Lisp-Syntax.jazz"})
              (<Jazz-File>       name: Lisp-Text-Actions     source: {File :context "src" "jazz" "editor" "lisp" "text" "Lisp-Text-Actions.jazz"})
              (<Jazz-File>       name: Lisp-Text-Preferences source: {File :context "src" "jazz" "editor" "lisp" "text" "Lisp-Text-Preferences.jazz"})
              (<Jazz-File>       name: Lisp-Text-View        source: {File :context "src" "jazz" "editor" "lisp" "text" "Lisp-Text-View.jazz"})
              (<Jazz-File>       name: Lisp-Console-Manager  source: {File :context "src" "jazz" "editor" "lisp" "text" "Lisp-Console-Manager.jazz"}))
            (<Jazz-File>         name: _lisp                 source: {File :context "src" "jazz" "editor" "lisp" "_lisp.jazz"})
            (<Jazz-File>         name: autoload              source: {File :context "src" "jazz" "editor" "lisp" "autoload.jazz"})
            (<Jazz-File>         name: syntax                source: {File :context "src" "jazz" "editor" "lisp" "syntax.jazz"})
            (<Project-File>                                  source: {File :context "src" "jazz" "editor" "lisp" "install.scm"})
            (<Jazz-File>         name: literals              source: {File :context "src" "jazz" "editor" "lisp" "literals.jazz"}))))))))
