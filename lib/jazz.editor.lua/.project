;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.lua        description-file: {File :context ".package"}
    (<Project-Directory>                                      directory: {Directory :context "src"}
      (<Project-Directory>                                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                  directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                directory: {Directory :context "src" "jazz" "editor" "lua"}
            (<Project-Directory>                              directory: {Directory :context "src" "jazz" "editor" "lua" "parser"}
              (<Jazz-File>       name: Lua-Parser             source: {File :context "src" "jazz" "editor" "lua" "parser" "Lua-Parser.jazz"}))
            (<Project-Directory>                              directory: {Directory :context "src" "jazz" "editor" "lua" "syntax"}
              (<Jazz-File>       name: Lua-Syntax             source: {File :context "src" "jazz" "editor" "lua" "syntax" "Lua-Syntax.jazz"}))
            (<Project-Directory>                              directory: {Directory :context "src" "jazz" "editor" "lua" "text"}
              (<Jazz-File>       name: Lua-Explorer           source: {File :context "src" "jazz" "editor" "lua" "text" "Lua-Explorer.jazz"})
              (<Jazz-File>       name: Lua-Text-Preferences   source: {File :context "src" "jazz" "editor" "lua" "text" "Lua-Text-Preferences.jazz"})
              (<Jazz-File>       name: Lua-Text-View          source: {File :context "src" "jazz" "editor" "lua" "text" "Lua-Text-View.jazz"}))
            (<Jazz-File>         name: _lua                   source: {File :context "src" "jazz" "editor" "lua" "_lua.jazz"})
            (<Jazz-File>         name: autoload               source: {File :context "src" "jazz" "editor" "lua" "autoload.jazz"})
            (<Project-File>                                   source: {File :context "src" "jazz" "editor" "lua" "install.scm"})
            (<Project-Directory>                              directory: {Directory :context "src" "jazz" "editor" "lua" "couple"}
              (<Jazz-File>       name: catalog                source: {File :context "src" "jazz" "editor" "lua" "couple" "catalog.jazz"}))
            (<Jazz-File>         name: descriptors            source: {File :context "src" "jazz" "editor" "lua" "descriptors.jazz"}))))))))
