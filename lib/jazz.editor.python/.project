;;;==============
;;;  JazzScheme
;;;==============
;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.python      description-file: {File :context ".package"}
    (<Project-Directory>                                       directory: {Directory :context "src"}
      (<Project-Directory>                                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "editor" "python"}
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "python" "parser"}
              (<Jazz-File>       name: Python-Parser           source: {File :context "src" "jazz" "editor" "python" "parser" "Python-Parser.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "python" "syntax"}
              (<Jazz-File>       name: Python-Syntax           source: {File :context "src" "jazz" "editor" "python" "syntax" "Python-Syntax.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "python" "text"}
              (<Jazz-File>       name: Python-Explorer         source: {File :context "src" "jazz" "editor" "python" "text" "Python-Explorer.jazz"})
              (<Jazz-File>       name: Python-Text-Preferences source: {File :context "src" "jazz" "editor" "python" "text" "Python-Text-Preferences.jazz"})
              (<Jazz-File>       name: Python-Text-View        source: {File :context "src" "jazz" "editor" "python" "text" "Python-Text-View.jazz"}))
            (<Jazz-File>         name: _python                 source: {File :context "src" "jazz" "editor" "python" "_python.jazz"})
            (<Jazz-File>         name: autoload                source: {File :context "src" "jazz" "editor" "python" "autoload.jazz"})
            (<Project-File>                                    source: {File :context "src" "jazz" "editor" "python" "install.scm"})
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "python" "couple"}
              (<Jazz-File>       name: catalog                 source: {File :context "src" "jazz" "editor" "python" "couple" "catalog.jazz"}))
            (<Jazz-File>         name: descriptors             source: {File :context "src" "jazz" "editor" "python" "descriptors.jazz"}))))))))
