;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.scheme      description-file: {File :context ".package"}
    (<Project-Directory>                                       directory: {Directory :context "src"}
      (<Project-Directory>                                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "editor" "scheme"}
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "scheme" "parser"}
              (<Jazz-File>       name: Scheme-Parser           source: {File :context "src" "jazz" "editor" "scheme" "parser" "Scheme-Parser.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "scheme" "syntax"}
              (<Jazz-File>       name: Scheme-Syntax           source: {File :context "src" "jazz" "editor" "scheme" "syntax" "Scheme-Syntax.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "scheme" "text"}
              (<Jazz-File>       name: Scheme-Explorer         source: {File :context "src" "jazz" "editor" "scheme" "text" "Scheme-Explorer.jazz"})
              (<Jazz-File>       name: Scheme-Text-Actions     source: {File :context "src" "jazz" "editor" "scheme" "text" "Scheme-Text-Actions.jazz"})
              (<Jazz-File>       name: Scheme-Text-Preferences source: {File :context "src" "jazz" "editor" "scheme" "text" "Scheme-Text-Preferences.jazz"})
              (<Jazz-File>       name: Scheme-Text-View        source: {File :context "src" "jazz" "editor" "scheme" "text" "Scheme-Text-View.jazz"}))
            (<Jazz-File>         name: _scheme                 source: {File :context "src" "jazz" "editor" "scheme" "_scheme.jazz"})
            (<Jazz-File>         name: autoload                source: {File :context "src" "jazz" "editor" "scheme" "autoload.jazz"})
            (<Jazz-File>         name: forms                   source: {File :context "src" "jazz" "editor" "scheme" "forms.jazz"})
            (<Jazz-File>         name: designer                source: {File :context "src" "jazz" "editor" "scheme" "designer.jazz"}))))))))
