;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.sql         description-file: {File :context ".package"}
    (<Project-Directory>                                       directory: {Directory :context "src"}
      (<Project-Directory>                                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "editor" "sql"}
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "sql" "history"}
              (<Jazz-File>       name: History-Request-Item    source: {File :context "src" "jazz" "editor" "sql" "history" "History-Request-Item.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "sql" "inventory"}
              (<Jazz-File>       name: Inventory-Browser       source: {File :context "src" "jazz" "editor" "sql" "inventory" "Inventory-Browser.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "sql" "preference"}
              (<Jazz-File>       name: SQL-Preferences         source: {File :context "src" "jazz" "editor" "sql" "preference" "SQL-Preferences.jazz"})
              (<Jazz-File>       name: SQL-Text-Preferences    source: {File :context "src" "jazz" "editor" "sql" "preference" "SQL-Text-Preferences.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "sql" "sql"}
              (<Jazz-File>       name: SQL-Connections-Editor  source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Connections-Editor.jazz"})
              (<Jazz-File>       name: SQL-Connections-Manager source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Connections-Manager.jazz"})
              (<Jazz-File>       name: SQL-Connections-Tree    source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Connections-Tree.jazz"})
              (<Jazz-File>       name: SQL-Console-Manager     source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Console-Manager.jazz"})
              (<Jazz-File>       name: SQL-Console-Text-View   source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Console-Text-View.jazz"})
              (<Jazz-File>       name: SQL-Explorer            source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Explorer.jazz"})
              (<Jazz-File>       name: SQL-Output-Text-View    source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Output-Text-View.jazz"})
              (<Jazz-File>       name: SQL-Output-Tree         source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Output-Tree.jazz"})
              (<Jazz-File>       name: SQL-Syntax              source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Syntax.jazz"})
              (<Jazz-File>       name: SQL-Text-Output         source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Text-Output.jazz"})
              (<Jazz-File>       name: SQL-Text-View           source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Text-View.jazz"})
              (<Jazz-File>       name: SQL-Tree-Output         source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Tree-Output.jazz"})
              (<Jazz-File>       name: SQL-Console-View        source: {File :context "src" "jazz" "editor" "sql" "sql" "SQL-Console-View.jazz"}))
            (<Project-Directory>                               directory: {Directory :context "src" "jazz" "editor" "sql" "view"}
              (<Jazz-File>       name: _view                   source: {File :context "src" "jazz" "editor" "sql" "view" "_view.jazz"}))
            (<Jazz-File>         name: _sql                    source: {File :context "src" "jazz" "editor" "sql" "_sql.jazz"})
            (<Jazz-File>         name: autoload                source: {File :context "src" "jazz" "editor" "sql" "autoload.jazz"})
            (<Jazz-File>         name: JML-SQL                 source: {File :context "src" "jazz" "editor" "sql" "JML-SQL.jazz"})
            (<Jazz-File>         name: descriptors             source: {File :context "src" "jazz" "editor" "sql" "descriptors.jazz"}))))))))
