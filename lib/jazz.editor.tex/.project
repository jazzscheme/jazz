;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.editor.tex      catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "editor"}
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "editor" "tex"}
            (<Project-Directory>                            directory: {Directory :context "src" "jazz" "editor" "tex" "text"}
              (<Jazz-File>       name: Tex-Explorer         source: {File :context "src" "jazz" "editor" "tex" "text" "Tex-Explorer.jazz"})
              (<Jazz-File>       name: Tex-Syntax           source: {File :context "src" "jazz" "editor" "tex" "text" "Tex-Syntax.jazz"})
              (<Jazz-File>       name: Tex-Text-Preferences source: {File :context "src" "jazz" "editor" "tex" "text" "Tex-Text-Preferences.jazz"})
              (<Jazz-File>       name: Tex-Text-View        source: {File :context "src" "jazz" "editor" "tex" "text" "Tex-Text-View.jazz"}))
            (<Jazz-File>         name: _tex                 source: {File :context "src" "jazz" "editor" "tex" "_tex.jazz"})
            (<Jazz-File>         name: autoload             source: {File :context "src" "jazz" "editor" "tex" "autoload.jazz"}))))))))
