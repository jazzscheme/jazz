;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.editor           description-file: {File :context ".package"}
    (<Project-Directory>                                 directory: {Directory :context "src"}
      (<Project-Directory>                               directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                             directory: {Directory :context "src" "jazz" "editor"}
          (<Jazz-File>       name: Code-Explorer         source: {File :context "src" "jazz" "editor" "Code-Explorer.jazz"})
          (<Jazz-File>       name: Code-Text-Actions     source: {File :context "src" "jazz" "editor" "Code-Text-Actions.jazz"})
          (<Jazz-File>       name: Code-Text-Preferences source: {File :context "src" "jazz" "editor" "Code-Text-Preferences.jazz"})
          (<Jazz-File>       name: Code-Text-View        source: {File :context "src" "jazz" "editor" "Code-Text-View.jazz"})
          (<Jazz-File>       name: _editor               source: {File :context "src" "jazz" "editor" "_editor.jazz"})
          (<Jazz-File>       name: Code-Syntax           source: {File :context "src" "jazz" "editor" "Code-Syntax.jazz"})))))))
