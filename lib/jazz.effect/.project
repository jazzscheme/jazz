(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.effect description-file: {File :context ".package"}
    (<Project-Directory>                       directory: {Directory :context "src"}
      (<Project-Directory>                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                   directory: {Directory :context "src" "jazz" "effect"}
          (<Jazz-File>       name: Effect      source: {File :context "src" "jazz" "effect" "Effect.jazz"})
          (<Jazz-File>       name: Wipe-Effect source: {File :context "src" "jazz" "effect" "Wipe-Effect.jazz"})
          (<Jazz-File>       name: _effect     source: {File :context "src" "jazz" "effect" "_effect.jazz"})))))))
