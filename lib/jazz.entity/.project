;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.entity           description-file: {File :context ".package"}
    (<Project-Directory>                                   directory: {Directory :context "src"}
      (<Project-Directory>                                 directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                               directory: {Directory :context "src" "jazz" "entity"}
          (<Jazz-File>         name: _entity               source: {File :context "src" "jazz" "entity" "_entity.jazz"})
          (<Jazz-File>         name: Entity-Factory        source: {File :context "src" "jazz" "entity" "Entity-Factory.jazz"})
          (<Jazz-File>         name: Entity-Interface      source: {File :context "src" "jazz" "entity" "Entity-Interface.jazz"})
          (<Project-Directory>                             directory: {Directory :context "src" "jazz" "entity" "ui"}
            (<Jazz-File>       name: Entities-Controller   source: {File :context "src" "jazz" "entity" "ui" "Entities-Controller.jazz"})
            (<Jazz-File>       name: Entities-Tree-Actions source: {File :context "src" "jazz" "entity" "ui" "Entities-Tree-Actions.jazz"})
            (<Jazz-File>       name: Entities-Tree         source: {File :context "src" "jazz" "entity" "ui" "Entities-Tree.jazz"})
            (<Jazz-File>       name: Entity-Menu           source: {File :context "src" "jazz" "entity" "ui" "Entity-Menu.jazz"}))))))))
