(module protected jazz.entity.Entity-Interface jazz


(interface undocumented Entity-Interface
  
  
  (method public virtual abstract (get-factory self))
  (method public virtual abstract (get-factories self))
  (method public virtual abstract (get-presentation self))
  (method public virtual abstract (set-presentation self controller presentation))
  (method public virtual abstract (can-cut-entity? self))
  (method public virtual abstract (can-copy-entity? self))
  (method public virtual abstract (can-paste-entity? self))
  (method public virtual abstract (determine-paster self . rest))
  (method public virtual abstract (can-rename-entity? self))
  (method public virtual abstract (can-delete-entity? self))
  (method public virtual abstract (can-relocate-entity? self))
  (method public virtual abstract (can-view-properties? self))
  (method public virtual abstract (get-standard-properties self))))
