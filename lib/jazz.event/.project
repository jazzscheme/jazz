;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.event description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                  directory: {Directory :context "src" "jazz" "event"}
          (<Jazz-File>       name: _event     source: {File :context "src" "jazz" "event" "_event.jazz"})
          (<Project-File>                     source: {File :context "src" "jazz" "event" "install.scm"})
          (<Jazz-File>       name: Pulse      source: {File :context "src" "jazz" "event" "Pulse.jazz"})
          (<Jazz-File>       name: Event      source: {File :context "src" "jazz" "event" "Event.jazz"})
          (<Jazz-File>       name: autoload   source: {File :context "src" "jazz" "event" "autoload.jazz"})))))))
