(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.event description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                  directory: {Directory :context "src" "jazz" "event"}
          (<Jazz-File>       name: _event     source: {File :context "src" "jazz" "event" "_event.jazz"})
          (<Project-File>                     source: {File :context "src" "jazz" "event" "install.scm"})))))))
