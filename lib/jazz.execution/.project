(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.execution description-file: {File :context ".package"}
    (<Project-Directory>                          directory: {Directory :context "src"}
      (<Project-Directory>                        directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                      directory: {Directory :context "src" "jazz" "execution"}
          (<Jazz-File>       name: _execution     source: {File :context "src" "jazz" "execution" "_execution.jazz"})))))))
