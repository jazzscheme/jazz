(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.favorite       description-file: {File :context ".package"}
    (<Project-Directory>                               directory: {Directory :context "src"}
      (<Project-Directory>                             directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                           directory: {Directory :context "src" "jazz" "favorite"}
          (<Jazz-File>       name: _favorite           source: {File :context "src" "jazz" "favorite" "_favorite.jazz"})
          (<Jazz-File>       name: autoload            source: {File :context "src" "jazz" "favorite" "autoload.jazz"})
          (<Jazz-File>       name: Favorite-Clipboard  source: {File :context "src" "jazz" "favorite" "Favorite-Clipboard.jazz"})
          (<Jazz-File>       name: Favorite-Definition source: {File :context "src" "jazz" "favorite" "Favorite-Definition.jazz"})
          (<Jazz-File>       name: Favorite-Directory  source: {File :context "src" "jazz" "favorite" "Favorite-Directory.jazz"})
          (<Jazz-File>       name: Favorite-File       source: {File :context "src" "jazz" "favorite" "Favorite-File.jazz"})
          (<Jazz-File>       name: Favorite-Handler    source: {File :context "src" "jazz" "favorite" "Favorite-Handler.jazz"})
          (<Jazz-File>       name: Favorite-Launch     source: {File :context "src" "jazz" "favorite" "Favorite-Launch.jazz"})
          (<Jazz-File>       name: Favorite-Search     source: {File :context "src" "jazz" "favorite" "Favorite-Search.jazz"})
          (<Jazz-File>       name: Favorite            source: {File :context "src" "jazz" "favorite" "Favorite.jazz"})
          (<Jazz-File>       name: Favorites-Adder     source: {File :context "src" "jazz" "favorite" "Favorites-Adder.jazz"})
          (<Jazz-File>       name: Favorites-Editor    source: {File :context "src" "jazz" "favorite" "Favorites-Editor.jazz"})
          (<Jazz-File>       name: Favorites-Manager   source: {File :context "src" "jazz" "favorite" "Favorites-Manager.jazz"})
          (<Jazz-File>       name: Favorites-Tree      source: {File :context "src" "jazz" "favorite" "Favorites-Tree.jazz"})
          (<Jazz-File>       name: Favorites           source: {File :context "src" "jazz" "favorite" "Favorites.jazz"})))))))
