(package jazz.figure
  
  (title "Figure")
  (description "Figures")
  (authors "Guillaume Cartier")
  (stage release)
  (root "src")
  (products (jazz.figure (alias figure)
                         (update jazz.figure
                                 jazz.figure.preferences)
                         (dependencies jazz))
            (jazz.figure.desc (alias figure.desc)
