(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.fontconfig description-file: {File :context ".package"}
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                       directory: {Directory :context "src" "jazz" "fontconfig"}
          (<Jazz-File>       name: _fontconfig     source: {File :context "src" "jazz" "fontconfig" "_fontconfig.jazz"})
          (<Project-File>                          source: {File :context "src" "jazz" "fontconfig" "product.scm"})))))))
