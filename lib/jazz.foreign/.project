;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.foreign description-file: {File :context ".package"}
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "foreign"}
          (<Jazz-File>       name: _foreign     source: {File :context "src" "jazz" "foreign" "_foreign.jazz"})
          (<Jazz-File>       name: syntax       source: {File :context "src" "jazz" "foreign" "syntax.jazz"})
          (<Jazz-File>       name: features     source: {File :context "src" "jazz" "foreign" "features.jazz"})))))))
