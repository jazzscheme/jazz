(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.freetype description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "freetype"}
          (<Jazz-File>       name: _freetype     source: {File :context "src" "jazz" "freetype" "_freetype.jazz"})
          (<Project-File>                        source: {File :context "src" "jazz" "freetype" "product.scm"})
          (<Jazz-File>       name: test          source: {File :context "src" "jazz" "freetype" "test.jazz"})))))))
