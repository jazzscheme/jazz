(package jazz.git
  
  (title "Git")
  (description "Interface to the libgit2 library")
  (authors "Guillaume Cartier")
  (stage beta)
  (root "src")
  (install jazz.git.install)
  (products (jazz.git (unit jazz.git.product)
                      (update jazz.git
                              jazz.git.foreign
                              jazz.git.install
                              jazz.git.interface
                              jazz.git.product
                              jazz.git.ui)
                      (dependencies jazz)
                      (test jazz.git.validate.run))
            (git (alias jazz.git)))
  (project ".project"))
