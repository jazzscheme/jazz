;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
(jazz.editor.jazz))


(form
  (<Project>                   name: jazz.git
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                  directory: {Directory :context "src" "jazz" "git"}
          (<Jazz-File>         name: _git     source: {File :context "src" "jazz" "git" "_git.jazz"})
          (<Jazz-File>         name: ui       source: {File :context "src" "jazz" "git" "ui.jazz"})
          (<Project-Directory>                directory: {Directory Jazz/jazz.git "src" "jazz" "git" "foreign"}
            (<Jazz-File>       name: git      source: {File :context "src" "jazz" "git" "foreign" "git.jazz"}))
          (<Project-File>                     source: {File :context "src" "jazz" "git" "product.scm"})))))))
