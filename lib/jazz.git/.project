;;;==============
;;;  JazzScheme
;;;==============
;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.git   description-file: {File :context ".package"}
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "git"}
          (<Jazz-File>         name: _git       source: {File :context "src" "jazz" "git" "_git.jazz"})
          (<Jazz-File>         name: ui         source: {File :context "src" "jazz" "git" "ui.jazz"})
          (<Jazz-File>         name: foreign    source: {File :context "src" "jazz" "git" "foreign.jazz"})
          (<Project-File>                       source: {File :context "src" "jazz" "git" "product.scm"})
          (<Jazz-File>         name: interface  source: {File :context "src" "jazz" "git" "interface.jazz"})
          (<Project-Directory>                  directory: {Directory :context "src" "jazz" "git" "validate"}
            (<Jazz-File>       name: _validate  source: {File :context "src" "jazz" "git" "validate" "_validate.jazz"})
            (<Jazz-File>       name: Git-Tester source: {File :context "src" "jazz" "git" "validate" "Git-Tester.jazz"})
            (<Jazz-File>       name: run        source: {File :context "src" "jazz" "git" "validate" "run.jazz"}))
          (<Jazz-File>         name: syntax     source: {File :context "src" "jazz" "git" "syntax.jazz"})
          (<Project-File>                       source: {File :context "src" "jazz" "git" "install.scm"})))))))
