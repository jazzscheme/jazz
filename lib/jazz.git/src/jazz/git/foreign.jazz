(c-definition (credentials-acquire-procedure callbacks) (scheme-object) git_cred* "credentials_acquire_procedure" ""
  (and callbacks
       (let ((credentials-callback (get-credentials-callback callbacks)))
         (and credentials-callback
              (credentials-callback)))))

(c-declare git.foreign #/C/
    int credentials_cb(git_cred **out, const char * url, const char * username_from_url, unsigned int allowed_types, void * payload)
    {
        int id = *((int*)(&payload));
        git_cred* cred;
        gambit_enable_interrupts();
        cred = credentials_acquire_procedure(___EXT(___data_rc)(downloadInfos[id].data_rc));
        gambit_disable_interrupts();
        if (cred)
        {
            *out = cred;
            return 0;
        }
        else
            return GIT_PASSTHROUGH;
    }
//#)


(c-declare jiri.git #/C/
    int remote_download_callback(const git_transfer_progress *stats, void *payload)
    {
        int id = *((int*)(&payload));
        bool abort;
      
        lock_mutex();
        abort = abortRequested;
        downloadInfos[id].total_objects = stats->total_objects;
        downloadInfos[id].received_objects = stats->received_objects;
        downloadInfos[id].received_bytes = stats->received_bytes;
        unlock_mutex();
        
        if (! abort)
        {
            void* fd = downloadInfos[id].fd;
            post_notification(GIT_PROGRESS, fd);
            return 0;
        }
        else
            return -1;
    }

    THREADPROC(remote_download_proc, payload)
    {
        int id = *((int*)(&payload));
        git_remote* remote = downloadInfos[id].remote;
        
        git_remote_callbacks callbacks = GIT_REMOTE_CALLBACKS_INIT;
        callbacks.transfer_progress = remote_download_callback;
        callbacks.payload = payload;
        
        git_fetch_options options = GIT_FETCH_OPTIONS_INIT;
        options.callbacks = callbacks;
        int result = git_remote_download(remote, NULL, &options);
        
        void* fd = downloadInfos[id].fd;
        if (result == 0)
            post_notification(GIT_DONE, fd);
        else
            post_notification(GIT_ERROR, fd);
        
        return 0;
    }
//#)


(git-external (git-remote-connect git_remote* int int void* scheme-object) void
  #/C/
    git_remote* remote = ___arg1;
    git_direction direction = (git_direction) ___arg2;
    int id = ___arg3;
    void* fd = ___arg4;
    void* p = ___EXT(___alloc_rc)(0);
    ___EXT(___set_data_rc)(p, ___arg5);
    void* payload = (void*) id;

    downloadInfos[id].fd = fd;
    downloadInfos[id].data_rc = p;
    downloadInfos[id].remote = remote;
    downloadInfos[id].total_objects = 0;
    downloadInfos[id].received_objects = 0;
    downloadInfos[id].received_bytes = 0;
    
    git_remote_callbacks callbacks = GIT_REMOTE_CALLBACKS_INIT;
    callbacks.credentials = credentials_cb;
    callbacks.transfer_progress = remote_download_callback;
    callbacks.payload = payload;

    gambit_disable_interrupts();
    int result = git_remote_connect(remote, direction, &callbacks, NULL, NULL);
    gambit_enable_interrupts();
    
    if (result != 0)
        git_call_error_handler(result);
//#)


