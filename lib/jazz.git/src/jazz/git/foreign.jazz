  (= 1 (git-repository-is-bare repo)))


(definition public (git-repository-empty? repo)
  (= 1 (git-repository-is-empty repo)))


(definition public (git-repository-fetch repo name)
  (let ((remote (git-remote-load repo name)))
    (git-remote-connect remote 0)
    (git-remote-download remote)
    (git-remote-disconnect remote)
    (git-remote-update-tips remote)
    (git-remote-free remote)))


(definition public (with-repository path proc)
  (let ((repo #f))
    (unwind-protect
        (begin
          (set! repo (git-repository-open path))
          (proc repo))
      (when repo
        (git-repository-free repo)))))


