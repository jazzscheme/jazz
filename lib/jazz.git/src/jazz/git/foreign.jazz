;;;
;;;; Error
;;;


;; hack around a bug where throwing an exception from
;; inside a c-definition will crash Gambit miserably
(definition git-current-error
  #f)


(definition protected (set-git-current-error error)
  (set! git-current-error error))


(definition protected (clear-git-current-error)
  (set! git-current-error #f))


(definition protected (check-git-current-error)
  (when git-current-error
    (let ((error git-current-error))
      (set! git-current-error #f)
      (throw-git-error error))))


(definition protected (throw-git-error error)
  (define (error-message)
    (let ((err (giterr-last)))
      (if err
          (git-error-message err)
        #f)))
  
  (throw (new Git-Exception error (error-message))))


(definition protected (git-error)
  (throw-git-error #f))


(class Git-Exception extends Exception
  
  
  (slot code)
  (slot message)
  
  
  (method override (initialize self code message)
    (set! self.code code)
    (set! self.message message))
  
  
  (method override (print self output readably)
    (print-unreadable self output
      (lambda (output)
        (format output "{a}{? {a}~}" code message))))
  
  
  (method (get-code self)
    code)
  
  
  (method override (get-message self)
    (or message (format "Git exception (#x{x})" code))))


(c-definition (git-call-error-handler error) (int) void "git_call_error_handler" ""
  (handle-git-error error))


(definition protected (handle-git-error error)
  (set-git-current-error error))


@wait-hack
(definition protected (handle-git-error error)
  (throw (let ((err (giterr-last)))
           (if err
               (new Git-Exception error (git-error-message err))
             (new Git-Exception error #f)))))


