;;;
;;;; Interrupts
;;;

;___mask_heartbeat_interrupts_begin/end
;  structure to capture / restore current state of interrupts
;___mask_heartbeat_interrupts
;___mask_os_int
;___mask_user_int

; unix self pipe
; windows something else
; marc used those for the parallel gc
; there is pretty much already an abstraction of those

   
(definition protected (without-gambit-interrupts thunk)
  (let ((enabled? (interrupts-enabled?)))
    (unwind-protect
        (begin
          (disable-interrupts!)
          (thunk))
      (if enabled?
          (enable-interrupts!)
        (disable-interrupts!)))))


;;;
;;;; OS Interrupts
;;;


(cond-expand
  (windows
(c-declare git.foreign #/C/
#define gambit_disable_interrupts()
#define gambit_enable_interrupts()
//#))
  (else
(c-declare git.foreign #/C/
___mask_all_interrupts_state all_interrupts;
int mask_interrupts_calls = 0;

void gambit_disable_interrupts()
{
    // failsafe to using a global variable for the state
    if (mask_interrupts_calls > 0)
        exit(-1);
    
    mask_interrupts_calls++;
    ___EXT(___mask_all_interrupts_begin)(&all_interrupts);
}

void gambit_enable_interrupts()
{
    ___EXT(___mask_all_interrupts_end)(&all_interrupts);
    mask_interrupts_calls--;
}
//#)))


