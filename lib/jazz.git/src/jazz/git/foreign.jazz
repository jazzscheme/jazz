(c-definition (branch-foreach-procedure proc branch_name branch_type) (scheme-object char-string git_branch_t) void "branch_foreach_procedure" ""
  (proc branch_name branch_type))


(c-declare #<<end-of-c-declare
static int branch_foreach_cb(const char *branch_name, git_branch_t branch_type, void *payload)
{
    branch_foreach_procedure(payload, branch_name, branch_type);
    return 0;
}
end-of-c-declare
)


(definition public git-branch-for-each
  (c-function (git_repository* uint scheme-object) void
    #<<end-of-c-code
    git_branch_foreach(___arg1, ___arg2, &branch_foreach_cb, ___arg3);
end-of-c-code
