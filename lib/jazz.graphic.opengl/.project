;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                         catalog?: #t catalog-extensions: :jazz include-extensions: :jazz catalog-name: jazz.graphic.opengl
    (<Project-Directory>                             directory: {Directory :context "src"}
      (<Project-Directory>                           directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                         directory: {Directory :context "src" "jazz" "graphic"}
          (<Project-Directory>                       directory: {Directory :context "src" "jazz" "graphic" "opengl"}
            (<Project-File>                          source: {File :context "src" "jazz" "graphic" "opengl" "product.scm"})
            (<Jazz-File>         name: _opengl       source: {File :context "src" "jazz" "graphic" "opengl" "_opengl.jazz"})
            (<Jazz-File>         name: autoload      source: {File :context "src" "jazz" "graphic" "opengl" "autoload.jazz"})
            (<Project-Directory>                     directory: {Directory :context "src" "jazz" "graphic" "opengl" "foreign"}
              (<Jazz-File>       name: gl-header     source: {File :context "src" "jazz" "graphic" "opengl" "foreign" "gl-header.jazz"})
              (<Jazz-File>       name: gl            source: {File :context "src" "jazz" "graphic" "opengl" "foreign" "gl.jazz"})
              (<Jazz-File>       name: glu-header    source: {File :context "src" "jazz" "graphic" "opengl" "foreign" "glu-header.jazz"})
              (<Jazz-File>       name: glu           source: {File :context "src" "jazz" "graphic" "opengl" "foreign" "glu.jazz"})
              (<Jazz-File>       name: glut-header   source: {File :context "src" "jazz" "graphic" "opengl" "foreign" "glut-header.jazz"})
              (<Jazz-File>       name: glut          source: {File :context "src" "jazz" "graphic" "opengl" "foreign" "glut.jazz"}))
            (<Project-Directory>                     directory: {Directory :context "src" "jazz" "graphic" "opengl" "platform"}
              (<Jazz-File>       name: _platform     source: {File :context "src" "jazz" "graphic" "opengl" "platform" "_platform.jazz"})
              (<Jazz-File>       name: OpenGL-Window source: {File :context "src" "jazz" "graphic" "opengl" "platform" "OpenGL-Window.jazz"})
              (<Jazz-File>       name: windows       source: {File :context "src" "jazz" "graphic" "opengl" "platform" "windows.jazz"})
              (<Jazz-File>       name: WinOpenGL     source: {File :context "src" "jazz" "graphic" "opengl" "platform" "WinOpenGL.jazz"})
              (<Jazz-File>       name: x11           source: {File :context "src" "jazz" "graphic" "opengl" "platform" "x11.jazz"})))))))))
