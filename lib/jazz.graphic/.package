  (title "Graphic")
  (description "Graphic drawing")
  (authors "Guillaume Cartier")
  (stage release)
  (root "src")
  (install jazz.graphic.install)
  (products (jazz.graphic (alias graphic)
                          (test jazz.graphic.test)
                          (update jazz.graphic
