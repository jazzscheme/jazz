(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.graphic  description-file: {File :context ".package"}
    (<Project-Directory>                             directory: {Directory :context "src"}
      (<Project-Directory>                           directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                         directory: {Directory :context "src" "jazz" "graphic"}
          (<Project-Directory>                       directory: {Directory :context "src" "jazz" "graphic" "font"}
            (<Project-Directory>                     directory: {Directory :context "src" "jazz" "graphic" "font" "platform"}
              (<Jazz-File>       name: freetype      source: {File :context "src" "jazz" "graphic" "font" "platform" "freetype.jazz"})
              (<Jazz-File>       name: Freetype-Font source: {File :context "src" "jazz" "graphic" "font" "platform" "Freetype-Font.jazz"}))
            (<Jazz-File>         name: _font         source: {File :context "src" "jazz" "graphic" "font" "_font.jazz"})
            (<Jazz-File>         name: Font          source: {File :context "src" "jazz" "graphic" "font" "Font.jazz"})
            (<Jazz-File>         name: Concrete-Font source: {File :context "src" "jazz" "graphic" "font" "Concrete-Font.jazz"}))
          (<Project-Directory>                       directory: {Directory :context "src" "jazz" "graphic" "literals"}
            (<Jazz-File>         name: colors        source: {File :context "src" "jazz" "graphic" "literals" "colors.jazz"})
            (<Jazz-File>         name: fonts         source: {File :context "src" "jazz" "graphic" "literals" "fonts.jazz"})
            (<Jazz-File>         name: _literals     source: {File :context "src" "jazz" "graphic" "literals" "_literals.jazz"}))
          (<Jazz-File>           name: _graphic      source: {File :context "src" "jazz" "graphic" "_graphic.jazz"})
          (<Jazz-File>           name: autoload      source: {File :context "src" "jazz" "graphic" "autoload.jazz"})
          (<Jazz-File>           name: Color         source: {File :context "src" "jazz" "graphic" "Color.jazz"})
          (<Jazz-File>           name: DC-Snapshot   source: {File :context "src" "jazz" "graphic" "DC-Snapshot.jazz"})
          (<Jazz-File>           name: Font-Metrics  source: {File :context "src" "jazz" "graphic" "Font-Metrics.jazz"})
          (<Jazz-File>           name: Surface       source: {File :context "src" "jazz" "graphic" "Surface.jazz"})
          (<Jazz-File>           name: Viewable      source: {File :context "src" "jazz" "graphic" "Viewable.jazz"})
          (<Project-File>                            source: {File :context "src" "jazz" "graphic" "install.scm"})
          (<Jazz-File>           name: syntax        source: {File :context "src" "jazz" "graphic" "syntax.jazz"})
          (<Jazz-File>           name: Gradient      source: {File :context "src" "jazz" "graphic" "Gradient.jazz"})
          (<Jazz-File>           name: Texture       source: {File :context "src" "jazz" "graphic" "Texture.jazz"})
          (<Project-Directory>                       directory: {Directory :context "src" "jazz" "graphic" "image"}
            (<Jazz-File>         name: _image        source: {File :context "src" "jazz" "graphic" "image" "_image.jazz"})
            (<Jazz-File>         name: Image         source: {File :context "src" "jazz" "graphic" "image" "Image.jazz"})
            (<Jazz-File>         name: Portfolio     source: {File :context "src" "jazz" "graphic" "image" "Portfolio.jazz"}))
          (<Jazz-File>           name: pdf           source: {File :context "src" "jazz" "graphic" "pdf.jazz"})
          (<Jazz-File>           name: Image-Surface source: {File :context "src" "jazz" "graphic" "Image-Surface.jazz"})
          (<Jazz-File>           name: test          source: {File :context "src" "jazz" "graphic" "test.jazz"})
          (<Jazz-File>           name: descriptors   source: {File :context "src" "jazz" "graphic" "descriptors.jazz"})
          (<Jazz-File>           name: Region        source: {File :context "src" "jazz" "graphic" "Region.jazz"})))))))
