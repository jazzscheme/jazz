(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.gstreamer description-file: {File :context ".package"}
    (<Project-Directory>                          directory: {Directory :context "src"}
      (<Project-Directory>                        directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                      directory: {Directory :context "src" "jazz" "gstreamer"}
          (<Jazz-File>       name: _gstreamer     source: {File :context "src" "jazz" "gstreamer" "_gstreamer.jazz"})
          (<Project-File>                         source: {File :context "src" "jazz" "gstreamer" "product.scm"})
          (<Jazz-File>       name: foreign        source: {File :context "src" "jazz" "gstreamer" "foreign.jazz"})))))))
