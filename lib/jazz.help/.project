(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.help     catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "help"}
          (<Jazz-File>       name: autoload      source: {File :context "src" "jazz" "help" "autoload.jazz"})
          (<Jazz-File>       name: Help-Contents source: {File :context "src" "jazz" "help" "Help-Contents.jazz"})
          (<Jazz-File>       name: Help-Index    source: {File :context "src" "jazz" "help" "Help-Index.jazz"})
          (<Jazz-File>       name: _help         source: {File :context "src" "jazz" "help" "_help.jazz"})))))))
