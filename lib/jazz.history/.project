;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.history          description-file: {File :context ".package"}
    (<Project-Directory>                                 directory: {Directory :context "src"}
      (<Project-Directory>                               directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                             directory: {Directory :context "src" "jazz" "history"}
          (<Jazz-File>       name: _history              source: {File :context "src" "jazz" "history" "_history.jazz"})
          (<Jazz-File>       name: History-Item          source: {File :context "src" "jazz" "history" "History-Item.jazz"})
          (<Jazz-File>       name: History-Manager       source: {File :context "src" "jazz" "history" "History-Manager.jazz"})
          (<Jazz-File>       name: History-Provider      source: {File :context "src" "jazz" "history" "History-Provider.jazz"})
          (<Jazz-File>       name: History-Document-Item source: {File :context "src" "jazz" "history" "History-Document-Item.jazz"})
          (<Jazz-File>       name: History-File-Item     source: {File :context "src" "jazz" "history" "History-File-Item.jazz"})))))))
