(module jazz.history jazz


(hub add-current)
(hub add-history)
(hub append-forward-history)
(hub current-history-item)
(hub get-backward-history)
(hub get-forward-history)
(hub go-backward-history)
(hub go-forward-history)
(hub open-item)
(hub present)
(hub update-history-actions)
(hub valid?)


(export
  (jazz.history.History-Document-Item (autoload History-Document-Item))
  (jazz.history.History-File-Item (autoload History-File-Item))
  (jazz.history.History-Item (autoload History-Item))
  (jazz.history.History-Manager (autoload History-Manager))
  (jazz.history.History-Provider (autoload History-Provider))))
