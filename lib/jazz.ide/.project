          (<Jazz-File>       name: IDE-Status                source: {File jazz.ide "src" "jazz" "ide" "application" "IDE-Status.jazz"})
          (<Jazz-File>       name: IDE-Text-View             source: {File jazz.ide "src" "jazz" "ide" "application" "IDE-Text-View.jazz"})
          (<Jazz-File>       name: IDE-Toolbar               source: {File jazz.ide "src" "jazz" "ide" "application" "IDE-Toolbar.jazz"})
          (<Jazz-File>       name: IDE-Toplevel              source: {File jazz.ide "src" "jazz" "ide" "application" "IDE-Toplevel.jazz"})
