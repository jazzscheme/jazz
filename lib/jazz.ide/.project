        (<Project-Directory>                                 directory: {Directory jazz.ide "src" "jazz" "ide" "code"}
          (<Jazz-File>       name: Chapters-Browser-Actions  source: {File jazz.ide "src" "jazz" "ide" "code" "Chapters-Browser-Actions.jazz"})
