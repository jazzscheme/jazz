          (<Jazz-File>       name: _login                    source: {File jazz.ide "src" "jazz" "ide" "login" "_login.jazz"})
          (<Jazz-File>       name: Login-Bindings            source: {File jazz.ide "src" "jazz" "ide" "login" "Login-Bindings.jazz"})
