;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.ide
    (<Project-Directory>                                       directory: {Directory :context "src"}
      (<Project-Directory>                                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "ide"}
          (<Jazz-File>         name: _ide                      source: {File :context "src" "jazz" "ide" "_ide.jazz"})
          (<Jazz-File>         name: autoload                  source: {File :context "src" "jazz" "ide" "autoload.jazz"})
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "ide" "login"}
            (<Jazz-File>       name: _login                    source: {File :context "src" "jazz" "ide" "login" "_login.jazz"})
            (<Jazz-File>       name: Login-Bindings            source: {File :context "src" "jazz" "ide" "login" "Login-Bindings.jazz"})
            (<Jazz-File>       name: Login-Dialog              source: {File :context "src" "jazz" "ide" "login" "Login-Dialog.jazz"}))
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "ide" "code"}
            (<Jazz-File>       name: Chapters-Browser-Actions  source: {File :context "src" "jazz" "ide" "code" "Chapters-Browser-Actions.jazz"})
            (<Jazz-File>       name: Chapters-Browser          source: {File :context "src" "jazz" "ide" "code" "Chapters-Browser.jazz"})
            (<Jazz-File>       name: Code-Tree                 source: {File :context "src" "jazz" "ide" "code" "Code-Tree.jazz"})
            (<Jazz-File>       name: Hierarchy-Browser-Actions source: {File :context "src" "jazz" "ide" "code" "Hierarchy-Browser-Actions.jazz"})
            (<Jazz-File>       name: Hierarchy-Browser         source: {File :context "src" "jazz" "ide" "code" "Hierarchy-Browser.jazz"})
            (<Jazz-File>       name: Refactor-Menu             source: {File :context "src" "jazz" "ide" "code" "Refactor-Menu.jazz"})
            (<Jazz-File>       name: Source-Menu               source: {File :context "src" "jazz" "ide" "code" "Source-Menu.jazz"}))
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "ide" "application"}
            (<Jazz-File>       name: IDE-Actions               source: {File :context "src" "jazz" "ide" "application" "IDE-Actions.jazz"})
            (<Jazz-File>       name: IDE-Bindings              source: {File :context "src" "jazz" "ide" "application" "IDE-Bindings.jazz"})
            (<Jazz-File>       name: IDE-File-Menu             source: {File :context "src" "jazz" "ide" "application" "IDE-File-Menu.jazz"})
            (<Jazz-File>       name: IDE-Preferences           source: {File :context "src" "jazz" "ide" "application" "IDE-Preferences.jazz"})
            (<Jazz-File>       name: IDE-Profile               source: {File :context "src" "jazz" "ide" "application" "IDE-Profile.jazz"})
            (<Jazz-File>       name: IDE-Status                source: {File :context "src" "jazz" "ide" "application" "IDE-Status.jazz"})
            (<Jazz-File>       name: IDE-Text-View             source: {File :context "src" "jazz" "ide" "application" "IDE-Text-View.jazz"})
            (<Jazz-File>       name: IDE-Toolbar               source: {File :context "src" "jazz" "ide" "application" "IDE-Toolbar.jazz"})
            (<Jazz-File>       name: IDE-Toplevel              source: {File :context "src" "jazz" "ide" "application" "IDE-Toplevel.jazz"})
            (<Jazz-File>       name: IDE                       source: {File :context "src" "jazz" "ide" "application" "IDE.jazz"}))
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "ide" "server"}
            (<Jazz-File>       name: Server-Configuration      source: {File :context "src" "jazz" "ide" "server" "Server-Configuration.jazz"})
            (<Jazz-File>       name: Server-Configurations     source: {File :context "src" "jazz" "ide" "server" "Server-Configurations.jazz"})
            (<Jazz-File>       name: Servers-Actions           source: {File :context "src" "jazz" "ide" "server" "Servers-Actions.jazz"})
            (<Jazz-File>       name: Servers-Manager           source: {File :context "src" "jazz" "ide" "server" "Servers-Manager.jazz"}))
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "ide" "tool"}
            (<Jazz-File>       name: Skins-Manager             source: {File :context "src" "jazz" "ide" "tool" "Skins-Manager.jazz"}))
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "ide" "data"}
            (<Jazz-File>       name: favorites                 source: {File :context "src" "jazz" "ide" "data" "favorites.jazz"})
            (<Jazz-File>       name: pagesetup                 source: {File :context "src" "jazz" "ide" "data" "pagesetup.jazz"})
            (<Jazz-File>       name: project                   source: {File :context "src" "jazz" "ide" "data" "project.jazz"})
            (<Jazz-File>       name: servers                   source: {File :context "src" "jazz" "ide" "data" "servers.jazz"})
            (<Jazz-File>       name: skins                     source: {File :context "src" "jazz" "ide" "data" "skins.jazz"})
            (<Jazz-File>       name: toolbox                   source: {File :context "src" "jazz" "ide" "data" "toolbox.jazz"})
            (<Jazz-File>       name: workbench                 source: {File :context "src" "jazz" "ide" "data" "workbench.jazz"})
            (<Jazz-File>       name: workspaces                source: {File :context "src" "jazz" "ide" "data" "workspaces.jazz"})
            (<Jazz-File>       name: connections               source: {File :context "src" "jazz" "ide" "data" "connections.jazz"})
            (<Jazz-File>       name: colors                    source: {File :context "src" "jazz" "ide" "data" "colors.jazz"})
            (<Jazz-File>       name: fonts                     source: {File :context "src" "jazz" "ide" "data" "fonts.jazz"})
            (<Jazz-File>       name: styles                    source: {File :context "src" "jazz" "ide" "data" "styles.jazz"})
            (<Jazz-File>       name: formats                   source: {File :context "src" "jazz" "ide" "data" "formats.jazz"})
            (<Jazz-File>       name: doc                       source: {File :context "src" "jazz" "ide" "data" "doc.jazz"}))))))))
