            (<Jazz-File>       name: Scaled-View               source: {File :context "src" "jazz" "ide" "showcase" "Scaled-View.jazz"})
            (<Jazz-File>       name: Showcase                  source: {File :context "src" "jazz" "ide" "showcase" "Showcase.jazz"})
            (<Jazz-File>       name: Text-Showcase             source: {File :context "src" "jazz" "ide" "showcase" "Text-Showcase.jazz"})
