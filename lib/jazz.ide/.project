        (<Project-Directory>                                 directory: {Directory jazz.ide "src" "jazz" "ide" "application"}
          (<Jazz-File>       name: IDE-Actions               source: {File jazz.ide "src" "jazz" "ide" "application" "IDE-Actions.jazz"})
