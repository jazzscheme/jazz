            (<Jazz-File>       name: UI-Debugger               source: {File :context "src" "jazz" "ide" "ui" "UI-Debugger.jazz"})
            (<Jazz-File>       name: UI-Debugger-View          source: {File :context "src" "jazz" "ide" "ui" "UI-Debugger-View.jazz"}))
          (<Jazz-File>         name: menu                      source: {File :context "src" "jazz" "ide" "menu.jazz"})
          (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "ide" "log"}
            (<Jazz-File>       name: Loggable                  source: {File :context "src" "jazz" "ide" "log" "Loggable.jazz"})
            (<Jazz-File>       name: Output-Log                source: {File :context "src" "jazz" "ide" "log" "Output-Log.jazz"}))))))))
