;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.jas   description-file: {File :context ".package"}
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "jas"}
          (<Jazz-File>         name: _jas       source: {File :context "src" "jazz" "jas" "_jas.jazz"})
          (<Project-Directory>                  directory: {Directory :context "src" "jazz" "jas" "validate"}
            (<Jazz-File>       name: run        source: {File :context "src" "jazz" "jas" "validate" "run.jazz"})
            (<Jazz-File>       name: _validate  source: {File :context "src" "jazz" "jas" "validate" "_validate.jazz"})
            (<Jazz-File>       name: JAS-Tester source: {File :context "src" "jazz" "jas" "validate" "JAS-Tester.jazz"}))))))))
