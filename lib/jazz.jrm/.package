(package jazz.jrm
  
  (title "JRM")
  (description "Jazz Remote Method invocation")
  (authors "Guillaume Cartier")
  (stage release)
  (root "src")
  (install jazz.jrm.install)
  (products (jazz.jrm (alias jrm)
                      (update jazz.jrm
                              jazz.jrm.install
                              jazz.jrm.literals
                              jazz.jrm.protocol
