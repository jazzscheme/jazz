;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.jrm        documentation: {File :context "doc" "jrm.jml"} description-file: {File :context ".package"}
    (<Project-Directory>                             directory: {Directory :context "src"}
      (<Project-Directory>                           directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                         directory: {Directory :context "src" "jazz" "jrm"}
          (<Project-Directory>                       directory: {Directory :context "src" "jazz" "jrm" "register"}
            (<Jazz-File>       name: Register        source: {File :context "src" "jazz" "jrm" "register" "Register.jazz"})
            (<Jazz-File>       name: stub            source: {File :context "src" "jazz" "jrm" "register" "stub.jazz"}))
          (<Project-Directory>                       directory: {Directory :context "src" "jazz" "jrm" "remote"}
            (<Jazz-File>       name: IOR             source: {File :context "src" "jazz" "jrm" "remote" "IOR.jazz"})
            (<Jazz-File>       name: Local-Proxy     source: {File :context "src" "jazz" "jrm" "remote" "Local-Proxy.jazz"})
            (<Jazz-File>       name: Proxy           source: {File :context "src" "jazz" "jrm" "remote" "Proxy.jazz"})
            (<Jazz-File>       name: Remotable-Stub  source: {File :context "src" "jazz" "jrm" "remote" "Remotable-Stub.jazz"})
            (<Jazz-File>       name: Remote-Listener source: {File :context "src" "jazz" "jrm" "remote" "Remote-Listener.jazz"})
            (<Jazz-File>       name: Remote-Proxy    source: {File :context "src" "jazz" "jrm" "remote" "Remote-Proxy.jazz"})
            (<Jazz-File>       name: Stub-Interface  source: {File :context "src" "jazz" "jrm" "remote" "Stub-Interface.jazz"}))
          (<Jazz-File>         name: _jrm            source: {File :context "src" "jazz" "jrm" "_jrm.jazz"})
          (<Jazz-File>         name: autoload        source: {File :context "src" "jazz" "jrm" "autoload.jazz"})
          (<Project-File>                            source: {File :context "src" "jazz" "jrm" "install.scm"})
          (<Jazz-File>         name: literals        source: {File :context "src" "jazz" "jrm" "literals.jazz"})
          (<Jazz-File>         name: syntax          source: {File :context "src" "jazz" "jrm" "syntax.jazz"})
          (<Jazz-File>         name: protocol        source: {File :context "src" "jazz" "jrm" "protocol.jazz"})
          (<Jazz-File>         name: transmission    source: {File :context "src" "jazz" "jrm" "transmission.jazz"}))))
    (<Project-Directory>                             directory: {Directory :context "doc"}
      (<Project-File>                                source: {File :context "doc" "jrm.jml"})))))
