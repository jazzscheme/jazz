(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.json                description-file: {File :context ".package"}
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "json"}
          (<Jazz-File>       name: _json                    source: {File :context "src" "jazz" "json" "_json.jazz"})))))))
