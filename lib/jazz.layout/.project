;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.layout   description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "layout"}
          (<Jazz-File>       name: _layout       source: {File :context "src" "jazz" "layout" "_layout.jazz"})
          (<Jazz-File>       name: Border-Layout source: {File :context "src" "jazz" "layout" "Border-Layout.jazz"})
          (<Jazz-File>       name: Fit-Layout    source: {File :context "src" "jazz" "layout" "Fit-Layout.jazz"})
          (<Jazz-File>       name: Flow-Layout   source: {File :context "src" "jazz" "layout" "Flow-Layout.jazz"})
          (<Jazz-File>       name: Layout        source: {File :context "src" "jazz" "layout" "Layout.jazz"})
          (<Jazz-File>       name: Separator     source: {File :context "src" "jazz" "layout" "Separator.jazz"})
          (<Jazz-File>       name: Snap-Layout   source: {File :context "src" "jazz" "layout" "Snap-Layout.jazz"})
          (<Jazz-File>       name: Split-Layout  source: {File :context "src" "jazz" "layout" "Split-Layout.jazz"})
          (<Jazz-File>       name: Radial-Layout source: {File :context "src" "jazz" "layout" "Radial-Layout.jazz"})))))))
