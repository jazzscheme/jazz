;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.listener description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "listener"}
          (<Jazz-File>       name: _listener     source: {File :context "src" "jazz" "listener" "_listener.jazz"})
          (<Jazz-File>       name: product       source: {File :context "src" "jazz" "listener" "product.jazz"})
          (<Jazz-File>       name: foreign       source: {File :context "src" "jazz" "listener" "foreign.jazz"})))))))
