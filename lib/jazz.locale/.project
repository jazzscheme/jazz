;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.locale description-file: {File :context ".package"}
    (<Project-Directory>                       directory: {Directory :context "src"}
      (<Project-Directory>                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                   directory: {Directory :context "src" "jazz" "locale"}
          (<Jazz-File>       name: _locale     source: {File :context "src" "jazz" "locale" "_locale.jazz"})
          (<Jazz-File>       name: components  source: {File :context "src" "jazz" "locale" "components.jazz"})
          (<Jazz-File>       name: ui          source: {File :context "src" "jazz" "locale" "ui.jazz"})
          (<Project-File>                      source: {File :context "src" "jazz" "locale" "install.scm"})
          (<Jazz-File>       name: designer    source: {File :context "src" "jazz" "locale" "designer.jazz"})))))))
