(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>               name: jazz.log description-file: {File :context ".package"}
    (<Project-Directory>                  directory: {Directory :context "src"}
      (<Project-Directory>                directory: {Directory :context "src" "jazz"}
        (<Jazz-File>       name: log      source: {File :context "src" "jazz" "log.jazz"})
        (<Jazz-File>       name: logger   source: {File :context "src" "jazz" "logger.jazz"}))))))
