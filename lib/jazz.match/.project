;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.match description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                  directory: {Directory :context "src" "jazz" "match"}
          (<Jazz-File>       name: _match     source: {File :context "src" "jazz" "match" "_match.jazz"})
          (<Jazz-File>       name: syntax     source: {File :context "src" "jazz" "match" "syntax.jazz"})))))))
