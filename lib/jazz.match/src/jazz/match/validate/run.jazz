
(library jazz.match.validate.run jazz


(import (jazz.validate)
        (jazz.match.validate.Match-Tester))


(define (feedback format-string . rest)
  (apply format :terminal format-string rest)
  (format :terminal "{%}"))


(register-product-run 'match
  (lambda (descriptor)
    (validate~ (new Validation-Suite feedback: feedback) test: Match-Tester))))
