(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>               name: jazz.math description-file: {File :context ".package"}
    (<Project-Directory>                   directory: {Directory :context "src"}
      (<Project-Directory>                 directory: {Directory :context "src" "jazz"}
        (<Jazz-File>       name: geometry  source: {File :context "src" "jazz" "geometry.jazz"})
        (<Jazz-File>       name: math      source: {File :context "src" "jazz" "math.jazz"}))))))
