          (<Project-Directory>                         directory: {Directory :context "src" "jazz" "media" "openal"}
            (<Jazz-File>       name: _openal           source: {File :context "src" "jazz" "media" "openal" "_openal.jazz"})
            (<Project-File>                            source: {File :context "src" "jazz" "media" "openal" "product.scm"}))))))))
