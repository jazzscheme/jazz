;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.media catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "media"}
          (<Project-Directory>                  directory: {Directory :context "src" "jazz" "media" "platform"}
            (<Jazz-File>       name: carbon     source: {File :context "src" "jazz" "media" "platform" "carbon.jazz"})
            (<Jazz-File>       name: windows    source: {File :context "src" "jazz" "media" "platform" "windows.jazz"})
            (<Jazz-File>       name: x11        source: {File :context "src" "jazz" "media" "platform" "x11.jazz"}))
          (<Jazz-File>         name: _media     source: {File :context "src" "jazz" "media" "_media.jazz"})
          (<Jazz-File>         name: autoload   source: {File :context "src" "jazz" "media" "autoload.jazz"})))))))
