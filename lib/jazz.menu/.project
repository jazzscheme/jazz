(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.menu       description-file: {File :context ".package"}
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                       directory: {Directory :context "src" "jazz" "menu"}
          (<Jazz-File>       name: Check-Item      source: {File :context "src" "jazz" "menu" "Check-Item.jazz"})
          (<Jazz-File>       name: Context-Menu    source: {File :context "src" "jazz" "menu" "Context-Menu.jazz"})
          (<Jazz-File>       name: Edit-Base-Menu  source: {File :context "src" "jazz" "menu" "Edit-Base-Menu.jazz"})
          (<Jazz-File>       name: Edit-Menu       source: {File :context "src" "jazz" "menu" "Edit-Menu.jazz"})
          (<Jazz-File>       name: File-Base-Menu  source: {File :context "src" "jazz" "menu" "File-Base-Menu.jazz"})
          (<Jazz-File>       name: File-Menu       source: {File :context "src" "jazz" "menu" "File-Menu.jazz"})
          (<Jazz-File>       name: Item            source: {File :context "src" "jazz" "menu" "Item.jazz"})
          (<Jazz-File>       name: Label-Item      source: {File :context "src" "jazz" "menu" "Label-Item.jazz"})
          (<Jazz-File>       name: Menu-Actions    source: {File :context "src" "jazz" "menu" "Menu-Actions.jazz"})
          (<Jazz-File>       name: Menu            source: {File :context "src" "jazz" "menu" "Menu.jazz"})
          (<Jazz-File>       name: Menubar-Actions source: {File :context "src" "jazz" "menu" "Menubar-Actions.jazz"})
          (<Jazz-File>       name: Menubar         source: {File :context "src" "jazz" "menu" "Menubar.jazz"})
          (<Jazz-File>       name: Radio-Item      source: {File :context "src" "jazz" "menu" "Radio-Item.jazz"})
          (<Jazz-File>       name: Separator-Item  source: {File :context "src" "jazz" "menu" "Separator-Item.jazz"})
          (<Jazz-File>       name: Window-Menu     source: {File :context "src" "jazz" "menu" "Window-Menu.jazz"})
          (<Jazz-File>       name: Workspace-Menu  source: {File :context "src" "jazz" "menu" "Workspace-Menu.jazz"})
          (<Jazz-File>       name: _menu           source: {File :context "src" "jazz" "menu" "_menu.jazz"})))))))
