;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.network        catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                                 directory: {Directory :context "src"}
      (<Project-Directory>                               directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                             directory: {Directory :context "src" "jazz" "network"}
          (<Project-Directory>                           directory: {Directory :context "src" "jazz" "network" "host"}
            (<Jazz-File>       name: Host                source: {File :context "src" "jazz" "network" "host" "Host.jazz"})
            (<Jazz-File>       name: TCP-Client          source: {File :context "src" "jazz" "network" "host" "TCP-Client.jazz"})
            (<Jazz-File>       name: TCP-Server          source: {File :context "src" "jazz" "network" "host" "TCP-Server.jazz"}))
          (<Project-Directory>                           directory: {Directory :context "src" "jazz" "network" "http"}
            (<Jazz-File>       name: _http               source: {File :context "src" "jazz" "network" "http" "_http.jazz"})
            (<Jazz-File>       name: HTTP-Client         source: {File :context "src" "jazz" "network" "http" "HTTP-Client.jazz"})
            (<Jazz-File>       name: HTTP-Message        source: {File :context "src" "jazz" "network" "http" "HTTP-Message.jazz"})
            (<Jazz-File>       name: HTTP-Request        source: {File :context "src" "jazz" "network" "http" "HTTP-Request.jazz"})
            (<Jazz-File>       name: HTTP-Response       source: {File :context "src" "jazz" "network" "http" "HTTP-Response.jazz"})
            (<Jazz-File>       name: Web-Server          source: {File :context "src" "jazz" "network" "http" "Web-Server.jazz"})
            (<Jazz-File>       name: Cookie              source: {File :context "src" "jazz" "network" "http" "Cookie.jazz"})
            (<Jazz-File>       name: Session             source: {File :context "src" "jazz" "network" "http" "Session.jazz"})
            (<Jazz-File>       name: Session-Manager     source: {File :context "src" "jazz" "network" "http" "Session-Manager.jazz"})
            (<Jazz-File>       name: Dispatcher          source: {File :context "src" "jazz" "network" "http" "Dispatcher.jazz"})
            (<Jazz-File>       name: Handler             source: {File :context "src" "jazz" "network" "http" "Handler.jazz"})
            (<Jazz-File>       name: Default-404-Handler source: {File :context "src" "jazz" "network" "http" "Default-404-Handler.jazz"})
            (<Jazz-File>       name: File-Handler        source: {File :context "src" "jazz" "network" "http" "File-Handler.jazz"})
            (<Jazz-File>       name: Directory-Handler   source: {File :context "src" "jazz" "network" "http" "Directory-Handler.jazz"}))
          (<Jazz-File>         name: _network            source: {File :context "src" "jazz" "network" "_network.jazz"})
          (<Jazz-File>         name: autoload            source: {File :context "src" "jazz" "network" "autoload.jazz"})
          (<Project-Directory>                           directory: {Directory :context "src" "jazz" "network" "server"}
            (<Jazz-File>       name: Server-Panel        source: {File :context "src" "jazz" "network" "server" "Server-Panel.jazz"}))
          (<Project-File>                                source: {File :context "src" "jazz" "network" "install.scm"})
          (<Jazz-File>         name: literals            source: {File :context "src" "jazz" "network" "literals.jazz"})))))))
