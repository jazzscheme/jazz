(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.neural description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "neural"}
          (<Jazz-File>         name: _neural     source: {File :context "src" "jazz" "neural" "_neural.jazz"})
          (<Jazz-File>         name: math        source: {File :context "src" "jazz" "neural" "math.jazz"})
          (<Jazz-File>         name: loss        source: {File :context "src" "jazz" "neural" "loss.jazz"})
          (<Jazz-File>         name: layer       source: {File :context "src" "jazz" "neural" "layer.jazz"})
          (<Jazz-File>         name: activation  source: {File :context "src" "jazz" "neural" "activation.jazz"})
          (<Jazz-File>         name: optimizer   source: {File :context "src" "jazz" "neural" "optimizer.jazz"})
          (<Jazz-File>         name: data        source: {File :context "src" "jazz" "neural" "data.jazz"})
          (<Jazz-File>         name: overload    source: {File :context "src" "jazz" "neural" "overload.jazz"})
          (<Jazz-File>         name: io          source: {File :context "src" "jazz" "neural" "io.jazz"})
          (<Jazz-File>         name: tensor      source: {File :context "src" "jazz" "neural" "tensor.jazz"})
          (<Jazz-File>         name: product     source: {File :context "src" "jazz" "neural" "product.jazz"})
          (<Project-Directory>                   directory: {Directory :context "src" "jazz" "neural" "blas"}
            (<Jazz-File>       name: accelerate  source: {File :context "src" "jazz" "neural" "blas" "accelerate.jazz"})
            (<Jazz-File>       name: blis        source: {File :context "src" "jazz" "neural" "blas" "blis.jazz"})
            (<Jazz-File>       name: npopenblas  source: {File :context "src" "jazz" "neural" "blas" "npopenblas.jazz"})
            (<Jazz-File>       name: openblas    source: {File :context "src" "jazz" "neural" "blas" "openblas.jazz"})
            (<Jazz-File>       name: _blas       source: {File :context "src" "jazz" "neural" "blas" "_blas.jazz"}))))))))
