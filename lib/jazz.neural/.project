(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.neural description-file: {File :context ".package"}
    (<Project-Directory>                       directory: {Directory :context "src"}
      (<Project-Directory>                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                   directory: {Directory :context "src" "jazz" "neural"}
          (<Jazz-File>       name: _neural     source: {File :context "src" "jazz" "neural" "_neural.jazz"})
          (<Jazz-File>       name: math        source: {File :context "src" "jazz" "neural" "math.jazz"})
          (<Jazz-File>       name: array       source: {File :context "src" "jazz" "neural" "array.jazz"})))))))
