;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.object description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "object"}
          (<Jazz-File>         name: _object     source: {File :context "src" "jazz" "object" "_object.jazz"})
          (<Jazz-File>         name: syntax      source: {File :context "src" "jazz" "object" "syntax.jazz"})
          (<Project-Directory>                   directory: {Directory :context "src" "jazz" "object" "test"}
            (<Jazz-File>       name: _test       source: {File :context "src" "jazz" "object" "test" "_test.jazz"})
            (<Jazz-File>       name: p           source: {File :context "src" "jazz" "object" "test" "p.jazz"})
            (<Jazz-File>       name: q           source: {File :context "src" "jazz" "object" "test" "q.jazz"}))))))))
