(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.odbc     description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "odbc"}
          (<Jazz-File>       name: _odbc         source: {File :context "src" "jazz" "odbc" "_odbc.jazz"})
          (<Jazz-File>       name: odbc-lowlevel source: {File :context "src" "jazz" "odbc" "odbc-lowlevel.jazz"})
          (<Project-File>                        source: {File :context "src" "jazz" "odbc" "product.scm"})
          (<Project-File>                        source: {File :context "src" "jazz" "odbc" "install.scm"})))))))
