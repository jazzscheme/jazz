(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.openal description-file: {File :context ".package"}
    (<Project-Directory>                       directory: {Directory :context "src"}
      (<Project-Directory>                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                   directory: {Directory :context "src" "jazz" "openal"}
          (<Jazz-File>       name: _openal     source: {File :context "src" "jazz" "openal" "_openal.jazz"})
          (<Project-File>                      source: {File :context "src" "jazz" "openal" "product.scm"})))))))
