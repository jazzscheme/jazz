(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.opengl description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "opengl"}
          (<Project-Directory>                   directory: {Directory :context "src" "jazz" "opengl" "glew"}
            (<Jazz-File>       name: _glew       source: {File :context "src" "jazz" "opengl" "glew" "_glew.jazz"})
            (<Jazz-File>       name: foreign     source: {File :context "src" "jazz" "opengl" "glew" "foreign.jazz"})
            (<Jazz-File>       name: header      source: {File :context "src" "jazz" "opengl" "glew" "header.jazz"})
            (<Jazz-File>       name: windows     source: {File :context "src" "jazz" "opengl" "glew" "windows.jazz"})
            (<Jazz-File>       name: x11         source: {File :context "src" "jazz" "opengl" "glew" "x11.jazz"}))
          (<Project-Directory>                   directory: {Directory :context "src" "jazz" "opengl" "platform"}
            (<Jazz-File>       name: _platform   source: {File :context "src" "jazz" "opengl" "platform" "_platform.jazz"})
            (<Jazz-File>       name: cocoa       source: {File :context "src" "jazz" "opengl" "platform" "cocoa.jazz"})
            (<Jazz-File>       name: windows     source: {File :context "src" "jazz" "opengl" "platform" "windows.jazz"})
            (<Jazz-File>       name: x11         source: {File :context "src" "jazz" "opengl" "platform" "x11.jazz"}))
          (<Project-Directory>                   directory: {Directory :context "src" "jazz" "opengl" "window"}
            (<Jazz-File>       name: _window     source: {File :context "src" "jazz" "opengl" "window" "_window.jazz"})
            (<Jazz-File>       name: cocoa       source: {File :context "src" "jazz" "opengl" "window" "cocoa.jazz"})
            (<Jazz-File>       name: windows     source: {File :context "src" "jazz" "opengl" "window" "windows.jazz"})
            (<Jazz-File>       name: x11         source: {File :context "src" "jazz" "opengl" "window" "x11.jazz"}))
          (<Jazz-File>         name: _opengl     source: {File :context "src" "jazz" "opengl" "_opengl.jazz"})
          (<Jazz-File>         name: autoload    source: {File :context "src" "jazz" "opengl" "autoload.jazz"})
          (<Project-File>                        source: {File :context "src" "jazz" "opengl" "install.scm"})
          (<Project-File>                        source: {File :context "src" "jazz" "opengl" "product.scm"})
          (<Jazz-File>         name: syntax      source: {File :context "src" "jazz" "opengl" "syntax.jazz"})
          (<Project-Directory>                   directory: {Directory :context "src" "jazz" "opengl" "toplevel"}
            (<Jazz-File>       name: _toplevel   source: {File :context "src" "jazz" "opengl" "toplevel" "_toplevel.jazz"}))
          (<Jazz-File>         name: trace       source: {File :context "src" "jazz" "opengl" "trace.jazz"})))))))
