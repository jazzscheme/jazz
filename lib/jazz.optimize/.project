(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.optimize description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "optimize"}
          (<Jazz-File>       name: _optimize     source: {File :context "src" "jazz" "optimize" "_optimize.jazz"})
          (<Jazz-File>       name: syntax        source: {File :context "src" "jazz" "optimize" "syntax.jazz"})))))))
