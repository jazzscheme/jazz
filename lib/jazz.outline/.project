;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.outline     description-file: {File :context ".package"}
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                        directory: {Directory :context "src" "jazz" "outline"}
          (<Jazz-File>       name: Display-Outline  source: {File :context "src" "jazz" "outline" "Display-Outline.jazz"})
          (<Jazz-File>       name: Outline-Model    source: {File :context "src" "jazz" "outline" "Outline-Model.jazz"})
          (<Jazz-File>       name: Outline-Row      source: {File :context "src" "jazz" "outline" "Outline-Row.jazz"})
          (<Jazz-File>       name: Outline-View-Old source: {File :context "src" "jazz" "outline" "Outline-View-Old.jazz"})
          (<Jazz-File>       name: Outline-View     source: {File :context "src" "jazz" "outline" "Outline-View.jazz"})
          (<Jazz-File>       name: Outline          source: {File :context "src" "jazz" "outline" "Outline.jazz"})
          (<Jazz-File>       name: Sparse-Outline   source: {File :context "src" "jazz" "outline" "Sparse-Outline.jazz"})
          (<Jazz-File>       name: _outline         source: {File :context "src" "jazz" "outline" "_outline.jazz"})
          (<Jazz-File>       name: descriptors      source: {File :context "src" "jazz" "outline" "descriptors.jazz"})
          (<Jazz-File>       name: Root-Row         source: {File :context "src" "jazz" "outline" "Root-Row.jazz"})))))))
