;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.platform.com description-file: {File :context ".package"}
    (<Project-Directory>                               directory: {Directory :context "src"}
      (<Project-Directory>                             directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                           directory: {Directory :context "src" "jazz" "platform"}
          (<Project-Directory>                         directory: {Directory :context "src" "jazz" "platform" "com"}
            (<Jazz-File>       name: _com              source: {File :context "src" "jazz" "platform" "com" "_com.jazz"})
            (<Jazz-File>       name: COM-ffi-struct    source: {File :context "src" "jazz" "platform" "com" "COM-ffi-struct.jazz"})
            (<Jazz-File>       name: syntax            source: {File :context "src" "jazz" "platform" "com" "syntax.jazz"})
            (<Jazz-File>       name: product           source: {File :context "src" "jazz" "platform" "com" "product.jazz"}))))))))
