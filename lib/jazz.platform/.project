(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.platform  description-file: {File :context ".package"}
    (<Project-Directory>                              directory: {Directory :context "src"}
      (<Project-Directory>                            directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                          directory: {Directory :context "src" "jazz" "platform"}
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "cairo"}
            (<Jazz-File>         name: _cairo         source: {File :context "src" "jazz" "platform" "cairo" "_cairo.jazz"})
            (<Jazz-File>         name: cairo-freetype source: {File :context "src" "jazz" "platform" "cairo" "cairo-freetype.jazz"})
            (<Jazz-File>         name: cairo-logfont  source: {File :context "src" "jazz" "platform" "cairo" "cairo-logfont.jazz"})
            (<Jazz-File>         name: cairo-windows  source: {File :context "src" "jazz" "platform" "cairo" "cairo-windows.jazz"})
            (<Jazz-File>         name: cairo-x11      source: {File :context "src" "jazz" "platform" "cairo" "cairo-x11.jazz"})
            (<Jazz-File>         name: cairo-base     source: {File :context "src" "jazz" "platform" "cairo" "cairo-base.jazz"})
            (<Jazz-File>         name: cairo-quartz   source: {File :context "src" "jazz" "platform" "cairo" "cairo-quartz.jazz"}))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "carbon"}
            (<Jazz-File>         name: _carbon        source: {File :context "src" "jazz" "platform" "carbon" "_carbon.jazz"})
            (<Jazz-File>         name: carbon-types   source: {File :context "src" "jazz" "platform" "carbon" "carbon-types.jazz"})
            (<Jazz-File>         name: clipboard      source: {File :context "src" "jazz" "platform" "carbon" "clipboard.jazz"}))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "crash"}
            (<Jazz-File>         name: _crash         source: {File :context "src" "jazz" "platform" "crash" "_crash.jazz"})
            (<Jazz-File>         name: windows        source: {File :context "src" "jazz" "platform" "crash" "windows.jazz"})
            (<Jazz-File>         name: mac            source: {File :context "src" "jazz" "platform" "crash" "mac.jazz"})
            (<Jazz-File>         name: unix           source: {File :context "src" "jazz" "platform" "crash" "unix.jazz"}))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "freetype"}
            (<Jazz-File>         name: _freetype      source: {File :context "src" "jazz" "platform" "freetype" "_freetype.jazz"}))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "swig"}
            (<Project-Directory>                      directory: {Directory :context "src" "jazz" "platform" "swig" "example"}
              (<Project-File>                         source: {File :context "src" "jazz" "platform" "swig" "example" "example.h"})
              (<Project-File>                         source: {File :context "src" "jazz" "platform" "swig" "example" "example.scm"})))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "windows"}
            (<Project-Directory>                      directory: {Directory :context "src" "jazz" "platform" "windows" "com"}
              (<Jazz-File>       name: _com           source: {File :context "src" "jazz" "platform" "windows" "com" "_com.jazz"})
              (<Jazz-File>       name: COM-ffi-struct source: {File :context "src" "jazz" "platform" "windows" "com" "COM-ffi-struct.jazz"}))
            (<Jazz-File>         name: _windows       source: {File :context "src" "jazz" "platform" "windows" "_windows.jazz"})
            (<Jazz-File>         name: WinBase        source: {File :context "src" "jazz" "platform" "windows" "WinBase.jazz"})
            (<Jazz-File>         name: WinCtrl        source: {File :context "src" "jazz" "platform" "windows" "WinCtrl.jazz"})
            (<Jazz-File>         name: WinDef         source: {File :context "src" "jazz" "platform" "windows" "WinDef.jazz"})
            (<Jazz-File>         name: WinDlg         source: {File :context "src" "jazz" "platform" "windows" "WinDlg.jazz"})
            (<Jazz-File>         name: WinGDI         source: {File :context "src" "jazz" "platform" "windows" "WinGDI.jazz"})
            (<Jazz-File>         name: WinIDL         source: {File :context "src" "jazz" "platform" "windows" "WinIDL.jazz"})
            (<Jazz-File>         name: WinKernel      source: {File :context "src" "jazz" "platform" "windows" "WinKernel.jazz"})
            (<Jazz-File>         name: WinMM          source: {File :context "src" "jazz" "platform" "windows" "WinMM.jazz"})
            (<Jazz-File>         name: WinNT          source: {File :context "src" "jazz" "platform" "windows" "WinNT.jazz"})
            (<Jazz-File>         name: WinPerf        source: {File :context "src" "jazz" "platform" "windows" "WinPerf.jazz"})
            (<Jazz-File>         name: WinPSAPI       source: {File :context "src" "jazz" "platform" "windows" "WinPSAPI.jazz"})
            (<Jazz-File>         name: WinShell       source: {File :context "src" "jazz" "platform" "windows" "WinShell.jazz"})
            (<Jazz-File>         name: WinTypes       source: {File :context "src" "jazz" "platform" "windows" "WinTypes.jazz"})
            (<Jazz-File>         name: WinUser        source: {File :context "src" "jazz" "platform" "windows" "WinUser.jazz"}))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "x11"}
            (<Jazz-File>         name: _x11           source: {File :context "src" "jazz" "platform" "x11" "_x11.jazz"})
            (<Jazz-File>         name: x11-types      source: {File :context "src" "jazz" "platform" "x11" "x11-types.jazz"}))
          (<Jazz-File>           name: _platform      source: {File :context "src" "jazz" "platform" "_platform.jazz"})
          (<Project-File>                             source: {File :context "src" "jazz" "platform" "product.scm"})
          (<Jazz-File>           name: types-syntax   source: {File :context "src" "jazz" "platform" "types-syntax.jazz"})
          (<Jazz-File>           name: types          source: {File :context "src" "jazz" "platform" "types.jazz"})
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "odbc"}
            (<Jazz-File>         name: _odbc          source: {File :context "src" "jazz" "platform" "odbc" "_odbc.jazz"})
            (<Jazz-File>         name: odbc-lowlevel  source: {File :context "src" "jazz" "platform" "odbc" "odbc-lowlevel.jazz"}))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "minilzo"}
            (<Jazz-File>         name: _minilzo       source: {File :context "src" "jazz" "platform" "minilzo" "_minilzo.jazz"}))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "platform" "cocoa"}
            (<Jazz-File>         name: _cocoa         source: {File :context "src" "jazz" "platform" "cocoa" "_cocoa.jazz"})
            (<Jazz-File>         name: foreign        source: {File :context "src" "jazz" "platform" "cocoa" "foreign.jazz"}))))))))
