;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.platform description-file: {File :context ".package"}
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                       directory: {Directory :context "src" "jazz" "platform"}
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "windows"}
            (<Jazz-File>       name: _windows      source: {File :context "src" "jazz" "platform" "windows" "_windows.jazz"})
            (<Jazz-File>       name: Base          source: {File :context "src" "jazz" "platform" "windows" "Base.jazz"})
            (<Jazz-File>       name: Ctrl          source: {File :context "src" "jazz" "platform" "windows" "Ctrl.jazz"})
            (<Jazz-File>       name: Def           source: {File :context "src" "jazz" "platform" "windows" "Def.jazz"})
            (<Jazz-File>       name: Dlg           source: {File :context "src" "jazz" "platform" "windows" "Dlg.jazz"})
            (<Jazz-File>       name: GDI           source: {File :context "src" "jazz" "platform" "windows" "GDI.jazz"})
            (<Jazz-File>       name: IDL           source: {File :context "src" "jazz" "platform" "windows" "IDL.jazz"})
            (<Jazz-File>       name: Kernel        source: {File :context "src" "jazz" "platform" "windows" "Kernel.jazz"})
            (<Jazz-File>       name: MM            source: {File :context "src" "jazz" "platform" "windows" "MM.jazz"})
            (<Jazz-File>       name: NT            source: {File :context "src" "jazz" "platform" "windows" "NT.jazz"})
            (<Jazz-File>       name: Perf          source: {File :context "src" "jazz" "platform" "windows" "Perf.jazz"})
            (<Jazz-File>       name: PSAPI         source: {File :context "src" "jazz" "platform" "windows" "PSAPI.jazz"})
            (<Jazz-File>       name: Shell         source: {File :context "src" "jazz" "platform" "windows" "Shell.jazz"})
            (<Jazz-File>       name: Types         source: {File :context "src" "jazz" "platform" "windows" "Types.jazz"})
            (<Jazz-File>       name: User          source: {File :context "src" "jazz" "platform" "windows" "User.jazz"}))
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "x11"}
            (<Jazz-File>       name: _x11          source: {File :context "src" "jazz" "platform" "x11" "_x11.jazz"})
            (<Jazz-File>       name: x11-types     source: {File :context "src" "jazz" "platform" "x11" "x11-types.jazz"}))
          (<Jazz-File>         name: _platform     source: {File :context "src" "jazz" "platform" "_platform.jazz"})
          (<Project-File>                          source: {File :context "src" "jazz" "platform" "product.scm"})
          (<Jazz-File>         name: types-syntax  source: {File :context "src" "jazz" "platform" "types-syntax.jazz"})
          (<Jazz-File>         name: types         source: {File :context "src" "jazz" "platform" "types.jazz"})
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "cocoa"}
            (<Jazz-File>       name: _cocoa        source: {File :context "src" "jazz" "platform" "cocoa" "_cocoa.jazz"})
            (<Jazz-File>       name: foreign       source: {File :context "src" "jazz" "platform" "cocoa" "foreign.jazz"}))
          (<Project-File>                          source: {File :context "src" "jazz" "platform" "install.scm"})
          (<Jazz-File>         name: requests      source: {File :context "src" "jazz" "platform" "requests.jazz"})
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "ios"}
            (<Jazz-File>       name: _ios          source: {File :context "src" "jazz" "platform" "ios" "_ios.jazz"})
            (<Jazz-File>       name: foreign       source: {File :context "src" "jazz" "platform" "ios" "foreign.jazz"})
            (<Jazz-File>       name: camera        source: {File :context "src" "jazz" "platform" "ios" "camera.jazz"}))
          (<Jazz-File>         name: callback      source: {File :context "src" "jazz" "platform" "callback.jazz"})
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "library"}
            (<Jazz-File>       name: _library      source: {File :context "src" "jazz" "platform" "library" "_library.jazz"})
            (<Jazz-File>       name: cocoa         source: {File :context "src" "jazz" "platform" "library" "cocoa.jazz"}))
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "unix"}
            (<Jazz-File>       name: _unix         source: {File :context "src" "jazz" "platform" "unix" "_unix.jazz"})
            (<Jazz-File>       name: foreign       source: {File :context "src" "jazz" "platform" "unix" "foreign.jazz"}))
          (<Jazz-File>         name: syntax        source: {File :context "src" "jazz" "platform" "syntax.jazz"})
          (<Jazz-File>         name: trace         source: {File :context "src" "jazz" "platform" "trace.jazz"})))))))
