;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.platform description-file: {File :context ".package"}
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                       directory: {Directory :context "src" "jazz" "platform"}
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "windows"}
            (<Jazz-File>       name: _windows      source: {File :context "src" "jazz" "platform" "windows" "_windows.jazz"})
            (<Jazz-File>       name: WinBase       source: {File :context "src" "jazz" "platform" "windows" "WinBase.jazz"})
            (<Jazz-File>       name: WinCtrl       source: {File :context "src" "jazz" "platform" "windows" "WinCtrl.jazz"})
            (<Jazz-File>       name: WinDef        source: {File :context "src" "jazz" "platform" "windows" "WinDef.jazz"})
            (<Jazz-File>       name: WinDlg        source: {File :context "src" "jazz" "platform" "windows" "WinDlg.jazz"})
            (<Jazz-File>       name: WinGDI        source: {File :context "src" "jazz" "platform" "windows" "WinGDI.jazz"})
            (<Jazz-File>       name: WinIDL        source: {File :context "src" "jazz" "platform" "windows" "WinIDL.jazz"})
            (<Jazz-File>       name: WinKernel     source: {File :context "src" "jazz" "platform" "windows" "WinKernel.jazz"})
            (<Jazz-File>       name: WinMM         source: {File :context "src" "jazz" "platform" "windows" "WinMM.jazz"})
            (<Jazz-File>       name: WinNT         source: {File :context "src" "jazz" "platform" "windows" "WinNT.jazz"})
            (<Jazz-File>       name: WinPerf       source: {File :context "src" "jazz" "platform" "windows" "WinPerf.jazz"})
            (<Jazz-File>       name: WinPSAPI      source: {File :context "src" "jazz" "platform" "windows" "WinPSAPI.jazz"})
            (<Jazz-File>       name: WinShell      source: {File :context "src" "jazz" "platform" "windows" "WinShell.jazz"})
            (<Jazz-File>       name: WinTypes      source: {File :context "src" "jazz" "platform" "windows" "WinTypes.jazz"})
            (<Jazz-File>       name: WinUser       source: {File :context "src" "jazz" "platform" "windows" "WinUser.jazz"}))
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "x11"}
            (<Jazz-File>       name: _x11          source: {File :context "src" "jazz" "platform" "x11" "_x11.jazz"})
            (<Jazz-File>       name: x11-types     source: {File :context "src" "jazz" "platform" "x11" "x11-types.jazz"}))
          (<Jazz-File>         name: _platform     source: {File :context "src" "jazz" "platform" "_platform.jazz"})
          (<Project-File>                          source: {File :context "src" "jazz" "platform" "product.scm"})
          (<Jazz-File>         name: types-syntax  source: {File :context "src" "jazz" "platform" "types-syntax.jazz"})
          (<Jazz-File>         name: types         source: {File :context "src" "jazz" "platform" "types.jazz"})
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "cocoa"}
            (<Jazz-File>       name: _cocoa        source: {File :context "src" "jazz" "platform" "cocoa" "_cocoa.jazz"})
            (<Jazz-File>       name: foreign       source: {File :context "src" "jazz" "platform" "cocoa" "foreign.jazz"}))
          (<Project-File>                          source: {File :context "src" "jazz" "platform" "install.scm"})
          (<Jazz-File>         name: requests      source: {File :context "src" "jazz" "platform" "requests.jazz"})
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "platform" "ios"}
            (<Jazz-File>       name: _ios          source: {File :context "src" "jazz" "platform" "ios" "_ios.jazz"})
            (<Jazz-File>       name: foreign       source: {File :context "src" "jazz" "platform" "ios" "foreign.jazz"})
            (<Jazz-File>       name: camera        source: {File :context "src" "jazz" "platform" "ios" "camera.jazz"}))
          (<Jazz-File>         name: callback      source: {File :context "src" "jazz" "platform" "callback.jazz"})))))))
