;;;
;;;; Keyboard
;;;


(definition public (key-down? key)
  #f)


(definition public (shift-down?)
  #f)


(definition public (control-down?)
  #f)


(definition public (alt-down?)
  #f)


(definition public (meta-down?)
  #f)


(definition public (modifiers-down)
  (values (shift-down?)
          (control-down?)
          (alt-down?)
          (meta-down?)))


