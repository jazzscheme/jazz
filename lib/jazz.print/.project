(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.print           description-file: {File :context ".package"}
    (<Project-Directory>                                directory: {Directory :context "src"}
      (<Project-Directory>                              directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                            directory: {Directory :context "src" "jazz" "print"}
          (<Jazz-File>       name: _print               source: {File :context "src" "jazz" "print" "_print.jazz"})
          (<Jazz-File>       name: Page-Content         source: {File :context "src" "jazz" "print" "Page-Content.jazz"})
          (<Jazz-File>       name: Page-Document        source: {File :context "src" "jazz" "print" "Page-Document.jazz"})
          (<Jazz-File>       name: Page-Editor          source: {File :context "src" "jazz" "print" "Page-Editor.jazz"})
          (<Jazz-File>       name: Page-Footer          source: {File :context "src" "jazz" "print" "Page-Footer.jazz"})
          (<Jazz-File>       name: Page-Header          source: {File :context "src" "jazz" "print" "Page-Header.jazz"})
          (<Jazz-File>       name: Page-Printable       source: {File :context "src" "jazz" "print" "Page-Printable.jazz"})
          (<Jazz-File>       name: Page-Setup           source: {File :context "src" "jazz" "print" "Page-Setup.jazz"})
          (<Jazz-File>       name: Page-Status          source: {File :context "src" "jazz" "print" "Page-Status.jazz"})
          (<Jazz-File>       name: Page-Text-Palette    source: {File :context "src" "jazz" "print" "Page-Text-Palette.jazz"})
          (<Jazz-File>       name: Page-Text-View       source: {File :context "src" "jazz" "print" "Page-Text-View.jazz"})
