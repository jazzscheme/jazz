(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                catalog?: #t catalog-extensions: :text include-extensions: :text catalog-name: jazz.profile
    (<Project-Directory>                    directory: {Directory :context "src"}
      (<Project-Directory>                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                directory: {Directory :context "src" "jazz" "profile"}
          (<Jazz-File>       name: _profile source: {File :context "src" "jazz" "profile" "_profile.jazz"})
          (<Jazz-File>       name: autoload source: {File :context "src" "jazz" "profile" "autoload.jazz"})
          (<Jazz-File>       name: Me       source: {File :context "src" "jazz" "profile" "Me.jazz"})
          (<Jazz-File>       name: Profile  source: {File :context "src" "jazz" "profile" "Profile.jazz"})))))))
