(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.python              description-file: {File :context ".package"}
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "python"}
          (<Jazz-File>       name: _python                  source: {File :context "src" "jazz" "python" "_python.jazz"})))))))
