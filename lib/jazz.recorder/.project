(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.recorder     description-file: {File :context ".package"}
    (<Project-Directory>                               directory: {Directory :context "src"}
      (<Project-Directory>                             directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                           directory: {Directory :context "src" "jazz" "recorder"}
          (<Jazz-File>         name: _recorder         source: {File :context "src" "jazz" "recorder" "_recorder.jazz"})
          (<Jazz-File>         name: autoload          source: {File :context "src" "jazz" "recorder" "autoload.jazz"})
          (<Jazz-File>         name: Macro-Recorder    source: {File :context "src" "jazz" "recorder" "Macro-Recorder.jazz"})
          (<Jazz-File>         name: Recordable        source: {File :context "src" "jazz" "recorder" "Recordable.jazz"})
          (<Jazz-File>         name: Recording-Manager source: {File :context "src" "jazz" "recorder" "Recording-Manager.jazz"})
          (<Project-Directory>                         directory: {Directory :context "src" "jazz" "recorder" "playee"}
            (<Jazz-File>       name: _playee           source: {File :context "src" "jazz" "recorder" "playee" "_playee.jazz"})
            (<Jazz-File>       name: stub              source: {File :context "src" "jazz" "recorder" "playee" "stub.jazz"})
            (<Jazz-File>       name: Recorder-Playee   source: {File :context "src" "jazz" "recorder" "playee" "Recorder-Playee.jazz"}))
          (<Project-Directory>                         directory: {Directory :context "src" "jazz" "recorder" "player"}
            (<Jazz-File>       name: stub              source: {File :context "src" "jazz" "recorder" "player" "stub.jazz"})
            (<Jazz-File>       name: _player           source: {File :context "src" "jazz" "recorder" "player" "_player.jazz"})
            (<Jazz-File>       name: Recorder-Player   source: {File :context "src" "jazz" "recorder" "player" "Recorder-Player.jazz"}))))))))
