(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                      catalog?: #t catalog-extensions: :text include-extensions: :text catalog-name: jazz.recorder
    (<Project-Directory>                          directory: {Directory :context "src"}
      (<Project-Directory>                        directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                      directory: {Directory :context "src" "jazz" "recorder"}
          (<Jazz-File>       name: _recorder      source: {File :context "src" "jazz" "recorder" "_recorder.jazz"})
          (<Jazz-File>       name: autoload       source: {File :context "src" "jazz" "recorder" "autoload.jazz"})
          (<Jazz-File>       name: Macro-Recorder source: {File :context "src" "jazz" "recorder" "Macro-Recorder.jazz"})
          (<Jazz-File>       name: Recordable     source: {File :context "src" "jazz" "recorder" "Recordable.jazz"})))))))
