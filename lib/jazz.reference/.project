;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.reference       description-file: {File :context ".package"}
    (<Project-Directory>                                directory: {Directory :context "src"}
      (<Project-Directory>                              directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                            directory: {Directory :context "src" "jazz" "reference"}
          (<Jazz-File>       name: _reference           source: {File :context "src" "jazz" "reference" "_reference.jazz"})
          (<Jazz-File>       name: Class-Reference      source: {File :context "src" "jazz" "reference" "Class-Reference.jazz"})
          (<Jazz-File>       name: Environment-Provider source: {File :context "src" "jazz" "reference" "Environment-Provider.jazz"})
          (<Jazz-File>       name: Environment          source: {File :context "src" "jazz" "reference" "Environment.jazz"})
          (<Jazz-File>       name: Reference            source: {File :context "src" "jazz" "reference" "Reference.jazz"})
          (<Jazz-File>       name: autoload             source: {File :context "src" "jazz" "reference" "autoload.jazz"})
          (<Project-File>                               source: {File :context "src" "jazz" "reference" "install.scm"})
          (<Jazz-File>       name: syntax               source: {File :context "src" "jazz" "reference" "syntax.jazz"})))))))
