;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.reference description-file: {File :context ".package"}
    (<Project-Directory>                          directory: {Directory :context "src"}
      (<Project-Directory>                        directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                      directory: {Directory :context "src" "jazz" "reference"}
          (<Jazz-File>       name: _reference     source: {File :context "src" "jazz" "reference" "_reference.jazz"})
          (<Project-File>                         source: {File :context "src" "jazz" "reference" "install.scm"})
          (<Jazz-File>       name: syntax         source: {File :context "src" "jazz" "reference" "syntax.jazz"})))))))
