;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.reporter        description-file: {File :context ".package"}
    (<Project-Directory>                                directory: {Directory :context "src"}
      (<Project-Directory>                              directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                            directory: {Directory :context "src" "jazz" "reporter"}
          (<Jazz-File>       name: Log-Reporter         source: {File :context "src" "jazz" "reporter" "Log-Reporter.jazz"})
          (<Jazz-File>       name: Multiplexer-Reporter source: {File :context "src" "jazz" "reporter" "Multiplexer-Reporter.jazz"})
          (<Jazz-File>       name: Reporter             source: {File :context "src" "jazz" "reporter" "Reporter.jazz"})
          (<Jazz-File>       name: _reporter            source: {File :context "src" "jazz" "reporter" "_reporter.jazz"})))))))
