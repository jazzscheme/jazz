(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.resource description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "resource"}
          (<Jazz-File>       name: _resource     source: {File :context "src" "jazz" "resource" "_resource.jazz"})
          (<Project-File>                        source: {File :context "src" "jazz" "resource" "install.scm"})))))))
