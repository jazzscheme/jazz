;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.resource   description-file: {File :context ".package"}
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                       directory: {Directory :context "src" "jazz" "resource"}
          (<Jazz-File>       name: _resource       source: {File :context "src" "jazz" "resource" "_resource.jazz"})
          (<Jazz-File>       name: autoload        source: {File :context "src" "jazz" "resource" "autoload.jazz"})
          (<Jazz-File>       name: Cursor-Resource source: {File :context "src" "jazz" "resource" "Cursor-Resource.jazz"})
          (<Jazz-File>       name: Icon-Resource   source: {File :context "src" "jazz" "resource" "Icon-Resource.jazz"})
          (<Jazz-File>       name: Image-Resource  source: {File :context "src" "jazz" "resource" "Image-Resource.jazz"})
          (<Project-File>                          source: {File :context "src" "jazz" "resource" "install.scm"})
          (<Jazz-File>       name: literals        source: {File :context "src" "jazz" "resource" "literals.jazz"})
          (<Jazz-File>       name: Resource        source: {File :context "src" "jazz" "resource" "Resource.jazz"})
          (<Jazz-File>       name: Sound-Resource  source: {File :context "src" "jazz" "resource" "Sound-Resource.jazz"})))))))
