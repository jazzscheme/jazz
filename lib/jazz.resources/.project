(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                  catalog?: #t catalog-extensions: :text include-extensions: :text catalog-name: jazz.resources
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                  directory: {Directory :context "src" "jazz" "resources"}
          (<Jazz-File>       name: _resources source: {File :context "src" "jazz" "resources" "_resources.scm"})))))))
