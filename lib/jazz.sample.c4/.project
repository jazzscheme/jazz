(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.sample.c4 description-file: {File :context ".package"}
    (<*>                                            tag-reference: {File :context "c4.project"})
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                        directory: {Directory :context "src" "jazz" "sample"}
          (<Project-Directory>                      directory: {Directory :context "src" "jazz" "sample" "c4"}
            (<Jazz-File>       name: C4-AI          source: {File :context "src" "jazz" "sample" "c4" "C4-AI.jazz"})
            (<Jazz-File>       name: C4-Application source: {File :context "src" "jazz" "sample" "c4" "C4-Application.jazz"})
            (<Jazz-File>       name: C4-Board       source: {File :context "src" "jazz" "sample" "c4" "C4-Board.jazz"})
            (<Jazz-File>       name: _c4            source: {File :context "src" "jazz" "sample" "c4" "_c4.jazz"})
            (<Jazz-File>       name: autoload       source: {File :context "src" "jazz" "sample" "c4" "autoload.jazz"})
            (<Jazz-File>       name: graphics       source: {File :context "src" "jazz" "sample" "c4" "graphics.jazz"})
            (<Jazz-File>       name: run            source: {File :context "src" "jazz" "sample" "c4" "run.jazz"})
            (<Jazz-File>       name: C4-Human       source: {File :context "src" "jazz" "sample" "c4" "C4-Human.jazz"})
            (<Jazz-File>       name: C4-Game        source: {File :context "src" "jazz" "sample" "c4" "C4-Game.jazz"})
            (<Jazz-File>       name: test           source: {File :context "src" "jazz" "sample" "c4" "test.jazz"}))))))))
