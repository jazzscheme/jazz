(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.sample.fib catalog?: #t catalog-extensions: :jazz include-extensions: :jazz
    (<Project-Directory>                             directory: {Directory :context "fib"}
      (<Project-File>                                source: {File :context "fib" "fib.scm"}))
    (<Project-Directory>                             directory: {Directory :context "src"}
      (<Project-Directory>                           directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                         directory: {Directory :context "src" "jazz" "sample"}
          (<Project-Directory>                       directory: {Directory :context "src" "jazz" "sample" "fib"}
            (<Jazz-File>       name: _fib            source: {File :context "src" "jazz" "sample" "fib" "_fib.jazz"})))))
    (<Project-File>                                  source: {File :context "README"}))))
