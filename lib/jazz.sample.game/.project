(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.sample.game description-file: {File :context ".package"}
    (<Project-Directory>                              directory: {Directory :context "src"}
      (<Project-Directory>                            directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                          directory: {Directory :context "src" "jazz" "sample"}
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "sample" "game"}
            (<Jazz-File>       name: _game            source: {File :context "src" "jazz" "sample" "game" "_game.jazz"})
            (<Jazz-File>       name: minimax          source: {File :context "src" "jazz" "sample" "game" "minimax.jazz"}))))))))
