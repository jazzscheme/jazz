(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.sample.gameserver
    (<*>                                                    tag-reference: {File :context "gameserver.project"})
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "sample"}
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "sample" "gameserver"}
            (<Jazz-File>       name: Game-Pages             source: {File :context "src" "jazz" "sample" "gameserver" "Game-Pages.jazz"})
            (<Jazz-File>       name: Game-Server            source: {File :context "src" "jazz" "sample" "gameserver" "Game-Server.jazz"})
            (<Jazz-File>       name: Games-Pages            source: {File :context "src" "jazz" "sample" "gameserver" "Games-Pages.jazz"})
            (<Jazz-File>       name: _gameserver            source: {File :context "src" "jazz" "sample" "gameserver" "_gameserver.jazz"})
            (<Jazz-File>       name: autoload               source: {File :context "src" "jazz" "sample" "gameserver" "autoload.jazz"})
            (<Jazz-File>       name: imagemagick            source: {File :context "src" "jazz" "sample" "gameserver" "imagemagick.jazz"})
            (<Jazz-File>       name: run                    source: {File :context "src" "jazz" "sample" "gameserver" "run.jazz"})
            (<Jazz-File>       name: Gomoku-Pages           source: {File :context "src" "jazz" "sample" "gameserver" "Gomoku-Pages.jazz"})
            (<Jazz-File>       name: C4-Pages               source: {File :context "src" "jazz" "sample" "gameserver" "C4-Pages.jazz"}))))))))
