;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                            catalog?: #t catalog-extensions: :jazz include-extensions: :jazz catalog-name: jazz.sample.gomoku
    (<*>                                                tag-reference: {File :context "gomoku.project"})
    (<Project-Directory>                                directory: {Directory :context "src"}
      (<Project-Directory>                              directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                            directory: {Directory :context "src" "jazz" "sample"}
          (<Project-Directory>                          directory: {Directory :context "src" "jazz" "sample" "gomoku"}
            (<Jazz-File>       name: Gomoku-AI          source: {File :context "src" "jazz" "sample" "gomoku" "Gomoku-AI.jazz"})
            (<Jazz-File>       name: Gomoku-Application source: {File :context "src" "jazz" "sample" "gomoku" "Gomoku-Application.jazz"})
            (<Jazz-File>       name: Gomoku-Board       source: {File :context "src" "jazz" "sample" "gomoku" "Gomoku-Board.jazz"})
            (<Jazz-File>       name: _gomoku            source: {File :context "src" "jazz" "sample" "gomoku" "_gomoku.jazz"})
            (<Jazz-File>       name: autoload           source: {File :context "src" "jazz" "sample" "gomoku" "autoload.jazz"})
            (<Jazz-File>       name: graphics           source: {File :context "src" "jazz" "sample" "gomoku" "graphics.jazz"})
            (<Jazz-File>       name: run                source: {File :context "src" "jazz" "sample" "gomoku" "run.jazz"})
            (<Jazz-File>       name: test               source: {File :context "src" "jazz" "sample" "gomoku" "test.jazz"}))))))))
