(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.sample.mandelbrot description-file: {File :context ".package"}
    (<*>                                                    tag-reference: {File :context "mandelbrot.project"})
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "sample"}
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "sample" "mandelbrot"}
            (<Jazz-File>       name: Mandelbrot             source: {File :context "src" "jazz" "sample" "mandelbrot" "Mandelbrot.jazz"})
            (<Jazz-File>       name: _mandelbrot            source: {File :context "src" "jazz" "sample" "mandelbrot" "_mandelbrot.jazz"})
            (<Jazz-File>       name: autoload               source: {File :context "src" "jazz" "sample" "mandelbrot" "autoload.jazz"})
            (<Jazz-File>       name: test                   source: {File :context "src" "jazz" "sample" "mandelbrot" "test.jazz"}))))))))
