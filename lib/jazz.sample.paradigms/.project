      (<Project-Directory>                             directory: {Directory :context "src" "ldap"}
        (<Jazz-File>       name: ffi                   source: {File :context "src" "ldap" "ffi.jazz"})
        (<Jazz-File>       name: ldap0                 source: {File :context "src" "ldap" "ldap0.jazz"})
        (<Jazz-File>       name: ldap1                 source: {File :context "src" "ldap" "ldap1.jazz"})
        (<Jazz-File>       name: ldap2                 source: {File :context "src" "ldap" "ldap2.jazz"})
        (<Jazz-File>       name: ldap3                 source: {File :context "src" "ldap" "ldap3.jazz"}))))))
