(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                                catalog?: #t catalog-extensions: :jazz include-extensions: :jazz catalog-name: jazz.sample.world
    (<*>                                    tag-reference: {File :context "world.project"})
    (<Project-Directory>                    directory: {Directory :context "src"}
      (<Project-Directory>                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                directory: {Directory :context "src" "jazz" "sample"}
          (<Project-Directory>              directory: {Directory :context "src" "jazz" "sample" "world"}
            (<Jazz-File>       name: _world source: {File :context "src" "jazz" "sample" "world" "_world.jazz"})
            (<Jazz-File>       name: run    source: {File :context "src" "jazz" "sample" "world" "run.jazz"}))))))))
