;;;==============
;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.c)
        (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.sample
    (<Project-Directory>                           directory: {Directory :context "files"}
      (<Project-Directory>                         directory: {Directory :context "files" "c"}
        (<C-File>                                  source: {File :context "files" "c" "Sample.cpp"})
        (<Project-File>                            source: {File :context "files" "c" "Sample.h"}))
      (<Project-Directory>                         directory: {Directory :context "files" "css"}
        (<Project-File>                            source: {File :context "files" "css" "Sample.css"}))
      (<Project-Directory>                         directory: {Directory :context "files" "javascript"}
        (<Project-File>                            source: {File :context "files" "javascript" "Sample.js"}))
      (<Project-Directory>                         directory: {Directory :context "files" "lisp"}
        (<Project-File>                            source: {File :context "files" "lisp" "sample.lisp"}))
      (<Project-Directory>                         directory: {Directory :context "files" "lua"}
        (<Project-File>                            source: {File :context "files" "lua" "Sample.lua"}))
      (<Project-Directory>                         directory: {Directory :context "files" "properties"}
        (<Project-File>                            source: {File :context "files" "properties" "Sample.properties"}))
      (<Project-Directory>                         directory: {Directory :context "files" "python"}
        (<Project-File>                            source: {File :context "files" "python" "test.py"}))
      (<Project-Directory>                         directory: {Directory :context "files" "scheme"}
        (<Project-File>                            source: {File :context "files" "scheme" "Sample.scm"}))
      (<Project-Directory>                         directory: {Directory :context "files" "sql"}
        (<Project-File>                            source: {File :context "files" "sql" "Sample.sql"}))
      (<Project-Directory>                         directory: {Directory :context "files" "java"}
        (<Project-Directory>                       directory: {Directory :context "files" "java" "org"}
          (<Project-Directory>                     directory: {Directory :context "files" "java" "org" "jazz"}
            (<Project-File>                        source: {File :context "files" "java" "org" "jazz" "I.java"})
            (<Project-File>                        source: {File :context "files" "java" "org" "jazz" "J.java"})
            (<Project-File>                        source: {File :context "files" "java" "org" "jazz" "X.java"})
            (<Project-File>                        source: {File :context "files" "java" "org" "jazz" "Y.java"})
            (<Project-File>                        source: {File :context "files" "java" "org" "jazz" "Z.java"})))))
    (<Project-Directory>                           directory: {Directory :context "src"}
      (<Project-Directory>                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                       directory: {Directory :context "src" "jazz" "sample"}
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "sample" "error"}
            (<Project-Directory>                   directory: {Directory :context "src" "jazz" "sample" "error" "unit"}
              (<Jazz-File>       name: empty       source: {File :context "src" "jazz" "sample" "error" "unit" "empty.jazz"})
              (<Jazz-File>       name: extraneous  source: {File :context "src" "jazz" "sample" "error" "unit" "extraneous.jazz"})
              (<Jazz-File>       name: invalid     source: {File :context "src" "jazz" "sample" "error" "unit" "invalid.jazz"}))
            (<Jazz-File>         name: _error      source: {File :context "src" "jazz" "sample" "error" "_error.jazz"})
            (<Jazz-File>         name: module      source: {File :context "src" "jazz" "sample" "error" "module.jazz"}))
          (<Project-Directory>                     directory: {Directory :context "src" "jazz" "sample" "mod"}
            (<Jazz-File>         name: _mod        source: {File :context "src" "jazz" "sample" "mod" "_mod.jazz"})
            (<Jazz-File>         name: t           source: {File :context "src" "jazz" "sample" "mod" "t.jazz"}))
          (<Jazz-File>           name: module      source: {File :context "src" "jazz" "sample" "module.jazz"})
          (<Project-File>                          source: {File :context "src" "jazz" "sample" "unit.scm"})))))))
