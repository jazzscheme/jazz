(module jazz.sample.mod.t jazz


(definition public (make-t a)
  (new T a))


(definition public inline (t-a t)
  @wait
  (get-a t))


(class T extends Object
  
  
  (slot a)
  
  
  (method (initialize self a)
    (nextmethod self)
    (set! self.a a))))
