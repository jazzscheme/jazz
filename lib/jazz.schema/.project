;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.schema            catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                                    directory: {Directory :context "src"}
      (<Project-Directory>                                  directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                directory: {Directory :context "src" "jazz" "schema"}
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "schema" "archive"}
            (<Jazz-File>       name: Archive-Schema-Browser source: {File :context "src" "jazz" "schema" "archive" "Archive-Schema-Browser.jazz"})
            (<Jazz-File>       name: Archive-Schema-Source  source: {File :context "src" "jazz" "schema" "archive" "Archive-Schema-Source.jazz"})
            (<Jazz-File>       name: Schema-Archive         source: {File :context "src" "jazz" "schema" "archive" "Schema-Archive.jazz"}))
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "schema" "data"}
            (<Jazz-File>       name: Data-Column            source: {File :context "src" "jazz" "schema" "data" "Data-Column.jazz"})
            (<Jazz-File>       name: Data-Constraint        source: {File :context "src" "jazz" "schema" "data" "Data-Constraint.jazz"})
            (<Jazz-File>       name: Data-Container         source: {File :context "src" "jazz" "schema" "data" "Data-Container.jazz"})
            (<Jazz-File>       name: Data-Entity            source: {File :context "src" "jazz" "schema" "data" "Data-Entity.jazz"})
            (<Jazz-File>       name: Data-Index             source: {File :context "src" "jazz" "schema" "data" "Data-Index.jazz"})
            (<Jazz-File>       name: Data-Object            source: {File :context "src" "jazz" "schema" "data" "Data-Object.jazz"})
            (<Jazz-File>       name: Data-Primary-Key       source: {File :context "src" "jazz" "schema" "data" "Data-Primary-Key.jazz"})
            (<Jazz-File>       name: Data-Procedure         source: {File :context "src" "jazz" "schema" "data" "Data-Procedure.jazz"})
            (<Jazz-File>       name: Data-Relation          source: {File :context "src" "jazz" "schema" "data" "Data-Relation.jazz"})
            (<Jazz-File>       name: Data-Row               source: {File :context "src" "jazz" "schema" "data" "Data-Row.jazz"})
            (<Jazz-File>       name: Data-Schema            source: {File :context "src" "jazz" "schema" "data" "Data-Schema.jazz"})
            (<Jazz-File>       name: Data-Source            source: {File :context "src" "jazz" "schema" "data" "Data-Source.jazz"})
            (<Jazz-File>       name: Data-Sources           source: {File :context "src" "jazz" "schema" "data" "Data-Sources.jazz"})
            (<Jazz-File>       name: Data-Table             source: {File :context "src" "jazz" "schema" "data" "Data-Table.jazz"})
            (<Jazz-File>       name: Data-Trigger           source: {File :context "src" "jazz" "schema" "data" "Data-Trigger.jazz"})
            (<Jazz-File>       name: Data-View              source: {File :context "src" "jazz" "schema" "data" "Data-View.jazz"})
            (<Jazz-File>       name: Datatype               source: {File :context "src" "jazz" "schema" "data" "Datatype.jazz"})
            (<Jazz-File>       name: Logical-Datatype       source: {File :context "src" "jazz" "schema" "data" "Logical-Datatype.jazz"}))
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "schema" "factory"}
            (<Jazz-File>       name: Data-Schema-Factory    source: {File :context "src" "jazz" "schema" "factory" "Data-Schema-Factory.jazz"}))
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "schema" "inspector"}
            (<Jazz-File>       name: Table-Inspector-Tree   source: {File :context "src" "jazz" "schema" "inspector" "Table-Inspector-Tree.jazz"})
            (<Jazz-File>       name: Table-Inspector        source: {File :context "src" "jazz" "schema" "inspector" "Table-Inspector.jazz"}))
          (<Project-Directory>                              directory: {Directory :context "src" "jazz" "schema" "source"}
            (<Jazz-File>       name: Schema-Source          source: {File :context "src" "jazz" "schema" "source" "Schema-Source.jazz"}))
          (<Jazz-File>         name: _schema                source: {File :context "src" "jazz" "schema" "_schema.jazz"})
          (<Jazz-File>         name: autoload               source: {File :context "src" "jazz" "schema" "autoload.jazz"})
          (<Project-File>                                   source: {File :context "src" "jazz" "schema" "install.scm"})
          (<Jazz-File>         name: literals               source: {File :context "src" "jazz" "schema" "literals.jazz"})
          (<Jazz-File>         name: syntax                 source: {File :context "src" "jazz" "schema" "syntax.jazz"})))))))
