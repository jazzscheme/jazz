;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.screen description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "screen"}
          (<Project-Directory>                   directory: {Directory :context "src" "jazz" "screen" "platform"}
            (<Jazz-File>       name: cocoa       source: {File :context "src" "jazz" "screen" "platform" "cocoa.jazz"})
            (<Jazz-File>       name: windows     source: {File :context "src" "jazz" "screen" "platform" "windows.jazz"})
            (<Jazz-File>       name: x11         source: {File :context "src" "jazz" "screen" "platform" "x11.jazz"}))
          (<Jazz-File>         name: _screen     source: {File :context "src" "jazz" "screen" "_screen.jazz"})
          (<Jazz-File>         name: Screen      source: {File :context "src" "jazz" "screen" "Screen.jazz"})))))))
