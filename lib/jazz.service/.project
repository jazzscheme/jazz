(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.service description-file: {File :context ".package"}
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "service"}
          (<Jazz-File>       name: _service     source: {File :context "src" "jazz" "service" "_service.jazz"})))))))
