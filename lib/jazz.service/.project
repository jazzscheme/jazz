;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                     name: jazz.service    description-file: {File :context ".package"}
    (<Project-Directory>                               directory: {Directory :context "src"}
      (<Project-Directory>                             directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                           directory: {Directory :context "src" "jazz" "service"}
          (<Project-Directory>                         directory: {Directory :context "src" "jazz" "service" "platform"}
            (<Project-Directory>                       directory: {Directory :context "src" "jazz" "service" "platform" "windows"}
              (<Jazz-File>       name: autoload        source: {File :context "src" "jazz" "service" "platform" "windows" "autoload.jazz"})
              (<Jazz-File>       name: Service-Handle  source: {File :context "src" "jazz" "service" "platform" "windows" "Service-Handle.jazz"})
              (<Jazz-File>       name: service-impl    source: {File :context "src" "jazz" "service" "platform" "windows" "service-impl.jazz"})
              (<Jazz-File>       name: Service-Manager source: {File :context "src" "jazz" "service" "platform" "windows" "Service-Manager.jazz"})
              (<Jazz-File>       name: Service         source: {File :context "src" "jazz" "service" "platform" "windows" "Service.jazz"})))
          (<Jazz-File>           name: _service        source: {File :context "src" "jazz" "service" "_service.jazz"})))))))
