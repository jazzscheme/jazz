(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.skin description-file: {File :context ".package"}
    (<Project-Directory>                     directory: {Directory :context "src"}
      (<Project-Directory>                   directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                 directory: {Directory :context "src" "jazz" "skin"}
          (<Jazz-File>       name: _skin     source: {File :context "src" "jazz" "skin" "_skin.jazz"})))))))
