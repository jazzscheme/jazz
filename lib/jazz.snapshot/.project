(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.snapshot description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "snapshot"}
          (<Jazz-File>       name: _snapshot     source: {File :context "src" "jazz" "snapshot" "_snapshot.jazz"})
          (<Jazz-File>       name: test          source: {File :context "src" "jazz" "snapshot" "test.jazz"})))))))
