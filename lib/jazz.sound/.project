;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.sound description-file: {File :context ".package"}
    (<Project-Directory>                      directory: {Directory :context "src"}
      (<Project-Directory>                    directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                  directory: {Directory :context "src" "jazz" "sound"}
          (<Jazz-File>       name: _sound     source: {File :context "src" "jazz" "sound" "_sound.jazz"})
          (<Project-File>                     source: {File :context "src" "jazz" "sound" "product.scm"})
          (<Jazz-File>       name: test       source: {File :context "src" "jazz" "sound" "test.jazz"})
          (<Jazz-File>       name: foreign    source: {File :context "src" "jazz" "sound" "foreign.jazz"})))))))
