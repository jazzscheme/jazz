(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.splash description-file: {File :context ".package"}
    (<Project-Directory>                       directory: {Directory :context "src"}
      (<Project-Directory>                     directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                   directory: {Directory :context "src" "jazz" "splash"}
          (<Jazz-File>       name: _splash     source: {File :context "src" "jazz" "splash" "_splash.jazz"})
          (<Jazz-File>       name: product     source: {File :context "src" "jazz" "splash" "product.jazz"})))))))
