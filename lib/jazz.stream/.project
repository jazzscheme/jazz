;;;==============
;;;  JazzScheme
;;;==============
;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.stream description-file: {File :context ".package"}
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "stream"}
          (<Jazz-File>         name: _stream     source: {File :context "src" "jazz" "stream" "_stream.jazz"})
          (<Jazz-File>         name: foreign     source: {File :context "src" "jazz" "stream" "foreign.jazz"})
          (<Jazz-File>         name: gstreamer   source: {File :context "src" "jazz" "stream" "gstreamer.jazz"})
          (<Jazz-File>         name: run         source: {File :context "src" "jazz" "stream" "run.jazz"})
          (<Jazz-File>         name: test        source: {File :context "src" "jazz" "stream" "test.jazz"})
          (<Jazz-File>         name: product     source: {File :context "src" "jazz" "stream" "product.jazz"})
          (<Jazz-File>         name: setup       source: {File :context "src" "jazz" "stream" "setup.jazz"})
          (<Jazz-File>         name: syntax      source: {File :context "src" "jazz" "stream" "syntax.jazz"})
          (<Jazz-File>         name: directories source: {File :context "src" "jazz" "stream" "directories.jazz"})
          (<Jazz-File>         name: delayed     source: {File :context "src" "jazz" "stream" "delayed.jazz"})
          (<Jazz-File>         name: header      source: {File :context "src" "jazz" "stream" "header.jazz"})
          (<Project-Directory>                   directory: {Directory :context "src" "jazz" "stream" "platform"}
            (<Jazz-File>       name: mac         source: {File :context "src" "jazz" "stream" "platform" "mac.jazz"})
            (<Jazz-File>       name: _platform   source: {File :context "src" "jazz" "stream" "platform" "_platform.jazz"})))
        (<Project-Directory>                     directory: {Directory :context "src" "jazz" "flow"}
          (<Jazz-File>         name: _flow       source: {File :context "src" "jazz" "flow" "_flow.jazz"})))))))
