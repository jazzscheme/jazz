;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                       name: jazz.system
    (<Project-Directory>                                           directory: {Directory :context "src"}
      (<Project-Directory>                                         directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                       directory: {Directory :context "src" "jazz" "system"}
          (<Project-Directory>                                     directory: {Directory :context "src" "jazz" "system" "application"}
            (<Jazz-File>           name: _application              source: {File :context "src" "jazz" "system" "application" "_application.jazz"})
            (<Jazz-File>           name: Application-Actions       source: {File :context "src" "jazz" "system" "application" "Application-Actions.jazz"})
            (<Jazz-File>           name: Application-Bindings      source: {File :context "src" "jazz" "system" "application" "Application-Bindings.jazz"})
            (<Jazz-File>           name: Application-Event         source: {File :context "src" "jazz" "system" "application" "Application-Event.jazz"})
            (<Jazz-File>           name: Application-Preferences   source: {File :context "src" "jazz" "system" "application" "Application-Preferences.jazz"})
            (<Jazz-File>           name: Application               source: {File :context "src" "jazz" "system" "application" "Application.jazz"})
            (<Jazz-File>           name: autoload                  source: {File :context "src" "jazz" "system" "application" "autoload.jazz"})
            (<Jazz-File>           name: Exception-Presenter       source: {File :context "src" "jazz" "system" "application" "Exception-Presenter.jazz"}))
          (<Project-Directory>                                     directory: {Directory :context "src" "jazz" "system" "log"}
            (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "system" "log" "platform"}
              (<Jazz-File>         name: windows                   source: {File :context "src" "jazz" "system" "log" "platform" "windows.jazz"}))
            (<Jazz-File>           name: _log                      source: {File :context "src" "jazz" "system" "log" "_log.jazz"}))
          (<Project-Directory>                                     directory: {Directory :context "src" "jazz" "system" "performance"}
            (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "system" "performance" "platform"}
              (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "system" "performance" "platform" "windows"}
                (<Jazz-File>       name: _windows                  source: {File :context "src" "jazz" "system" "performance" "platform" "windows" "_windows.jazz"})
                (<Jazz-File>       name: Performance-Query         source: {File :context "src" "jazz" "system" "performance" "platform" "windows" "Performance-Query.jazz"})))
            (<Jazz-File>           name: _performance              source: {File :context "src" "jazz" "system" "performance" "_performance.jazz"}))
          (<Project-Directory>                                     directory: {Directory :context "src" "jazz" "system" "platform"}
            (<Jazz-File>           name: carbon                    source: {File :context "src" "jazz" "system" "platform" "carbon.jazz"})
            (<Jazz-File>           name: windows                   source: {File :context "src" "jazz" "system" "platform" "windows.jazz"})
            (<Jazz-File>           name: x11                       source: {File :context "src" "jazz" "system" "platform" "x11.jazz"}))
          (<Project-Directory>                                     directory: {Directory :context "src" "jazz" "system" "process"}
            (<Jazz-File>           name: _process                  source: {File :context "src" "jazz" "system" "process" "_process.jazz"})
            (<Jazz-File>           name: autoload                  source: {File :context "src" "jazz" "system" "process" "autoload.jazz"})
            (<Jazz-File>           name: Command-Line              source: {File :context "src" "jazz" "system" "process" "Command-Line.jazz"})
            (<Jazz-File>           name: Exit-Listener             source: {File :context "src" "jazz" "system" "process" "Exit-Listener.jazz"})
            (<Jazz-File>           name: Process-Preferences       source: {File :context "src" "jazz" "system" "process" "Process-Preferences.jazz"})
            (<Jazz-File>           name: Process                   source: {File :context "src" "jazz" "system" "process" "Process.jazz"})
            (<Jazz-File>           name: Save-Environment-Listener source: {File :context "src" "jazz" "system" "process" "Save-Environment-Listener.jazz"})
            (<Jazz-File>           name: stub                      source: {File :context "src" "jazz" "system" "process" "stub.jazz"}))
          (<Project-Directory>                                     directory: {Directory :context "src" "jazz" "system" "service"}
            (<Project-Directory>                                   directory: {Directory :context "src" "jazz" "system" "service" "platform"}
              (<Project-Directory>                                 directory: {Directory :context "src" "jazz" "system" "service" "platform" "windows"}
                (<Jazz-File>       name: Service-Handle            source: {File :context "src" "jazz" "system" "service" "platform" "windows" "Service-Handle.jazz"})
                (<Jazz-File>       name: Service-Manager           source: {File :context "src" "jazz" "system" "service" "platform" "windows" "Service-Manager.jazz"})
                (<Jazz-File>       name: Service                   source: {File :context "src" "jazz" "system" "service" "platform" "windows" "Service.jazz"})
                (<Jazz-File>       name: service-impl              source: {File :context "src" "jazz" "system" "service" "platform" "windows" "service-impl.jazz"})
                (<Jazz-File>       name: autoload                  source: {File :context "src" "jazz" "system" "service" "platform" "windows" "autoload.jazz"})))
            (<Jazz-File>           name: _service                  source: {File :context "src" "jazz" "system" "service" "_service.jazz"}))
          (<Jazz-File>             name: _system                   source: {File :context "src" "jazz" "system" "_system.jazz"})
          (<Jazz-File>             name: access                    source: {File :context "src" "jazz" "system" "access.jazz"})))))))
