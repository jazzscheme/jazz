;;;==============
;;;  JazzScheme
;;;==============
;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                       name: jazz.system       documentation: {File :context "doc" "system.jml"} description-file: {File :context ".package"}
    (<Project-Directory>                                   directory: {Directory :context "src"}
      (<Project-Directory>                                 directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                               directory: {Directory :context "src" "jazz" "system"}
          (<Project-Directory>                             directory: {Directory :context "src" "jazz" "system" "log"}
            (<Project-Directory>                           directory: {Directory :context "src" "jazz" "system" "log" "platform"}
              (<Jazz-File>         name: windows           source: {File :context "src" "jazz" "system" "log" "platform" "windows.jazz"}))
            (<Jazz-File>           name: _log              source: {File :context "src" "jazz" "system" "log" "_log.jazz"}))
          (<Project-Directory>                             directory: {Directory :context "src" "jazz" "system" "performance"}
            (<Project-Directory>                           directory: {Directory :context "src" "jazz" "system" "performance" "platform"}
              (<Project-Directory>                         directory: {Directory :context "src" "jazz" "system" "performance" "platform" "windows"}
                (<Jazz-File>       name: _windows          source: {File :context "src" "jazz" "system" "performance" "platform" "windows" "_windows.jazz"})
                (<Jazz-File>       name: Performance-Query source: {File :context "src" "jazz" "system" "performance" "platform" "windows" "Performance-Query.jazz"})))
            (<Jazz-File>           name: _performance      source: {File :context "src" "jazz" "system" "performance" "_performance.jazz"}))
          (<Jazz-File>             name: _system           source: {File :context "src" "jazz" "system" "_system.jazz"}))))
    (<Project-Directory>                                   directory: {Directory :context "doc"}
      (<Project-File>                                      source: {File :context "doc" "system.jml"})))))
