      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "test"}
          (<Project-Directory>                  directory: {Directory :context "src" "jazz" "test" "module"}
