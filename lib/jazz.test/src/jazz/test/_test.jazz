@w
(class TC extends Object
  
  
  (slot row <Integer+> initialize 1)
  (slot col <fx>       initialize 3)


  (method (f)
    (when row
      (subseq! "Hello" 0 col)))


  (method (g)
    (subseq! "Hello" 0 col)))


@w
(definition (f)
  (list (class-of (f (new TC)))
        (class-of (g (new TC)))))


