(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.time description-file: {File :context ".package"}
    (<Project-Directory>                     directory: {Directory :context "src"}
      (<Project-Directory>                   directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                 directory: {Directory :context "src" "jazz" "time"}
          (<Jazz-File>       name: _time     source: {File :context "src" "jazz" "time" "_time.jazz"})
          (<Project-File>                    source: {File :context "src" "jazz" "time" "classes.scm"})
          (<Project-File>                    source: {File :context "src" "jazz" "time" "install.scm"})))))))
