          (<Jazz-File>       name: Tree-Path-Column                  source: {File :context "src" "jazz" "tree" "Tree-Path-Column.jazz"})
          (<Jazz-File>       name: Tree-Path                         source: {File :context "src" "jazz" "tree" "Tree-Path.jazz"})
          (<Jazz-File>       name: Tree-Row                          source: {File :context "src" "jazz" "tree" "Tree-Row.jazz"})
          (<Jazz-File>       name: Tree-View                         source: {File :context "src" "jazz" "tree" "Tree-View.jazz"})
          (<Jazz-File>       name: autoload                          source: {File :context "src" "jazz" "tree" "autoload.jazz"})
          (<Jazz-File>       name: preferences                       source: {File :context "src" "jazz" "tree" "preferences.jazz"})))))))
