(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.tree                         description-file: {File :context ".package"}
    (<Project-Directory>                                             directory: {Directory :context "src"}
      (<Project-Directory>                                           directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                                         directory: {Directory :context "src" "jazz" "tree"}
          (<Jazz-File>       name: _tree                             source: {File :context "src" "jazz" "tree" "_tree.jazz"})
          (<Jazz-File>       name: Table-Row                         source: {File :context "src" "jazz" "tree" "Table-Row.jazz"})
          (<Jazz-File>       name: Table                             source: {File :context "src" "jazz" "tree" "Table.jazz"})
          (<Jazz-File>       name: Tree-Actions                      source: {File :context "src" "jazz" "tree" "Tree-Actions.jazz"})
          (<Jazz-File>       name: Tree-Column                       source: {File :context "src" "jazz" "tree" "Tree-Column.jazz"})
          (<Jazz-File>       name: Tree-Data-View                    source: {File :context "src" "jazz" "tree" "Tree-Data-View.jazz"})
          (<Jazz-File>       name: Tree-Data                         source: {File :context "src" "jazz" "tree" "Tree-Data.jazz"})
          (<Jazz-File>       name: Tree-Drag-Source                  source: {File :context "src" "jazz" "tree" "Tree-Drag-Source.jazz"})
          (<Jazz-File>       name: Tree-Drop-Target                  source: {File :context "src" "jazz" "tree" "Tree-Drop-Target.jazz"})
          (<Jazz-File>       name: Tree-Embedded-Search-Text-Actions source: {File :context "src" "jazz" "tree" "Tree-Embedded-Search-Text-Actions.jazz"})
          (<Jazz-File>       name: Tree-Embedded-Search-Text-View    source: {File :context "src" "jazz" "tree" "Tree-Embedded-Search-Text-View.jazz"})
          (<Jazz-File>       name: Tree-Header                       source: {File :context "src" "jazz" "tree" "Tree-Header.jazz"})
          (<Jazz-File>       name: Tree-Highlight                    source: {File :context "src" "jazz" "tree" "Tree-Highlight.jazz"})
          (<Jazz-File>       name: Tree-Image-Column                 source: {File :context "src" "jazz" "tree" "Tree-Image-Column.jazz"})
          (<Jazz-File>       name: Tree-Image                        source: {File :context "src" "jazz" "tree" "Tree-Image.jazz"})
          (<Jazz-File>       name: Tree-Label-Column                 source: {File :context "src" "jazz" "tree" "Tree-Label-Column.jazz"})
          (<Jazz-File>       name: Tree-Label                        source: {File :context "src" "jazz" "tree" "Tree-Label.jazz"})
          (<Jazz-File>       name: Tree-Node-Column                  source: {File :context "src" "jazz" "tree" "Tree-Node-Column.jazz"})
