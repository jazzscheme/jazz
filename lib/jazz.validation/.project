;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.validation  catalog?: #t catalog-extensions: :text include-extensions: :text
    (<Project-Directory>                              directory: {Directory :context "src"}
      (<Project-Directory>                            directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                          directory: {Directory :context "src" "jazz" "validation"}
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "validation" "tester"}
            (<Jazz-File>       name: Tester-Error     source: {File :context "src" "jazz" "validation" "tester" "Tester-Error.jazz"})
            (<Jazz-File>       name: Tester           source: {File :context "src" "jazz" "validation" "tester" "Tester.jazz"}))
          (<Project-Directory>                        directory: {Directory :context "src" "jazz" "validation" "validate"}
            (<Jazz-File>       name: Validate         source: {File :context "src" "jazz" "validation" "validate" "Validate.jazz"})
            (<Jazz-File>       name: Validation-Suite source: {File :context "src" "jazz" "validation" "validate" "Validation-Suite.jazz"}))
          (<Jazz-File>         name: _validation      source: {File :context "src" "jazz" "validation" "_validation.jazz"})
          (<Jazz-File>         name: autoload         source: {File :context "src" "jazz" "validation" "autoload.jazz"})
          (<Jazz-File>         name: syntax           source: {File :context "src" "jazz" "validation" "syntax.jazz"})))))))
