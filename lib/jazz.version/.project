;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.version description-file: {File :context ".package"}
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "version"}
          (<Jazz-File>       name: _version     source: {File :context "src" "jazz" "version" "_version.jazz"})
          (<Project-File>                       source: {File :context "src" "jazz" "version" "install.scm"})))))))
