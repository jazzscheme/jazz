;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.view    description-file: {File :context ".package"}
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                    directory: {Directory :context "src" "jazz" "view"}
          (<Jazz-File>       name: _view        source: {File :context "src" "jazz" "view" "_view.jazz"})
          (<Jazz-File>       name: Drawing      source: {File :context "src" "jazz" "view" "Drawing.jazz"})
          (<Jazz-File>       name: View-Actions source: {File :context "src" "jazz" "view" "View-Actions.jazz"})
          (<Jazz-File>       name: View         source: {File :context "src" "jazz" "view" "View.jazz"})
          (<Jazz-File>       name: autoload     source: {File :context "src" "jazz" "view" "autoload.jazz"})
          (<Jazz-File>       name: dnd          source: {File :context "src" "jazz" "view" "dnd.jazz"})
          (<Jazz-File>       name: descriptors  source: {File :context "src" "jazz" "view" "descriptors.jazz"})))))))
