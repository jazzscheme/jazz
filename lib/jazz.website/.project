;;;==============
;;;  JazzScheme
;;;==============
;;;
;;;; Website Manifest
;;;


(<Project>                                                    title: "jazz.website" catalog?: #t catalog-extensions: :jazz include-extensions: :jazz
  (<Project-Directory>                                        directory: {Directory jazz.website "src"}
    (<Project-Directory>                                      directory: {Directory jazz.website "src" "jazz"}
      (<Project-Directory>                                    directory: {Directory jazz.website "src" "jazz" "website"}
        (<Project-Directory>                                  directory: {Directory jazz.website "src" "jazz" "website" "server"}
          (<Jazz-File>       name: JazzScheme-Server          source: {File jazz.website "src" "jazz" "website" "server" "JazzScheme-Server.jazz"})
          (<Jazz-File>       name: JazzScheme-Site            source: {File jazz.website "src" "jazz" "website" "server" "JazzScheme-Site.jazz"}))
        (<Project-Directory>                                  directory: {Directory jazz.website "src" "jazz" "website" "transformation"}
          (<Jazz-File>       name: JazzWebsite-Transformation source: {File jazz.website "src" "jazz" "website" "transformation" "JazzWebsite-Transformation.jazz"}))
        (<Jazz-File>         name: _website                   source: {File jazz.website "src" "jazz" "website" "_website.jazz"})
        (<Jazz-File>         name: autoload                   source: {File jazz.website "src" "jazz" "website" "autoload.jazz"})))
    (<Project-Directory>                                      directory: {Directory jazz.website "src" "profile"}
      (<Project-Directory>                                    directory: {Directory jazz.website "src" "profile" "website"}
        (<Project-Directory>                                  directory: {Directory jazz.website "src" "profile" "website" "catalogs"})
        (<Project-Directory>                                  directory: {Directory jazz.website "src" "profile" "website" "settings"}
          (<Project-File>                                     source: {File jazz.website "src" "profile" "website" "settings" "Bindings.jml"})
          (<Project-File>                                     source: {File jazz.website "src" "profile" "website" "settings" "Preferences.jml"})
          (<Project-File>                                     source: {File jazz.website "src" "profile" "website" "settings" "Servers.jml"})
          (<Project-File>                                     source: {File jazz.website "src" "profile" "website" "settings" "Workbench.jml"}))
        (<Jazz-File>         name: Website                    source: {File jazz.website "src" "profile" "website" "Website.jazz"})))))
