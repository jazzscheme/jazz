;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: jazz.window    description-file: {File :context ".package"}
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                        directory: {Directory :context "src" "jazz" "window"}
          (<Project-Directory>                      directory: {Directory :context "src" "jazz" "window" "platform"}
            (<Jazz-File>       name: cocoa          source: {File :context "src" "jazz" "window" "platform" "cocoa.jazz"})
            (<Jazz-File>       name: windows        source: {File :context "src" "jazz" "window" "platform" "windows.jazz"})
            (<Jazz-File>       name: x11            source: {File :context "src" "jazz" "window" "platform" "x11.jazz"})
            (<Jazz-File>       name: independent    source: {File :context "src" "jazz" "window" "platform" "independent.jazz"}))
          (<Jazz-File>         name: _window        source: {File :context "src" "jazz" "window" "_window.jazz"})
          (<Jazz-File>         name: Desktop        source: {File :context "src" "jazz" "window" "Desktop.jazz"})
          (<Jazz-File>         name: External-Frame source: {File :context "src" "jazz" "window" "External-Frame.jazz"})
          (<Jazz-File>         name: Menu-Transient source: {File :context "src" "jazz" "window" "Menu-Transient.jazz"})
          (<Jazz-File>         name: Overlapped     source: {File :context "src" "jazz" "window" "Overlapped.jazz"})
          (<Jazz-File>         name: Palette        source: {File :context "src" "jazz" "window" "Palette.jazz"})
          (<Jazz-File>         name: Popup          source: {File :context "src" "jazz" "window" "Popup.jazz"})
          (<Jazz-File>         name: Tooltip        source: {File :context "src" "jazz" "window" "Tooltip.jazz"})
          (<Jazz-File>         name: Toplevel       source: {File :context "src" "jazz" "window" "Toplevel.jazz"})
          (<Jazz-File>         name: Transient      source: {File :context "src" "jazz" "window" "Transient.jazz"})
          (<Jazz-File>         name: View-Player    source: {File :context "src" "jazz" "window" "View-Player.jazz"})
          (<Jazz-File>         name: Window-Manager source: {File :context "src" "jazz" "window" "Window-Manager.jazz"})
          (<Jazz-File>         name: Window         source: {File :context "src" "jazz" "window" "Window.jazz"})
          (<Jazz-File>         name: autoload       source: {File :context "src" "jazz" "window" "autoload.jazz"})
          (<Jazz-File>         name: descriptors    source: {File :context "src" "jazz" "window" "descriptors.jazz"})
          (<Jazz-File>         name: Playable       source: {File :context "src" "jazz" "window" "Playable.jazz"})
          (<Jazz-File>         name: Windowable     source: {File :context "src" "jazz" "window" "Windowable.jazz"})
          (<Jazz-File>         name: Internal-Popup source: {File :context "src" "jazz" "window" "Internal-Popup.jazz"})))))))
