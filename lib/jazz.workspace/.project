;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jazz.workspace description-file: {File :context ".package"}
    (<Project-Directory>                          directory: {Directory :context "src"}
      (<Project-Directory>                        directory: {Directory :context "src" "jazz"}
        (<Project-Directory>                      directory: {Directory :context "src" "jazz" "workspace"}
          (<Jazz-File>       name: _workspace     source: {File :context "src" "jazz" "workspace" "_workspace.jazz"})
          (<Jazz-File>       name: ui             source: {File :context "src" "jazz" "workspace" "ui.jazz"})
          (<Jazz-File>       name: descriptors    source: {File :context "src" "jazz" "workspace" "descriptors.jazz"})))))))
