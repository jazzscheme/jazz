(package jazz.zlib
  
  (title "zlib")
  (description "Interface to the zlib data compression library")
  (authors "Guillaume Cartier")
  (stage release)
  (root "src")
  (products (jazz.zlib (alias zlib)
                       (remote (ios "togethersphere.com/jazz-zlib-ios")
                               (linux "togethersphere.com/jazz-zlib-linux")
                               (windows "togethersphere.com/jazz-zlib-windows"))
                       (test jazz.zlib.test)
                       (product jazz.zlib.product)
                       (update jazz.zlib
                               jazz.zlib.product)
                       (dependencies jazz)))
  (project ".project"))
