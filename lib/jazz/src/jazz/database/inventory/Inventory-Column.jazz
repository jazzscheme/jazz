(library jazz.database.inventory.Inventory-Column jazz


(import (jazz.database)
        (jazz.library)
        (jazz.utilities))


(class Inventory-Column extends Object
  
  
  (slot inventory)
  (slot parent)
  (slot index-column?)
  
  
  (method (initialize inventory parent index-column?)
    (nextmethod)
    (set! inventory~self inventory)
    (set! parent~self parent)
    (set! index-column?~self index-column?))
  
  
