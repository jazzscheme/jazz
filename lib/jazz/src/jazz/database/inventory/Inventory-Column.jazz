  ;;;; Access
  ;;;
  
  
  (method public (get-inventory)
    inventory)
  
  
  (method public (get-parent)
    parent)
  
  
  (method public (get-table)
    parent)
  
  
  (method public (get-index-column?)
    index-column?)
  
  
