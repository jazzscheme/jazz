(module jazz.validate jazz


(export (jazz.validate.autoload))
(import (jazz.validate.autoload)
        (jazz.validation))


(definition public Jazz-Suite
  (new Validation-Suite
    "Jazz"
    (list (new Array-Tester)
          (new Binding-Tester)
          (new Boolean-Tester)
          (new Catalog-Tester)
          (new Char-Tester)
          (new Collector-Tester)
          (new Component-Tester)
          (new Control-Tester)
          (new Debugger-Tester)
          (new Enumeration-Tester)
          (new Error-Tester)
          (new Explorer-Tester)
          (new Form-Tester)
          (new Former-Tester)
          (new Format-Tester)
          (new Function-Tester)
          (new Iterator-Tester)
          (new Language-Tester)
          (new List-Tester)
          (new Loop-Tester)
          (new Number-Tester)
          (new Object-Tester)
          (new Pathname-Tester)
          (new Reader-Tester)
          (new Scheme-Tester)
          (new Sequence-Tester)
          (new String-Tester)
          (new Structure-Tester)
          (new Symbol-Tester)
          (new Syntax-Tester)
          (new Table-Tester)
          (new Thread-Tester)
          (new Trait-Tester)
          (new Values-Tester)
          (new Variable-Tester)
          (new Vector-Tester)))))
