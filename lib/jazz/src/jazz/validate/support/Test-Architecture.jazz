(module protected jazz.validate.support.Test-Architecture jazz


(import (jazz.library)
        (jazz.validate))


(class Test-Architecture extends Component
  
  
  (property author initialize #f accessors generate)
  
  
  ;;;
  ;;;; Descriptor
  ;;;
  
  
  (method meta override (get-class-descriptor)
    (new Test-Architecture-Descriptor)))


;;;
;;;; Design
;;;


(submodule design


(import (jazz.designer))


(class Test-Architecture-Descriptor extends Component-Descriptor
  
  
  (method override (class-image class)
    {Image-Resource "DesignArchitecture"})
  
  
  (method override (class-presentation class)
    "Architecture")

  
  (method override (class-property-presentation class property)
    (case (field-name property)
      ((author) "Author")
      (else (nextmethod class property))))
  
  
  (method override (component-get-categorized-properties component)
    '(class-info name-info author))
  
  
  (method override (component-get-addable-default-trait component)
    Test-Model)
  
  
  (method override (component-get-addable-classes component)
    (list
      Test-Table)))))
