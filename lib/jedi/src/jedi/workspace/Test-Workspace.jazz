(library protected jedi.workspace.Test-Workspace jazz


(import (jazz.editor.lisp)
        (jazz.jml)
        (jazz.library)
        (jazz.ui))


(class Test-Workspace extends Workspace-Preferences

  
  (form
    (<install>                                  title: "Text" icon: {Bitmap-Resource "WorkspaceText"} shortcut: {Shortcut :alt :shift #\S} position: {Point 1 -2} size: {Dimension 1236 76} maximized?: #t frame-model: Jazz-Text-View
      (<Toolbar-Preferences>                    location: root model-name: :menubar)
      (<Palette-Preferences>    name: toolbar   title: "Toolbar" position: {Point 350 92} size: {Dimension 792 54}
        (<Toolbar-Preferences>                  location: first model-name: :toolbar))
      (<Palette-Preferences>    name: status    title: "Status" position: {Point 350 614} size: {Dimension 792 54}
        (<Toolbar-Preferences>                  location: second model-name: :status))
      (<Palette-Preferences>    name: workbench title: "Workbench" position: {Point 1 111} size: {Dimension 255 461}
        (<Pad-Preferences>                      location: first
          (<Docked-Preferences>                 location: selection model-name: Workbench-Manager)
          (<Docked-Preferences>                 location: #f model-name: Repositories-Manager)))
      (<Palette-Preferences>    name: text      title: "Text" position: {Point 1 602} size: {Dimension 255 395}
        (<Splitter-Preferences>                 mode: relative division: 31 location: second orientation: vert location: second
          (<Docked-Preferences>                 location: first model-name: Chapters-Browser)
          (<Docked-Preferences>                 location: second model-name: Hierarchy-Browser)))
      (<Palette-Preferences>    name: console   title: "Console" position: {Point 267 699} size: {Dimension 969 300}
        (<Pad-Preferences>                      location: first
          (<Docked-Preferences>                 location: selection model-name: Lisp-Console-Manager)
          (<Docked-Preferences>                 location: #f model-name: Properties-Manager)
          (<Docked-Preferences>                 location: #f model-name: Search-Results)
          (<Docked-Preferences>                 location: #f model-name: Profile-Results)))
      (<Palette-Preferences>    name: stage     title: "Stage" position: {Point 294 162} size: {Dimension 907 421}
        (<Stage-Preferences>))))
  
  
  (method override (default-name)
    'text)))
