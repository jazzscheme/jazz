                    (<Pad-Preferences>                                 location: second
                      (<Docked-Preferences>                            location: selection model-name: Lisp-Console-Manager)
                      (<Docked-Preferences>                            location: #f model-name: Properties-Manager)
                      (<Docked-Preferences>                            location: #f model-name: Search-Results)
                      (<Docked-Preferences>                            location: #f model-name: Profile-Results)
                      (<Docked-Preferences>                            location: #f model-name: Servers-Manager))))))))))
    (<Workspace-Preferences>                  name: groupware          title: "Groupware" icon: {Image-Resource "WorkspaceGroupware"} shortcut: {Shortcut :shift :alt #\G}
      (<Menubar-Preferences>                  name: menubar            location: root
        (<Mainbar-Preferences>                name: toolbar            location: second
          (<Minibuffer-Preferences>           name: minibuffer         location: second
            (<Status-Preferences>             name: status             location: first
              (<Splitter-Preferences>         name: top                location: first state: zoom target: second orientation: horz mode: relative division: 25 insets: {Rect 5 5 5 5}
                (<Splitter-Preferences>                                location: first orientation: vert mode: relative division: -31
                  (<Pad-Preferences>                                   location: first
                    (<Docked-Preferences>                              location: selection model-name: Workbench-Manager)
                    (<Docked-Preferences>                              location: #f model-name: Repositories-Manager))
                  (<Pad-Preferences>                                   location: second
                    (<Docked-Preferences>                              location: selection model-name: Lisp-Console-Manager)
                    (<Docked-Preferences>                              location: #f model-name: Properties-Manager)
                    (<Docked-Preferences>                              location: #f model-name: Search-Results)))
                (<Stage-Preferences>                                   location: second)))))))
    (<Workspace-Preferences>                  name: sql                title: "SQL" icon: {Image-Resource "WorkspaceSQL"} shortcut: {Shortcut :shift :alt #\L} frame-model: SQL-Text-View
      (<Menubar-Preferences>                  name: menubar            location: root
        (<Mainbar-Preferences>                name: toolbar            location: second
          (<Minibuffer-Preferences>           name: minibuffer         location: second
            (<Status-Preferences>             name: status             location: first
              (<Splitter-Preferences>         name: top                location: first orientation: horz mode: relative division: 33 insets: {Rect 5 5 5 5}
                (<Splitter-Preferences>                                location: first orientation: vert mode: relative division: -27
                  (<Pad-Preferences>                                   location: second
                    (<Docked-Preferences>                              location: selection model-name: SQL-Console-Manager)
                    (<Docked-Preferences>                              location: #f model-name: Lisp-Console-Manager)
                    (<Docked-Preferences>                              location: #f model-name: Properties-Manager)
                    (<Docked-Preferences>                              location: #f model-name: Search-Results))
                  (<Splitter-Preferences>                              location: first mode: relative division: 48 orientation: horz
                    (<Splitter-Preferences>                            location: second mode: relative division: -43 orientation: vert
