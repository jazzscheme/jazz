  (<workspaces~>                                                       active: home
    (<Workspace-Preferences>                  name: home               title: "Home" icon: {Image-Resource "WorkspaceHome"} shortcut: {Shortcut :shift :alt #\A}
      (<Menubar-Preferences>                  name: menubar            location: root
        (<Mainbar-Preferences>                name: toolbar            location: second
          (<Minibuffer-Preferences>           name: minibuffer         location: second
            (<Status-Preferences>             name: status             location: first
              (<Splitter-Preferences>         name: top                location: first orientation: horz mode: relative division: 20 insets: {Rect 5 5 5 5}
                (<Splitter-Preferences>                                location: second orientation: vert mode: relative division: -27
                  (<Stage-Preferences>                                 location: first maximized?: #f)
                  (<Pad-Preferences>                                   location: second
                    (<Docked-Preferences>                              location: selection model-name: Lisp-Console-Manager)
                    (<Docked-Preferences>                              location: #f model-name: Properties-Manager)
                    (<Docked-Preferences>                              location: #f model-name: Search-Results)
                    (<Docked-Preferences>                              location: #f model-name: Profile-Results)
                    (<Docked-Preferences>                              location: #f model-name: Servers-Manager)))
                (<Pad-Preferences>                                     location: first
                  (<Docked-Preferences>                                location: selection model-name: Workbench-Manager)
                  (<Docked-Preferences>                                location: #f model-name: Repositories-Manager))))))))
    (<Workspace-Preferences>                  name: text               title: "Text" icon: {Image-Resource "WorkspaceText"} shortcut: {Shortcut :shift :alt #\S} frame-model: Jazz-Text-View
      (<Menubar-Preferences>                  name: menubar            location: root
        (<Mainbar-Preferences>                name: toolbar            location: second
          (<Minibuffer-Preferences>           name: minibuffer         location: second state: split
            (<Status-Preferences>             name: status             location: first
              (<Splitter-Preferences>         name: top                location: first orientation: horz mode: relative division: 33 insets: {Rect 5 5 5 5}
