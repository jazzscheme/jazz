  (let ((y <fx> v))
    v))


;;;
;;;; Syntaxes
;;;


(define (binding)
  (let ((x v))
    (let* ((x v))
      (letrec ((x v))
        (receive (x) v
          v))))
  (parameterize ((v v))
    v)
  (with ((x v))
    (with-closed ((x v))
      v))
  (bind (x) v
    (bind-optionals ((x v)) v
      (bind-keywords ((x v)) v
        v))))


(define (functional)
  (lambda (x)
    v)
  (function (x)
    v))


(define (quotation)
  'x
  (quote x)
  `(x ,v ,@v)
  (quasiquote (x (unquote v) (unquote-splicing v))))


(define (logical)
  (and v)
  (or v)
  (essay v)
  (either v))


(define (conditional)
  (if v
      v
    v)
  (when v
    v)
  (unless v
    v)
  (cond (v v)
        (else v))
  (case v
    ((x) v)
    (else v))
  (ecase v
    ((v) v)
    (else v))
  (typecase v
    ((v) v)
    (else v)))


(define (control)
  (begin
    v)
  (prog1 v
    v)
  (catch v
    v)
  (unwind-protect
      v
    v)
  (delay v)
  (do ((x v))
      (v v)
    v)
  (while v
    v))


(define (assertion)
  (assert v)
  (assert-type v v)
  (error? v))


(define (iteration)
  (loop (for x from v to v by v)
        (collect v)
        (do v)))


(define (assignment)
  (set! v v)
  (increase! v v)
  (decrease! v v))


(define (other)
  (tie v v)
  (construct v v)
  (time v))


