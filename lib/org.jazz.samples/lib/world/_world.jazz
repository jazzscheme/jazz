(library world jazz


(import (jazz.platform)
        (jazz.platform.literals)
        (jazz.system)
        (jazz.system.process)
        (jazz.utilities))


(class World extends Process
  
  
  (method (start-process rest)
    (message-box "Hello World!")
    (exit)))


;;;
;;;; Product
;;;


(register-product 'world
  (lambda ()
    (boot-environment~ (new World) '()))
  #f))
