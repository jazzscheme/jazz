(library jazz.network jazz


(export (jazz.network.autoload))
(import (jazz.network.autoload))


;;;
;;;; Ports
;;;


(definition Port-Numbers
  '((any                  .     0)
    (echo                 .     7)
    (http                 .    80)
    (router-stub-listener . 32000)))


(definition public (get-port-number name)
  (if (integer? name)
      name
    (cdr (assq name Port-Numbers)))))
