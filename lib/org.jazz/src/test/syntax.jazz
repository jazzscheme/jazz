(library test.syntax jazz

(define (ya)
  (values 1 2))

(class X extends Object
  
  (method virtual (foo)
    (error "boum")))

(class Y extends X
  
  (method (foo)
    (list 3 (nextmethod))))

(class Wo extends Object
  
  (slot y)
  
  (method (initialize value)
    (nextmethod)
    (set! y value))
  
  (method (call)
    (receive (a b) (ya)
      (let ((n 10))
        (while (> n 0)
          (decrease! n)
          (foo~ y))))))

(call~ (new Wo (new Y))))
