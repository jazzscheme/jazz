            (<Jazz-File>              name: Manager-View          source: {File org.jedi "lib" "jedi" "designer" "Manager-View.jazz"})
            (<Jazz-File>              name: Menu-Editor           source: {File org.jedi "lib" "jedi" "designer" "Menu-Editor.jazz"}))
          (<Project-Group>            name: "generator"
            (<Jazz-File>              name: Code-Generator        source: {File org.jedi "lib" "jedi" "generator" "Code-Generator.jazz"}))
          (<Project-Group>            name: "parameters"
            (<Project-File>                                       source: {File org.jedi "lib" "jedi" "parameters" "Properties.prop"}))
          (<Project-Group>            name: "preference"
            (<Jazz-File>              name: Jedi-Actions          source: {File org.jedi "lib" "jedi" "preference" "Jedi-Actions.jazz"})
            (<Jazz-File>              name: Jedi-Bindings         source: {File org.jedi "lib" "jedi" "preference" "Jedi-Bindings.jazz"})
            (<Jazz-File>              name: Jedi-Preferences      source: {File org.jedi "lib" "jedi" "preference" "Jedi-Preferences.jazz"}))
          (<Project-Group>            name: "profile"
            (<Jazz-File>              name: Jedi-Profile          source: {File org.jedi "lib" "jedi" "profile" "Jedi-Profile.jazz"}))
          (<Project-Group>            name: "project"
            (<Jazz-File>              name: Jedi-Module           source: {File org.jedi "lib" "jedi" "project" "Jedi-Module.jazz"})
            (<Jazz-File>              name: Jedi-Product          source: {File org.jedi "lib" "jedi" "project" "Jedi-Product.jazz"}))
          (<Project-Group>            name: "search"
            (<Jazz-File>              name: Jedi-Search           source: {File org.jedi "lib" "jedi" "search" "Jedi-Search.jazz"}))
          (<Project-Group>            name: "showcase"
            (<Jazz-File>              name: Outline-Showcase      source: {File org.jedi "lib" "jedi" "showcase" "Outline-Showcase.jazz"})
            (<Jazz-File>              name: Scaled-View           source: {File org.jedi "lib" "jedi" "showcase" "Scaled-View.jazz"})
            (<Jazz-File>              name: Showcase              source: {File org.jedi "lib" "jedi" "showcase" "Showcase.jazz"})
            (<Jazz-File>              name: Text-Showcase         source: {File org.jedi "lib" "jedi" "showcase" "Text-Showcase.jazz"})
            (<Jazz-File>              name: Threads-Showcase      source: {File org.jedi "lib" "jedi" "showcase" "Threads-Showcase.jazz"})
            (<Jazz-File>              name: Tree-Showcase-Actions source: {File org.jedi "lib" "jedi" "showcase" "Tree-Showcase-Actions.jazz"})
            (<Jazz-File>              name: Tree-Showcase         source: {File org.jedi "lib" "jedi" "showcase" "Tree-Showcase.jazz"}))
          (<Project-Group>            name: "smoker"
            (<Jazz-File>              name: Smoker-Builder        source: {File org.jedi "lib" "jedi" "smoker" "Smoker-Builder.jazz"})
            (<Jazz-File>              name: Smoker-Suite          source: {File org.jedi "lib" "jedi" "smoker" "Smoker-Suite.jazz"})
            (<Jazz-File>              name: Smoker                source: {File org.jedi "lib" "jedi" "smoker" "Smoker.jazz"}))
          (<Project-Group>            name: "templates"
            (<Project-Group>          name: "new"))
          (<Project-Group>            name: "test"
            (<Jazz-File>              name: Horizontal-View       source: {File org.jedi "lib" "jedi" "test" "Horizontal-View.jazz"})
            (<Jazz-File>              name: Vertical-View         source: {File org.jedi "lib" "jedi" "test" "Vertical-View.jazz"}))
          (<Project-Group>            name: "text"
            (<Jazz-File>              name: Completion-Popup      source: {File org.jedi "lib" "jedi" "text" "Completion-Popup.jazz"}))
          (<Jazz-File>                name: _jedi                 source: {File org.jedi "lib" "jedi" "_jedi.jazz"})
          (<Jazz-File>                name: autoload              source: {File org.jedi "lib" "jedi" "autoload.jazz"})
          (<Jazz-File>                name: product               source: {File org.jedi "lib" "jedi" "product.jazz"}))
        (<Jazz-File>                  name: Jedi-Project          source: {File org.jedi "lib" "Jedi-Project.jazz"})))))


(register-autoclass Jedi-Project))
