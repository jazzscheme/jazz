                      (<Project-File>                             source: {File org.jedi "lib" "jedi" "defaults" "lib" "profiles" "profile" "default" "settings" "WebServers.jml"})
                      (<Project-File>                             source: {File org.jedi "lib" "jedi" "defaults" "lib" "profiles" "profile" "default" "settings" "Workbench.jml"}))
                    (<Jazz-File>      name: _default              source: {File org.jedi "lib" "jedi" "defaults" "lib" "profiles" "profile" "default" "_default.jazz"})))
                (<Project-File>                                   source: {File org.jedi "lib" "jedi" "defaults" "lib" "profiles" "profiles.pck"})))
            (<Project-File>                                       source: {File org.jedi "lib" "jedi" "defaults" ".jazzini"}))
          (<Project-Group>            name: "designer"
            (<Jazz-File>              name: Alignment-Editor      source: {File org.jedi "lib" "jedi" "designer" "Alignment-Editor.jazz"})
