(library Jedi-Project jazz


(import (jazz.builder)
        (jazz.io)
        (jazz.jml)
        (jazz.library)
        (jazz.literals))


(class Jedi-Project extends Project
  
  
  (form
    (<install>                                                    title: "org.jedi"
      (<Jedi-Product>                 name: jedi                  tag-module: jedi.project.Jedi-Product)
      (<Project-Group>                name: "doc"
        (<Project-Group>              name: "screenshots")
        (<Project-File>                                           source: {File org.jedi "doc" "org.jedi.jml"}))
      (<Project-Group>                name: "lib"
        (<Project-Group>              name: "jedi"
          (<Project-Group>            name: "application"
            (<Jazz-File>              name: Jedi-Menubar          source: {File org.jedi "lib" "jedi" "application" "Jedi-Menubar.jazz"})
            (<Jazz-File>              name: Jedi-Status           source: {File org.jedi "lib" "jedi" "application" "Jedi-Status.jazz"})
            (<Jazz-File>              name: Jedi-Toolbar          source: {File org.jedi "lib" "jedi" "application" "Jedi-Toolbar.jazz"})
            (<Jazz-File>              name: Jedi-Toplevel         source: {File org.jedi "lib" "jedi" "application" "Jedi-Toplevel.jazz"})
            (<Jazz-File>              name: Jedi                  source: {File org.jedi "lib" "jedi" "application" "Jedi.jazz"}))
          (<Project-Group>            name: "defaults"
            (<Project-Group>          name: "lib"
              (<Project-Group>        name: "profiles"
                (<Project-Group>      name: "profile"
                  (<Project-Group>    name: "default"
