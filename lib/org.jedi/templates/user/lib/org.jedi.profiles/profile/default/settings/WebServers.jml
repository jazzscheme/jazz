(<Web-Server-Configurations>
  (<Web-Server-Configuration>
    name: Game
    module: jazz.sample
    class: Game-Server
    host: {Host name: "localhost"}
    service: 8080
    root-directory: {Directory Jazz "lib" "org.jazz.sample" "site" "content"}
    default-resource: "/Games"
    description: "Game Server"
    debug?: #f))
