(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>               name: profiler  description-file: {File :context ".package"}
    (<Project-Directory>                   directory: {Directory :context "src"}
      (<Project-Directory>                 directory: {Directory :context "src" "profiler"}
        (<Jazz-File>       name: _profiler source: {File :context "src" "profiler" "_profiler.jazz"})
        (<Project-File>                    source: {File :context "src" "profiler" "implementation.scm"}))))))
