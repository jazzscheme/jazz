          (<Jazz-File>       name: ffi   source: {File sample.ldap "src" "ldap" "ffi.jazz"})
          (<Jazz-File>       name: ldap0 source: {File sample.ldap "src" "ldap" "ldap0.jazz"})
          (<Jazz-File>       name: ldap1 source: {File sample.ldap "src" "ldap" "ldap1.jazz"})
          (<Jazz-File>       name: ldap2 source: {File sample.ldap "src" "ldap" "ldap2.jazz"})
          (<Jazz-File>       name: ldap3 source: {File sample.ldap "src" "ldap" "ldap3.jazz"}))))))


(register-autoclass Ldap-Manifest))
