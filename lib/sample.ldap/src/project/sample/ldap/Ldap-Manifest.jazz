(library project.sample.ldap.Ldap-Manifest jazz


(import (jazz.io)
        (jazz.jml)
        (jedi))


(class Ldap-Manifest extends Project
  
  
  (form
    (<install>                           title: "sample.ldap" catalog?: #t catalog-extensions: :text include-extensions: :text module-name: project.sample.ldap.Ldap-Manifest
      (<Project-Directory>               directory: {Directory sample.ldap "src"}
