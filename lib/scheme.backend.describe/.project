(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: scheme.backend.describe description-file: {File :context ".package"}
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "scheme"}
        (<Project-Directory>                        directory: {Directory :context "src" "scheme" "backend"}
          (<Project-Directory>                      directory: {Directory :context "src" "scheme" "backend" "describe"}
            (<Jazz-File>       name: _describe      source: {File :context "src" "scheme" "backend" "describe" "_describe.jazz"})
            (<Jazz-File>       name: emit           source: {File :context "src" "scheme" "backend" "describe" "emit.jazz"}))))))))
