(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                   name: scheme.backend.javascript description-file: {File :context ".package"}
    (<Project-Directory>                                       directory: {Directory :context "src"}
      (<Project-Directory>                                     directory: {Directory :context "src" "scheme"}
        (<Project-Directory>                                   directory: {Directory :context "src" "scheme" "backend"}
          (<Project-Directory>                                 directory: {Directory :context "src" "scheme" "backend" "javascript"}
            (<Jazz-File>       name: _javascript               source: {File :context "src" "scheme" "backend" "javascript" "_javascript.jazz"})
            (<Jazz-File>       name: emit                      source: {File :context "src" "scheme" "backend" "javascript" "emit.jazz"}))))))))
