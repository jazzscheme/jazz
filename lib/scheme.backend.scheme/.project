;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
(jazz.editor.jazz))


(form
  (<Project>                     name: scheme.backend.scheme
    (<Project-Directory>                                     directory: {Directory :context "src"}
      (<Project-Directory>                                   directory: {Directory :context "src" "scheme"}
        (<Project-Directory>                                 directory: {Directory :context "src" "scheme" "backend"}
          (<Project-Directory>                               directory: {Directory :context "src" "scheme" "backend" "scheme"}
            (<Project-Directory>                             directory: {Directory :context "src" "scheme" "backend" "scheme" "runtime"}
              (<Project-File>                                source: {File :context "src" "scheme" "backend" "scheme" "runtime" "_runtime.scm"})
              (<Project-File>                                source: {File :context "src" "scheme" "backend" "scheme" "runtime" "classes.scm"})
              (<Project-File>                                source: {File :context "src" "scheme" "backend" "scheme" "runtime" "syntax.scm"}))
            (<Project-File>                                  source: {File :context "src" "scheme" "backend" "scheme" "emit.scm"}))))))))
