;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz)
        (jazz.project))


(form
  (<Project>                 name: scheme.core description-file: {File :context ".package"}
    (<Project-Directory>                       directory: {Directory :context "src"}
      (<Project-Directory>                     directory: {Directory :context "src" "scheme"}
        (<Project-Directory>                   directory: {Directory :context "src" "scheme" "core"}
          (<Project-File>                      source: {File :context "src" "scheme" "core" "kernel.scm"})
          (<Jazz-File>       name: _core       source: {File :context "src" "scheme" "core" "_core.jazz"})))))))
