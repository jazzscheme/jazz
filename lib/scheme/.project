;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.editor.jazz)
        (jazz.project))


(form
  (<Project>               name: scheme description-file: {File :context ".package"}
    (<Project-Directory>                directory: {Directory :context "src"}
      (<Project-Directory>              directory: {Directory :context "src" "scheme"}
        (<Project-File>                 source: {File :context "src" "scheme" "_scheme.scm"})
        (<Project-File>                 source: {File :context "src" "scheme" "backend.scm"})
        (<Project-File>                 source: {File :context "src" "scheme" "syntax.scm"})
        (<Project-File>                 source: {File :context "src" "scheme" "dialect.scm"})
        (<Project-File>                 source: {File :context "src" "scheme" "syntax-rules.scm"})
        (<Project-File>                 source: {File :context "src" "scheme" "runtime.scm"})
        (<Jazz-File>       name: _core  source: {File :context "src" "scheme" "_core.jazz"})
        (<Project-File>                 source: {File :context "src" "scheme" "kernel.scm"}))))))
