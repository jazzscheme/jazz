(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>               name: srfi-19 description-file: {File :context ".package"}
    (<Project-Directory>                 directory: {Directory :context "src"}
      (<Project-Directory>               directory: {Directory :context "src" "time"}
        (<Jazz-File>       name: _time   source: {File :context "src" "time" "_time.jazz"})
        (<Project-File>                  source: {File :context "src" "time" "classes.scm"})
        (<Jazz-File>       name: exports source: {File :context "src" "time" "exports.jazz"})
        (<Project-File>                  source: {File :context "src" "time" "implementation.scm"})
        (<Project-File>                  source: {File :context "src" "time" "tests.scm"}))))))
