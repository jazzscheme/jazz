(library test.component jazz

(import (jazz.jml)
        (jazz.library)
        (jazz.literals))


(register-autoload 'C 'test.component)

(class C extends Component
  
  (property x initialize 2 accessors generate)
  (property title)
  (property position)
  
  (method (print output detail)
    (print-unreadable self output
      (function (output)
        (format output "x: {s} title: {s} position: {s}" x title position)))))


(class A extends Component
  
  (form
    (<install>
      (<C> name: c x: 1 title: "Search" position: {Point 763 14}))))


(class B extends A
  
  (form
    (<install>
      (<!> name: c x: 5
        (<C> x: 9))
      (<C> name: d
        (<C> name: e x: 7))
      (<C> x: 3)))))
