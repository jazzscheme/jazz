(library test.window jazz

(import (jazz.library)
        (jazz.platform.windows))


(definition (process-messages window)
  (let ((msg (MSG-make)))
    (let iter ()
      (let ((flag (GetMessage msg window 0 0)))
        (TranslateMessage msg)
        (DispatchMessage msg)
        (if flag
            (iter))))))

(let ((a (new Application)))
  (boot-environment~ a {})))
