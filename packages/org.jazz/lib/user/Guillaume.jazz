(library user.Guillaume jazz


(import (dev)
        (jazz.jazz)
        (jazz.io)
        (jazz.library)
        (jazz.literals)
        (jazz.platform)
        (jazz.ui)
        (jazz.ui.dialog)
        (jazz.ui.view)
        (jazz.ui.window)
        (jazz.utilities))


(class Guillaume extends User
  
  
  (method (test)
    (let ((root (new Dialog-Root)))
      (debug root (get-handle~ root))))
  
  
  (method (test-shift)
    (debug (choose-file)))
  
  
  (method (test1)
    (let ((appl (get-application)))
      (let ((catalog (get-catalog~ (get-cataloguer~ appl) 'org.jedi)))
        (debug catalog (get-completions~ catalog "An")))))
  
  
  (method (test2)
    )
  
  
  (method (test5)
    (new-palette Statprof-Viewer))
  
  
  (method (read-statprof-report file)
    (call-with-input-file file
      read))
  
  
  (method (test6)
    (start-statprof)
    (user-message "Statprof started"))
  
  
  (method (test7)
    (stop-statprof)
    (user-message "Statprof stopped"))
  
  
  (method (test8)
    (let ((name (report-statprof)))
      (user-message "Statprof report sent to {a}" name)))
  
  
  (method (test9)
    (time (edit-module {File Root "packages" "org.jazz" "lib" "jazz" "ui" "window" "Window.jazz"})))
  
  
  (method (test0)
    (let ((text (new Jazz-Text-View)))
      (time (set-moniker~ text {File Root "packages" "org.jazz" "lib" "jazz" "ui" "window" "Window.jazz"}))))
  
  
  (method (edit-module module-file)
    (set! %h (edit-document~ (get-application) module-file))
    (set! % (get-guest~ %h)))))
