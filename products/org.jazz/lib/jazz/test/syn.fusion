(library jazz.test.syn jazz

(definition (foo . rest)
  (bind-keywords ((lala 1) (lolo 3)) rest
    lala))

(definition (bar . rest)
  (bind-optionals ((lala 1) (lolo 3)) rest
    (list lala lolo)))

(debug (foo lala: 2))
(debug (bar))
(debug (bar 'a))
(debug (bar 'a 'b))
(debug (bar 'a 'b 'c)))
